#include <stdio.h>
#include <string.h>

char input[100005];
char result[100005];

int main() {
    scanf("%s", input);
    int n = strlen(input);

    int k = 0;
    int insert_pos = 0;

    for (int i = 0; i < n; i++) {
        if (input[i] == '0') {
            result[k++] = '0';
            insert_pos++;
        } else if (input[i] == '2') {
            result[k++] = '2';
        }
    }

    int count1 = 0;
    for (int i = 0; i < n; i++)
        if (input[i] == '1') count1++;

    memmove(result + insert_pos + count1, result + insert_pos, k - insert_pos);

    for (int i = 0; i < count1; i++)
        result[insert_pos + i] = '1';

    k += count1;
    result[k] = '\0';

    printf("%s\n", result);
    return 0;
}

