#include <stdio.h>
#include <string.h>
#include <stdbool.h>

#define MAXN 200005

typedef struct {
    int l, r;
} Operation;

char a[MAXN], b[MAXN];
Operation ops[MAXN + 10];
int op_count;

bool is_equal(char *s1, char *s2, int n) {
    for (int i = 0; i < n; i++) {
        if (s1[i] != s2[i]) return false;
    }
    return true;
}

bool is_opposite(char *s1, char *s2, int n) {
    for (int i = 0; i < n; i++) {
        if (s1[i] == s2[i]) return false;
    }
    return true;
}

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        int n;
        scanf("%d", &n);
        scanf("%s", a);
        scanf("%s", b);

        op_count = 0;

        for (int i = 0; i < n; i++) {
            if (a[i] == '1') {
                ops[op_count].l = i + 1;
                ops[op_count].r = i + 1;
                op_count++;
            }
        }

        bool same = is_equal(a, b, n);
        bool opposite = is_opposite(a, b, n);

        if (!same && !opposite) {
            printf("NO\n");
            continue;
        }

        if (opposite) {
            ops[op_count].l = 1;
            ops[op_count].r = n;
            op_count++;
        }

        printf("YES\n");
        printf("%d\n", op_count);
        for (int i = 0; i < op_count; i++) {
            printf("%d %d\n", ops[i].l, ops[i].r);
        }
    }

    return 0;
}

