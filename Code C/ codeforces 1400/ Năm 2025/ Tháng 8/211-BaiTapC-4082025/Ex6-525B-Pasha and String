#include <stdio.h>
#include <string.h>

#define MAX 200005

char s[MAX];
char flip[MAX];

void swap(char *a, char *b) {
    char temp = *a;
    *a = *b;
    *b = temp;
}

int main() {
    scanf("%s", s + 1);
    int n = strlen(s + 1);

    int m;
    scanf("%d", &m);

    while (m--) {
        int a;
        scanf("%d", &a);
        flip[a] ^= 1;
    }

    int parity = 0;
    for (int i = 1; i <= n / 2; i++) {
        parity ^= flip[i];
        if (parity) {
            swap(&s[i], &s[n - i + 1]);
        }
    }

    printf("%s\n", s + 1);
    return 0;
}

