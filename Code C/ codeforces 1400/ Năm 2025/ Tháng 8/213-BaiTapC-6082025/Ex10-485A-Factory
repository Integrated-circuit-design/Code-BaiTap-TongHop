#include <stdio.h>

const char* kiem_tra_dung(int a, int m) {
    static char danh_dau[100005] = {0};

    for (int i = 0; i < m; i++) danh_dau[i] = 0;

    while (1) {
        int du = a % m;

        if (du == 0) return "Yes";
        if (danh_dau[du]) return "No";

        danh_dau[du] = 1;
        a += du;
    }
}

int main() {
    int a, m;
    scanf("%d %d", &a, &m);
    printf("%s\n", kiem_tra_dung(a, m));
    return 0;
}

