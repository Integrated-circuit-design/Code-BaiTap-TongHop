#include <stdio.h>

char grid[105][105];

int main() {
    int n, k;
    scanf("%d %d", &n, &k);

    int max_islands = (n * n + 1) / 2;
    if (k > max_islands) {
        puts("NO");
        return 0;
    }

    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            grid[i][j] = 'S';

    for (int i = 0; i < n && k > 0; i++) {
        for (int j = (i % 2 == 0 ? 0 : 1); j < n && k > 0; j += 2) {
            grid[i][j] = 'L';
            k--;
        }
    }

    puts("YES");
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++)
            putchar(grid[i][j]);
        putchar('\n');
    }

    return 0;
}

