#include <stdio.h>
#include <string.h>

#define MAX 1005

int n, m;
char grid[MAX][MAX];
int row[MAX], col[MAX];
int total_walls = 0;

void read_input() {
    scanf("%d %d", &n, &m);
    for (int i = 0; i < n; ++i) {
        scanf("%s", grid[i]);
        for (int j = 0; j < m; ++j) {
            if (grid[i][j] == '*') {
                row[i]++;
                col[j]++;
                total_walls++;
            }
        }
    }
}

int can_bomb_clear_all(int x, int y) {
    int count = row[x] + col[y];
    if (grid[x][y] == '*') count--;
    return count == total_walls;
}

int find_bomb_position(int *res_x, int *res_y) {
    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < m; ++j) {
            if (can_bomb_clear_all(i, j)) {
                *res_x = i + 1;
                *res_y = j + 1;
                return 1;
            }
        }
    }
    return 0;
}

int main() {
    int x, y;
    read_input();

    if (find_bomb_position(&x, &y)) {
        printf("YES\n%d %d\n", x, y);
    } else {
        printf("NO\n");
    }

    return 0;
}

