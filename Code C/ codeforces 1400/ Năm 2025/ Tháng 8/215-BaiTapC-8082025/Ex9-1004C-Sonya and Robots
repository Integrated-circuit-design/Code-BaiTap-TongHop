#include <stdio.h>
#include <string.h>

#define MAXN 100005

int a[MAXN];
int seen_left[MAXN];
int seen_right[MAXN];
int diff_count_right[MAXN];

int main() {
    int n;
    scanf("%d", &n);

    for (int i = 0; i < n; i++)
        scanf("%d", &a[i]);

    int total_right = 0;
    for (int i = n - 1; i >= 0; i--) {
        if (!seen_right[a[i]]) {
            seen_right[a[i]] = 1;
            total_right++;
        }
        diff_count_right[i] = total_right;
    }

    long long result = 0;
    memset(seen_left, 0, sizeof(seen_left));

    for (int i = 0; i < n - 1; i++) {
        if (!seen_left[a[i]]) {
            seen_left[a[i]] = 1;
            result += diff_count_right[i + 1];
        }
    }

    printf("%lld\n", result);
    return 0;
}
