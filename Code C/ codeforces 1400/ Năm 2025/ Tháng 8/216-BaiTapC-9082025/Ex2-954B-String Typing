#include <stdio.h>
#include <string.h>

int main() {
    int n;
    char s[105];
    scanf("%d %s", &n, s);

    int ans = n;
    for (int k = 1; k <= n / 2; k++) {
        if (strncmp(s, s + k, k) == 0) {
            int steps = n - k + 1;
            if (steps < ans) ans = steps;
        }
    }

    printf("%d\n", ans);
    return 0;
}

