#include <stdio.h>

int main() {
    int n;
    long long k;
    scanf("%d %lld", &n, &k);

    int a[100005];
    long long total = 0;
    int i;

    for (i = 0; i < n; i++) {
        scanf("%d", &a[i]);
        total += a[i] / 10;
    }

    while (k > 0) {
        int best_index = -1, min_cost = 11;
        for (i = 0; i < n; i++) {
            if (a[i] < 100) {
                int rem = a[i] % 10;
                if (rem > 0) {
                    int cost = 10 - rem;
                    if (cost < min_cost) {
                        min_cost = cost;
                        best_index = i;
                    }
                }
            }
        }
        if (best_index == -1) break;
        if (k >= min_cost) {
            k -= min_cost;
            a[best_index] += min_cost;
            total++;
        } else {
            break;
        }
    }

    for (i = 0; i < n && k >= 10; i++) {
        while (a[i] < 100 && k >= 10) {
            a[i] += 10;
            k -= 10;
            total++;
        }
    }

    printf("%lld\n", total);
    return 0;
}

