#include <stdio.h>

#define MAX 205

int main() {
    int n;
    scanf("%d", &n);

    int a[MAX], pos[MAX]; 
    int ans = 0;

    for (int i = 0; i < 2 * n; i++) {
        scanf("%d", &a[i]);
        pos[i] = -1;
    }

    for (int i = 0; i < 2 * n; i++) {
        if (pos[a[i]] == -1) {
            pos[a[i]] = i;
        } else {
            for (int j = i; j > pos[a[i]] + 1; j--) {
                int tmp = a[j];
                a[j] = a[j - 1];
                a[j - 1] = tmp;
                ans++;
            }
            for (int k = 0; k < 2 * n; k++) pos[a[k]] = k;
        }
    }

    printf("%d\n", ans);
    return 0;
}

