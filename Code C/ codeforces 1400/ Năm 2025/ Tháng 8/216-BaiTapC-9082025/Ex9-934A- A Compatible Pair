#include <stdio.h>
#include <limits.h>

int main() {
    int n, m;
    long long a[55], b[55];
    scanf("%d %d", &n, &m);
    for (int i = 0; i < n; i++) scanf("%lld", &a[i]);
    for (int j = 0; j < m; j++) scanf("%lld", &b[j]);

    long long ans = LLONG_MAX;
    for (int hide = 0; hide < n; hide++) {
        long long banban_max = LLONG_MIN;
        for (int i = 0; i < n; i++) {
            if (i == hide) continue;
            for (int j = 0; j < m; j++) {
                long long prod = a[i] * b[j];
                if (prod > banban_max) {
                    banban_max = prod;
                }
            }
        }
        if (banban_max < ans) ans = banban_max;
    }

    printf("%lld\n", ans);
    return 0;
}

