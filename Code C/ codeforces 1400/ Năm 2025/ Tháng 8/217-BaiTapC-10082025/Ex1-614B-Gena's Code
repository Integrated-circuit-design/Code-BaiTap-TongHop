#include <stdio.h>
#include <string.h>

int checkBeautiful(const char *str) {
    if (str[0] == '0' && str[1] == '\0') return -1;
    int count1 = 0;
    int len = 0;
    while (str[len] != '\0') {
        if (str[len] == '1') count1++;
        else if (str[len] != '0') return 0;
        len++;
    }
    if (count1 <= 1) return len - 1;
    return 0;
}

int main() {
    int n;
    scanf("%d", &n);

    char buf[100005];
    char base[100005] = "1";
    int hasBase = 0;
    int totalZeros = 0;

    for (int i = 0; i < n; i++) {
        scanf("%s", buf);
        int type = checkBeautiful(buf);
        if (type == -1) {
            puts("0");
            return 0;
        }
        else if (type > 0) {
            totalZeros += type;
        }
        else if (type == 0) {
            if (!hasBase) {
                strcpy(base, buf);
                hasBase = 1;
            }
        }
    }

    printf("%s", base);
    for (int i = 0; i < totalZeros; i++) putchar('0');
    putchar('\n');

    return 0;
}

