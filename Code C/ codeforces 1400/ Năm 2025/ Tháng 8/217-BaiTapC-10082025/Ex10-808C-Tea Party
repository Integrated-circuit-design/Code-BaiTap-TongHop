#include <stdio.h>

int main() {
    int n, w;
    scanf("%d %d", &n, &w);

    int a[105], minTea[105], extra[105];
    int sumMin = 0;

    for (int i = 0; i < n; i++) {
        scanf("%d", &a[i]);
        minTea[i] = (a[i] + 1) / 2;
        extra[i] = a[i] - minTea[i];
        sumMin += minTea[i];
    }

    if (sumMin > w) {
        printf("-1\n");
        return 0;
    }

    int rem = w - sumMin;

    for (int r = 0; r < n && rem > 0; r++) {
        int pos = 0;
        for (int i = 1; i < n; i++) {
            if (extra[i] > extra[pos]) pos = i;
        }
        int add = (extra[pos] < rem) ? extra[pos] : rem;
        minTea[pos] += add;
        rem -= add;
        extra[pos] = 0;
    }

    for (int i = 0; i < n; i++) {
        printf("%d%c", minTea[i], i == n - 1 ? '\n' : ' ');
    }

    return 0;
}

