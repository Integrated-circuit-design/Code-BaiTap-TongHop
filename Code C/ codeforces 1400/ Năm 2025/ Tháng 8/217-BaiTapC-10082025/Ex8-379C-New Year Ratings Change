#include <stdio.h>
#include <stdlib.h>

typedef long long ll;

ll arr[300005][2];
ll res[300005];

int cmp_pair(const void *p1, const void *p2) {
    ll a = ((ll*)p1)[0];
    ll b = ((ll*)p2)[0];
    return (a > b) - (a < b);
}

int main() {
    int n;
    scanf("%d", &n);
    for (int i = 0; i < n; i++) {
        scanf("%lld", &arr[i][0]);
        arr[i][1] = i;
    }

    qsort(arr, n, sizeof(arr[0]), cmp_pair);

    ll prev = 0;
    for (int i = 0; i < n; i++) {
        ll give = arr[i][0];
        if (i > 0 && give <= prev) give = prev + 1;
        res[arr[i][1]] = give;
        prev = give;
    }

    for (int i = 0; i < n; i++) {
        if (i) printf(" ");
        printf("%lld", res[i]);
    }
    printf("\n");
    return 0;
}

