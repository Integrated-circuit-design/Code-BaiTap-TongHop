#include <stdio.h>
#include <string.h>

int main() {
    char s[16];
    scanf("%s", s);

    int n = strlen(s);
    int a[12];

    for (int i = 0; i < n; i++) {
        a[i] = s[i] - 'A';
    }

    if (n <= 2) {
        printf("YES\n");
        return 0;
    }

    for (int i = 2; i < n; i++) {
        if (a[i] != (a[i-1] + a[i-2]) % 26) {
            printf("NO\n");
            return 0;
        }
    }

    printf("YES\n");
    return 0;
}

