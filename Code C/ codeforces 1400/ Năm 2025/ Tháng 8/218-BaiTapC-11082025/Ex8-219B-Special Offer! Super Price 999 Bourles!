#include <stdio.h>

int count_trailing_nines(long long x) {
    int cnt = 0;
    while (x % 10 == 9) {
        cnt++;
        x /= 10;
    }
    return cnt;
}

int main() {
    long long p, d;
    scanf("%lld %lld", &p, &d);

    long long best_price = p;
    int best_nines = count_trailing_nines(p);

    for (long long price = p; price >= p - d; price--) {
        int nines = count_trailing_nines(price);
        if (nines > best_nines || (nines == best_nines && price > best_price)) {
            best_price = price;
            best_nines = nines;
        }
    }

    printf("%lld\n", best_price);
    return 0;
}
