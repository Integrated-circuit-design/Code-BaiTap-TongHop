#include <stdio.h>

const char* determine_winner(int nodes) {
    int moves = nodes - 1;
    if (moves % 2 == 1) {
        return "Ron";
    } else {
        return "Hermione";
    }
}

int main() {
    int n, t;
    scanf("%d%d", &n, &t);

    for (int i = 0; i < n - 1; i++) {
        int a, b;
        scanf("%d%d", &a, &b);
    }

    int start_node;
    scanf("%d", &start_node);

    printf("%s\n", determine_winner(n));

    return 0;
}

