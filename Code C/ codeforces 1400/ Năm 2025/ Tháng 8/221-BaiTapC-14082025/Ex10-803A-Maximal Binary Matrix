#include <stdio.h>

typedef struct { int r, c; } Cell;
Cell pos[10005];
int mat[105][105];

int main() {
    int n, k;
    scanf("%d %d", &n, &k);

    if (k > n * n) {
        puts("-1");
        return 0;
    }

    int idx = 0;
    for (int i = 0; i < n; i++) {
        pos[idx++] = (Cell){i, i};
    }
    for (int i = 0; i < n; i++) {
        for (int j = i + 1; j < n; j++) {
            pos[idx++] = (Cell){i, j};
        }
    }

    int p = 0;
    while (p < idx && k > 0) {
        int r = pos[p].r, c = pos[p].c;
        if (r == c) {
            mat[r][c] = 1;
            k--;
        } else if (k >= 2) {
            mat[r][c] = mat[c][r] = 1;
            k -= 2;
        }
        p++;
    }

    if (k != 0) {
        puts("-1");
        return 0;
    }

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++)
            printf("%d%c", mat[i][j], (j == n - 1) ? '\n' : ' ');
    }
    return 0;
}

