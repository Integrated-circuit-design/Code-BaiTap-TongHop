#include <stdio.h>

int n, k;
int nums[8][8];
int perm[8];
int used[8];
int ans = 1000000000;

void dfs(int depth) {
    if (depth == k) {
        int minv = 1000000000, maxv = -1;
        for (int i = 0; i < n; i++) {
            int val = 0;
            for (int j = 0; j < k; j++)
                val = val * 10 + nums[i][perm[j]];
            if (val < minv) minv = val;
            if (val > maxv) maxv = val;
        }
        if (maxv - minv < ans) ans = maxv - minv;
        return;
    }
    for (int i = 0; i < k; i++) {
        if (!used[i]) {
            used[i] = 1;
            perm[depth] = i;
            dfs(depth + 1);
            used[i] = 0;
        }
    }
}

int main() {
    scanf("%d %d", &n, &k);
    for (int i = 0; i < n; i++) {
        char s[9];
        scanf("%s", s);
        for (int j = 0; j < k; j++)
            nums[i][j] = s[j] - '0';
    }

    for (int i = 0; i < k; i++) used[i] = 0;

    dfs(0);

    printf("%d\n", ans);
    return 0;
}

