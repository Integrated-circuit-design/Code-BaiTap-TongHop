#include <stdio.h>
#include <string.h>

int main() {
    char s[100005];
    scanf("%s", s);
    int n = strlen(s);

    int distinct = 0;
    for(char c = 'a'; c <= 'z'; c++) {
        for(int i = 0; i < n; i++) {
            if(s[i] == c) {
                distinct++;
                break;
            }
        }
    }

    if(distinct == 1) {
        printf("No\n");
    } 
    else if(distinct == 2) {
        int foundTwice = 0;
        for(char c = 'a'; c <= 'z'; c++) {
            int count = 0;
            for(int i = 0; i < n; i++) {
                if(s[i] == c) count++;
            }
            if(count >= 2) {
                foundTwice = 1;
                break;
            }
        }
        if(foundTwice) printf("Yes\n");
        else printf("No\n");
    } 
    else {
        printf("Yes\n");
    }

    return 0;
}

