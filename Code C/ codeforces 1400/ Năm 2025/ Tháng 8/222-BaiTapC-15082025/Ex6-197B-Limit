#include <stdio.h>
#include <stdlib.h>

int gcd_iter(int a, int b) {
    while (b != 0) {
        int r = a % b;
        a = b;
        b = r;
    }
    return a;
}

int main() {
    int degP, degQ;
    scanf("%d %d", &degP, &degQ);

    int leadP, leadQ;

    for (int i = 0; i <= degP; i++) {
        int coef;
        scanf("%d", &coef);
        if (i == 0) leadP = coef;
    }

    for (int i = 0; i <= degQ; i++) {
        int coef;
        scanf("%d", &coef);
        if (i == 0) leadQ = coef;
    }

    if (degP > degQ) {
        if ((leadP > 0 && leadQ > 0) || (leadP < 0 && leadQ < 0))
            puts("Infinity");
        else
            puts("-Infinity");
    } 
    else if (degP < degQ) {
        puts("0/1");
    } 
    else {
        int g = gcd_iter(abs(leadP), abs(leadQ));
        int num = leadP / g;
        int den = leadQ / g;
        if (den < 0) { num = -num; den = -den; }
        printf("%d/%d\n", num, den);
    }

    return 0;
}

