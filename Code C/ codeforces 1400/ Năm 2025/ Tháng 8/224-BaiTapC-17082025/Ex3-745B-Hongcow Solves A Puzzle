#include <stdio.h>

int main() {
    int n, m;
    scanf("%d %d", &n, &m);

    char grid[n][m+1];
    int minRow = n, maxRow = -1;
    int minCol = m, maxCol = -1;
    int totalX = 0;

    for (int i = 0; i < n; i++) {
        scanf("%s", grid[i]);
        for (int j = 0; j < m; j++) {
            if (grid[i][j] == 'X') {
                totalX++;
                if (i < minRow) minRow = i;
                if (i > maxRow) maxRow = i;
                if (j < minCol) minCol = j;
                if (j > maxCol) maxCol = j;
            }
        }
    }

    int solid = 1;
    for (int i = minRow; i <= maxRow && solid; i++) {
        for (int j = minCol; j <= maxCol; j++) {
            if (grid[i][j] != 'X') {
                solid = 0;
                break;
            }
        }
    }

    if (solid)
        printf("YES\n");
    else
        printf("NO\n");

    return 0;
}

