#include <stdio.h>
#define INF 1e18

double cookSoup(int n, int V, int a[], int b[]) {
    double sumA = 0;
    for (int i = 0; i < n; i++) sumA += a[i];

    double minRatio = INF;
    for (int i = 0; i < n; i++) {
        double ratio = (double)b[i] / a[i];
        if (ratio < minRatio) minRatio = ratio;
    }

    double result = minRatio * sumA;
    if (result > V) result = V;
    return result;
}

int main() {
    int n, V;
    scanf("%d %d", &n, &V);

    int a[25], b[25];
    for (int i = 0; i < n; i++) scanf("%d", &a[i]);
    for (int i = 0; i < n; i++) scanf("%d", &b[i]);

    double ans = cookSoup(n, V, a, b);
    printf("%.10f\n", ans);

    return 0;
}

