#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    long long wallet[200000];
    for (int i = 0; i < n; i++) {
        scanf("%lld", &wallet[i]);
    }

    long long limit, fee;
    scanf("%lld %lld", &limit, &fee);

    long long result = 0;

    for (int i = 0; i < n; i++) {
        if (wallet[i] > limit) {
            long long extra = wallet[i] - limit;
            long long step = limit + fee;
            long long cnt = (extra + step - 1) / step;
            result += cnt * fee;
        }
    }

    printf("%lld\n", result);
    return 0;
}

