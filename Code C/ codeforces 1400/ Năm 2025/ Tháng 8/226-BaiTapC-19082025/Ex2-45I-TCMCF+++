#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);
    int c[105];
    for (int i = 0; i < n; i++) scanf("%d", &c[i]);

    int negCount = 0, hasZero = 0;
    int minNeg = -101, minAbsNeg = 101;
    int maxNegIndex = -1, minAbsNegIndex = -1;

    for (int i = 0; i < n; i++) {
        if (c[i] < 0) {
            negCount++;
            if (c[i] > minNeg) { minNeg = c[i]; maxNegIndex = i; }
            if (-c[i] < minAbsNeg) { minAbsNeg = -c[i]; minAbsNegIndex = i; }
        }
        if (c[i] == 0) hasZero = 1;
    }

    int chosen[105] = {0};

    if (n == 1) {
        printf("%d\n", c[0]);
        return 0;
    }

    if (negCount % 2 == 1) {
        if (hasZero) {
            for (int i = 0; i < n; i++) if (c[i] > 0) chosen[i] = 1;
            if (negCount == n) {
                printf("0\n");
                return 0;
            }
        } else {
            for (int i = 0; i < n; i++) {
                if (i != minAbsNegIndex) chosen[i] = 1;
            }
        }
    } else {
        for (int i = 0; i < n; i++) {
            if (c[i] != 0) chosen[i] = 1;
        }
        int any = 0;
        for (int i = 0; i < n; i++) if (chosen[i]) any = 1;
        if (!any) {
            printf("0\n");
            return 0;
        }
    }

    int first = 1;
    for (int i = 0; i < n; i++) {
        if (chosen[i]) {
            if (!first) printf(" ");
            printf("%d", c[i]);
            first = 0;
        }
    }
    printf("\n");
    return 0;
}

