#include <stdio.h>

#define MAXN 100000

int main() {
    int n;
    long long a[MAXN];
    scanf("%d", &n);
    for (int i = 0; i < n; i++) scanf("%lld", &a[i]);

    for (int i = 0; i < n-1; i++) {
        int minIdx = i;
        for (int j = i+1; j < n; j++) {
            if (a[j] < a[minIdx]) minIdx = j;
        }
        if (minIdx != i) {
            long long tmp = a[i];
            a[i] = a[minIdx];
            a[minIdx] = tmp;
        }
    }

    long long maxSize = a[n-1];
    for (int i = 0; i < n-1; i++) {
        if (a[i] > maxSize / 2) {
            printf("YES\n");
            return 0;
        }
    }

    printf("NO\n");
    return 0;
}

