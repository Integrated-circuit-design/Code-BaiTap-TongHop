#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    char codes[1005][7];
    for (int i = 0; i < n; i++) {
        scanf("%s", codes[i]);
    }

    int minDist = 6;

    for (int i = 0; i < n; i++) {
        for (int j = i + 1; j < n; j++) {
            int diff = 0;
            for (int k = 0; k < 6; k++) {
                if (codes[i][k] != codes[j][k]) {
                    diff++;
                }
            }
            if (diff < minDist) {
                minDist = diff;
            }
        }
    }

    int k = (minDist - 1) / 2;
    printf("%d\n", k);

    return 0;
}

