#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int r[5001];
    for (int i = 0; i < n; i++) {
        scanf("%d", &r[i]);
    }

    int ans = 0;

    for (int i = 0; i < n; i++) {
        long long sum = 0;
        for (int j = i; j < n; j++) {
            sum += r[j];
            int length = j - i + 1;
            if (sum > 100LL * length) {
                if (length > ans) ans = length;
            }
        }
    }

    printf("%d\n", ans);
    return 0;
}

