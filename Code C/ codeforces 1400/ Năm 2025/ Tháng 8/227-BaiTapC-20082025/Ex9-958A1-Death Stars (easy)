#include <stdio.h>

#define MAXN 10
char A[MAXN][MAXN+1], B[MAXN][MAXN+1];
int n;

int check(int t) {
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            int x=i, y=j;
            if (t == 0) { x=i; y=j; }
            if (t == 1) { x=n-1-j; y=i; }
            if (t == 2) { x=n-1-i; y=n-1-j; }
            if (t == 3) { x=j; y=n-1-i; }
            if (t == 4) { x=i; y=n-1-j; }
            if (t == 5) { x=n-1-i; y=j; }
            if (t == 6) { x=j; y=i; }
            if (t == 7) { x=n-1-j; y=n-1-i; }

            if (A[x][y] != B[i][j]) return 0;
        }
    }
    return 1;
}

int main() {
    scanf("%d", &n);
    for (int i=0; i<n; i++) scanf("%s", A[i]);
    for (int i=0; i<n; i++) scanf("%s", B[i]);

    for (int t=0; t<8; t++) {
        if (check(t)) {
            puts("Yes");
            return 0;
        }
    }
    puts("No");
    return 0;
}

