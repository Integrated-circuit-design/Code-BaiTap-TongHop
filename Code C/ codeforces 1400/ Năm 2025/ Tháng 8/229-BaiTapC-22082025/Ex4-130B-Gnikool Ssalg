#include <stdio.h>
#include <string.h>

void reverseString(char str[]) {
    int i = 0, j = strlen(str) - 1;
    while(i < j) {
        char temp = str[i];
        str[i] = str[j];
        str[j] = temp;
        i++;
        j--;
    }
}

int main() {
    char s[105];
    scanf("%s", s);

    reverseString(s);
    printf("%s\n", s);
    return 0;
}

