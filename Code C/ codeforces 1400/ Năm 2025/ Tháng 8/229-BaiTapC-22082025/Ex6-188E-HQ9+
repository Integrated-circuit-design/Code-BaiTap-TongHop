#include <stdio.h>
#include <string.h>

int main() {
    char program[101];
    scanf("%100s", program);

    char *p = program;
    while (*p) {
        if (*p == 'H' || *p == 'Q' || *p == '9') {
            printf("YES\n");
            return 0;
        }
        p++;
    }

    printf("NO\n");
    return 0;
}

