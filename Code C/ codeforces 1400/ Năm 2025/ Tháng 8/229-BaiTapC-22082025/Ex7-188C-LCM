#include <stdio.h>

int main() {
    int a, b;
    scanf("%d %d", &a, &b);

    int originalA = a, originalB = b;

    while (b != 0) {
        int temp = b;
        b = a % b;
        a = temp;
    }

    int gcd = a;
    int lcm = (originalA * originalB) / gcd;
    printf("%d\n", lcm);

    return 0;
}

