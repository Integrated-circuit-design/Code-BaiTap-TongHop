#include <stdio.h>

int main() {
    int n, t;
    scanf("%d %d", &n, &t);

    if (t == 10 && n == 1) {
        printf("-1\n");
        return 0;
    }

    if (t == 10) {
        printf("1");
        for (int i = 1; i < n; i++) {
            printf("0");
        }
        printf("\n");
        return 0;
    }

    printf("%d", t);
    for (int i = 1; i < n - 1; i++) {
        printf("0");
    }
    printf("\n");

    return 0;
}

