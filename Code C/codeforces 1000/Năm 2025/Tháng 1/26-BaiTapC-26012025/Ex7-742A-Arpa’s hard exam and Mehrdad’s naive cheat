#include <stdio.h>

int main() {
    unsigned int n;
    scanf("%u", &n);

    if (n == 0) {
        printf("1\n");
    } else {
        int result;
        switch (n % 4) {
            case 1:
                result = 8;
                break;
            case 2:
                result = 4;
                break;
            case 3:
                result = 2;
                break;
            case 0:
                result = 6;
                break;
        }
        printf("%d\n", result);
    }

    return 0;
}

