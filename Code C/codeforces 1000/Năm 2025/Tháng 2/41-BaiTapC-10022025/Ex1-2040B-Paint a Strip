#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        int n;
        scanf("%d", &n);
        
        int operations = 0;
        
        while (n > 0) {
            n = n / 2;
            operations++;
        }
        
        printf("%d\n", operations);
    }
    
    return 0;
}

