#include <stdio.h>
#include <string.h>

int main() {
    char door[10];
    int rail;
    
    scanf("%s", door);
    scanf("%d", &rail);
    
    if ((strcmp(door, "front") == 0 && rail == 1) || (strcmp(door, "back") == 0 && rail == 2)) {
        printf("L\n");
    } else {
        printf("R\n");
    }
    
    return 0;
}

