#include <stdio.h>

int fibonacci(int n) {
    if (n == 1 || n == 2)
        return 1;
    return fibonacci(n - 1) + fibonacci(n - 2);
}

int main() {
    int n;
    printf("Nhập số nguyên n: ");
    scanf("%d", &n);
    
    printf("Số Fibonacci thứ %d là: %d\n", n, fibonacci(n));
    
    return 0;
}

