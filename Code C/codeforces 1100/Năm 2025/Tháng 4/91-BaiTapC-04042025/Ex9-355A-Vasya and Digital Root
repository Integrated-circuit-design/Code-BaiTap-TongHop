#include <stdio.h>

int main() {
    int k, d;
    scanf("%d %d", &k, &d);

    if (k > 1 && d == 0) {
        printf("No solution\n");
    } else {
        if (k == 1 && d == 0) {
            printf("0\n");
        } else {
            printf("%d", d);
            for (int i = 1; i < k; i++) {
                printf("0");
            }
            printf("\n");
        }
    }

    return 0;
}

