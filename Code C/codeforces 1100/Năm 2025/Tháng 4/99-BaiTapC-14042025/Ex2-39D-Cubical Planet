#include <stdio.h>

int main() {
    int x1, y1, z1;
    int x2, y2, z2;

    scanf("%d %d %d", &x1, &y1, &z1);
    scanf("%d %d %d", &x2, &y2, &z2);

    int count = 0;
    if (x1 != x2) count++;
    if (y1 != y2) count++;
    if (z1 != z2) count++;

    if (count == 1) {
        printf("YES\n");
    } else {
        printf("NO\n");
    }

    return 0;
}

