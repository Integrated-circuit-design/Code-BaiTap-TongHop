#include <stdio.h>

void kiemTraTongSoLe() {
    int t;
    scanf("%d", &t);
    
    while (t--) {
        int n, k;
        scanf("%d %d", &n, &k);
        
        int minTong = k * k;
        
        if (minTong > n || (n % 2 != k % 2)) {
            printf("KHONG\n");
        } else {
            printf("CO\n");
        }
    }
}

int main() {
    kiemTraTongSoLe();
    return 0;
}

