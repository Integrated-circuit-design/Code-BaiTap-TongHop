#include <stdio.h>

int main() {
    int coins[5], total = 0;
    
    for (int i = 0; i < 5; i++) {
        scanf("%d", &coins[i]);
        total += coins[i];
    }

    if (total > 0 && total % 5 == 0) {
        printf("%d\n", total / 5);
    } else {
        printf("-1\n");
    }

    return 0;
}

