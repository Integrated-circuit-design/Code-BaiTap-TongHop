#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);
    printf("%d\n", n);

    int left = 1, right = n;

    while (left <= right) {
        printf("%d ", left);
        left++;
        if (left <= right) {
            printf("%d ", right);
            right--;
        }
    }

    printf("\n");

    return 0;
}

