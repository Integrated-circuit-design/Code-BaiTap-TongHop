#include <stdio.h>

long long find_min_bulbs(long long k) {
    return k * k;
}

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        long long k;
        scanf("%lld", &k);
        
        long long result = find_min_bulbs(k);
        
        printf("%lld\n", result);
    }

    return 0;
}

