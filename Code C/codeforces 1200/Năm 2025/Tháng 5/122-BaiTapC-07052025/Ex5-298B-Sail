#include <stdio.h>

int main() {
    int t, sx, sy, ex, ey;
    char winds[100001];

    scanf("%d %d %d %d %d", &t, &sx, &sy, &ex, &ey);
    scanf("%s", winds);

    int x = sx, y = sy;

    for (int time = 0; time < t; time++) {
        char wind = winds[time];

        if (wind == 'E') {
            x++;
        } else if (wind == 'S') {
            y--;
        } else if (wind == 'W') {
            x--;
        } else if (wind == 'N') {
            y++;
        }

        if (x == ex && y == ey) {
            printf("%d\n", time + 1);
            return 0;
        }
    }

    printf("-1\n");

    return 0;
}

