#include <stdio.h>

#define MAXN 100005

void buildTree(int n) {
    for (int i = 1; i < n; i++) {
        printf("%d %d\n", i, i + 1);
    }
}

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        int n, m;
        scanf("%d %d", &n, &m);

        for (int i = 0; i < m; i++) {
            int a, b, c;
            scanf("%d %d %d", &a, &b, &c);
        }

        buildTree(n);
    }

    return 0;
}

