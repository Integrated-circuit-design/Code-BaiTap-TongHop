#include <stdio.h>

int main() {
    int a, b;
    
    scanf("%d %d", &a, &b);

    if (a == b) {
        printf("1\n");
        printf("%d\n", a);
    } else {
        printf("3\n");

        int sum = 3 * a - b;
        int x1 = sum / 2;
        int x2 = sum - x1;

        printf("%d %d %d\n", x1, b, x2);
    }

    return 0;
}

