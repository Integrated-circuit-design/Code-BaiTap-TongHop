#include <stdio.h>

int main() {
    int a, x, y;
    scanf("%d %d %d", &a, &x, &y);

    if (x > 0 && x < a && y > 0 && y < a) {
        printf("0\n");
    }
    else if (x == 0 || x == a || y == 0 || y == a) {
        if (x >= 0 && x <= a && y >= 0 && y <= a) {
            printf("1\n");
        }
    }
    else {
        printf("2\n");
    }

    return 0;
}

