#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        int n;
        scanf("%d", &n);

        int p[n];
        for (int i = 0; i < n; i++) {
            scanf("%d", &p[i]);
        }

        int first = p[0];
        int last = p[n - 1];
        
        if (first != last) {
            printf("2\n");
            printf("%d %d\n", first, last);
        } else {
            printf("2\n");
            printf("%d %d\n", first, p[1]);
        }
    }

    return 0;
}

