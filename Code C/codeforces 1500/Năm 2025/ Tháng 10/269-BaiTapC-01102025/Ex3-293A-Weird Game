#include <stdio.h>

int main() {
    int n; 
    scanf("%d", &n);
    char s[2000005], t[2000005];
    scanf("%s%s", s, t);

    int a=0, b=0, c=0, d=0; 
    for (int i=0; i<2*n; i++) {
        if (s[i]=='1' && t[i]=='1') a++;
        else if (s[i]=='1') b++;
        else if (t[i]=='1') c++;
        else d++;
    }

    int y=0, an=0;
    for (int i=0; i<2*n; i++) {
        if (i%2==0) {
            if (a) { y++; a--; }
            else if (b) { y++; b--; }
            else if (c) c--;
            else d--;
        } else {
            if (a) { an++; a--; }
            else if (c) { an++; c--; }
            else if (b) b--;
            else d--;
        }
    }

    puts(y>an ? "First" : y<an ? "Second" : "Draw");
    return 0;
}

