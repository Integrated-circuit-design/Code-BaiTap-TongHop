#include <stdio.h>

int a[25];

int same4(int x,int y,int z,int w){
    return (a[x]==a[y] && a[x]==a[z] && a[x]==a[w]);
}

int check(){
    if(same4(0,1,2,3)){
        if((a[4]==a[6] && a[6]==a[8] && a[8]==a[10] &&
            a[5]==a[7] && a[7]==a[9] && a[9]==a[11]) ||
           (a[4]==a[5] && a[5]==a[9] && a[9]==a[8] &&
            a[6]==a[7] && a[7]==a[11] && a[11]==a[10]))
            return 1;
    }
    if(same4(20,21,22,23)){
        if((a[6]==a[7] && a[7]==a[12] && a[12]==a[13] &&
            a[14]==a[15] && a[15]==a[16] && a[16]==a[17]) ||
           (a[6]==a[14] && a[14]==a[16] && a[16]==a[7] &&
            a[12]==a[13] && a[13]==a[15] && a[15]==a[17]))
            return 1;
    }
    if(same4(16,17,18,19)){
        if((a[0]==a[2] && a[2]==a[21] && a[21]==a[20] &&
            a[4]==a[5] && a[5]==a[22] && a[22]==a[23]) ||
           (a[0]==a[4] && a[4]==a[22] && a[22]==a[20] &&
            a[2]==a[5] && a[5]==a[23] && a[23]==a[21]))
            return 1;
    }
    if(same4(8,9,10,11)){
        if((a[1]==a[3] && a[3]==a[12] && a[12]==a[13] &&
            a[6]==a[7] && a[7]==a[20] && a[20]==a[21]) ||
           (a[1]==a[6] && a[6]==a[20] && a[20]==a[12] &&
            a[3]==a[7] && a[7]==a[21] && a[21]==a[13]))
            return 1;
    }
    if(same4(4,5,6,7)){
        if((a[2]==a[3] && a[3]==a[8] && a[8]==a[9] &&
            a[16]==a[17] && a[17]==a[20] && a[20]==a[21]) ||
           (a[2]==a[9] && a[9]==a[16] && a[16]==a[20] &&
            a[3]==a[8] && a[8]==a[17] && a[17]==a[21]))
            return 1;
    }
    if(same4(12,13,14,15)){
        if((a[0]==a[1] && a[1]==a[10] && a[10]==a[11] &&
            a[18]==a[19] && a[19]==a[22] && a[22]==a[23]) ||
           (a[0]==a[11] && a[11]==a[18] && a[18]==a[22] &&
            a[1]==a[10] && a[10]==a[19] && a[19]==a[23]))
            return 1;
    }
    return 0;
}

int main(){
    for(int i=0;i<24;i++) scanf("%d",&a[i]);
    if(check()) printf("YES\n");
    else printf("NO\n");
    return 0;
}

