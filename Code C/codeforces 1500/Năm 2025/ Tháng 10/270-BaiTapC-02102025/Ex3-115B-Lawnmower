#include <stdio.h>
#include <stdlib.h>

#define MAXN 155
#define MAXM 155

int main() {
    int n, m;
    char grid[MAXN][MAXM];
    
    scanf("%d %d", &n, &m);
    for (int i = 0; i < n; i++) scanf("%s", grid[i]);

    int totalMoves = 0;
    int leftPos = 0, rightPos = 0;
    int facingRight = 1;

    for (int i = 0; i < n; i++) {
        int minW = m, maxW = -1;
        for (int j = 0; j < m; j++) {
            if (grid[i][j] == 'W') {
                if (j < minW) minW = j;
                if (j > maxW) maxW = j;
            }
        }

        if (maxW == -1) {
            if (i < n - 1) totalMoves++;
            continue;
        }

        if (facingRight) {
            totalMoves += (maxW - rightPos);
            rightPos = maxW; 
        } else {
            totalMoves += (leftPos - minW);
            rightPos = minW;
        }

        if (i < n - 1) {
            totalMoves++;
            facingRight = 1 - facingRight;
        }

        leftPos = rightPos;
    }

    printf("%d\n", totalMoves);
    return 0;
}

