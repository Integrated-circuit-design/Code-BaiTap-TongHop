#include <stdio.h>

typedef long long ll;

int main() {
    int n;
    scanf("%d", &n);

    int i;
    int Xmin = 31400, Ymin = 31400;
    int Xmax = 0, Ymax = 0;
    ll area_sum = 0;

    for (i = 0; i < n; i++) {
        int a, b, c, d;
        scanf("%d %d %d %d", &a, &b, &c, &d);

        if (a < Xmin) Xmin = a;
        if (b < Ymin) Ymin = b;
        if (c > Xmax) Xmax = c;
        if (d > Ymax) Ymax = d;

        area_sum += (ll)(c - a) * (d - b);
    }

    int side1 = Xmax - Xmin;
    int side2 = Ymax - Ymin;
    ll box_area = (ll)side1 * side2;

    if (side1 == side2 && area_sum == box_area)
        puts("YES");
    else
        puts("NO");

    return 0;
}

