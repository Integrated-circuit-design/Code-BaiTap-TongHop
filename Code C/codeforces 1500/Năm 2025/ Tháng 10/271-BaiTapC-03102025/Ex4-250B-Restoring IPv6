#include <stdio.h>
#include <string.h>

int main() {
    int n; 
    scanf("%d\n", &n);
    while (n--) {
        char s[256];
        fgets(s, sizeof(s), stdin);
        int L = strlen(s);
        if (L && s[L-1] == '\n') s[L-1] = '\0';

        char *dbl = strstr(s, "::");
        char parts[16][8];
        int cnt = 0;

        if (dbl) {
            char tmp[256]; strcpy(tmp, s);
            int pos = dbl - s;
            tmp[pos] = '\0';
            char *right = dbl + 2;

            char *tok = strtok(tmp, ":");
            while (tok) {
                strcpy(parts[cnt++], tok);
                tok = strtok(NULL, ":");
            }
            int left_cnt = cnt;

            char *tok2 = strtok(right, ":");
            char rparts[8][8]; int rc = 0;
            while (tok2) {
                strcpy(rparts[rc++], tok2);
                tok2 = strtok(NULL, ":");
            }

            int zeros = 8 - (left_cnt + rc);

            for (int i=0;i<left_cnt;i++) {
                if (i) printf(":");
                printf("%04s", parts[i]);
            }
            for (int i=0;i<zeros;i++) {
                if (left_cnt || i) printf(":");
                printf("0000");
            }
            for (int i=0;i<rc;i++) {
                if (left_cnt+zeros+i) printf(":");
                printf("%04s", rparts[i]);
            }
            printf("\n");
        } else {
            char *tok = strtok(s, ":");
            while (tok) {
                if (cnt) printf(":");
                printf("%04s", tok);
                cnt++;
                tok = strtok(NULL, ":");
            }
            printf("\n");
        }
    }
    return 0;
}

