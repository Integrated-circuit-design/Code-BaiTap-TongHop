#include <stdio.h>
#include <string.h>
#include <ctype.h>

#define MAXN 105

int main() {
    int n, q;
    char s[MAXN];
    scanf("%d %d", &n, &q);
    scanf("%s", s);

    for (int query = 0; query < q; query++) {
        int l, r;
        scanf("%d %d", &l, &r);
        l--; r--;

        char program[MAXN];
        int len = r - l + 1;
        for (int i = 0; i < len; i++) program[i] = s[l + i];

        int prints[10] = {0};
        int pos = 0;
        int dir = 1;

        while (pos >= 0 && pos < len) {
            char c = program[pos];
            if (c >= '0' && c <= '9') {
                int digit = c - '0';
                prints[digit]++;
                digit--;
                if (digit < 0) {
                    for (int i = pos; i < len - 1; i++)
                        program[i] = program[i + 1];
                    len--;
                    if (dir == -1) pos--;
                } else {
                    program[pos] = digit + '0';
                }
                pos += dir;
            } else if (c == '>') {
                dir = 1;
                pos += dir;
                if (pos >= 0 && pos < len && (program[pos] == '<' || program[pos] == '>')) {
                    for (int i = pos - 1; i < len - 1; i++)
                        program[i] = program[i + 1];
                    len--;
                    pos--;
                }
            } else if (c == '<') {
                dir = -1;
                pos += dir;
                if (pos >= 0 && pos < len && (program[pos] == '<' || program[pos] == '>')) {
                    for (int i = pos + 1; i < len - 1; i++)
                        program[i] = program[i + 1];
                    len--;
                }
            }
        }

        for (int i = 0; i < 10; i++) {
            if (i) printf(" ");
            printf("%d", prints[i]);
        }
        printf("\n");
    }

    return 0;
}

