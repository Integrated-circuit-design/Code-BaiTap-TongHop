#include <stdio.h>

int main() {
    char str[60];
    double p;
    int total = 0;
    int bad = 0;
    int unknown = 0;

    scanf("%s", str);
    scanf("%lf", &p);

    for (int i = 0; str[i] != '\0'; i++) {
        total++;
        if (str[i] == '1')
            bad++;
        else if (str[i] == '?')
            unknown++;
    }

    double sum = bad + unknown * p;
    double result = sum / total;

    printf("%.5f\n", result);

    return 0;
}

