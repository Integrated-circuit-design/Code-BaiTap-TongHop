#include <stdio.h>
#include <math.h>

int main() {
    double a, b, c;
    scanf("%lf %lf %lf", &a, &b, &c);

    double delta = b*b - 4*a*c;

    if (delta < 0) {
        return 0;
    }

    double r = sqrt(delta);
    double root1 = (-b + r) / (2 * a);
    double root2 = (-b - r) / (2 * a);

    if (fabs(root1 - root2) < 1e-9) {
        printf("%.6f\n", root1);
    } else {
        if (root1 > root2) {
            printf("%.6f %.6f\n", root2, root1);
        } else {
            printf("%.6f %.6f\n", root1, root2);
        }
    }

    return 0;
}

