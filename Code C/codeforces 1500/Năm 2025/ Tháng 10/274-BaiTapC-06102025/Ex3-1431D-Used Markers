#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);
    while (t--) {
        int n;
        scanf("%d", &n);
        int a[n+1];
        int pos[n+1];
        for (int i = 1; i <= n; i++) {
            scanf("%d", &a[i]);
            pos[i] = i;
        }

        int count[n+1];
        for (int i = 0; i <= n; i++) count[i] = 0;

        int order[n+1];
        int idx = 0;

        for (int val = n; val >= 1; val--) {
            for (int i = 1; i <= n; i++) {
                if (a[i] == val) {
                    order[idx++] = pos[i];
                }
            }
        }

        for (int i = 0; i < n; i++) {
            printf("%d%c", order[i], i == n-1 ? '\n' : ' ');
        }
    }
    return 0;
}

