#include <stdio.h>
#include <stdlib.h>

typedef long long ll;

int cmp(const void *a, const void *b) {
    ll x = *(ll*)a;
    ll y = *(ll*)b;
    if (x < y) return 1;
    if (x > y) return -1;
    return 0;
}

int main() {
    int n;
    scanf("%d", &n);

    ll a[2005];
    for (int i = 0; i < n; i++) scanf("%lld", &a[i]);

    ll sum = 0;
    int count = 0;
    ll neg[2005];
    int negSize = 0;

    for (int i = 0; i < n; i++) {
        sum += a[i];
        count++;

        if (a[i] < 0) {
            neg[negSize++] = a[i];
        }

        if (sum < 0) {
            qsort(neg, negSize, sizeof(ll), cmp);
            sum -= neg[0];
            for (int j = 1; j < negSize; j++) neg[j-1] = neg[j];
            negSize--;
            count--;
        }
    }

    printf("%d\n", count);
    return 0;
}

