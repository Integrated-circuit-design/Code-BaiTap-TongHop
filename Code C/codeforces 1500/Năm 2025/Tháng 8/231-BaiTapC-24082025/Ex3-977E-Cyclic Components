#include <stdio.h>

#define MAXN 200005
#define MAXM 200005

int n, m;
int deg[MAXN], vis[MAXN];
int head[MAXN], to[MAXM * 2], nxt[MAXM * 2], edgeCnt = 0;

void add_edge(int u, int v) {
    to[++edgeCnt] = v;
    nxt[edgeCnt] = head[u];
    head[u] = edgeCnt;
}

int queue[MAXN];

int bfs(int start) {
    int front = 0, back = 0;
    queue[back++] = start;
    vis[start] = 1;

    int ok = 1;
    while (front < back) {
        int u = queue[front++];
        if (deg[u] != 2) ok = 0;

        for (int e = head[u]; e; e = nxt[e]) {
            int v = to[e];
            if (!vis[v]) {
                vis[v] = 1;
                queue[back++] = v;
            }
        }
    }
    return ok;
}

int main() {
    scanf("%d %d", &n, &m);
    for (int i = 0; i < m; i++) {
        int u, v;
        scanf("%d %d", &u, &v);
        add_edge(u, v);
        add_edge(v, u);
        deg[u]++; deg[v]++;
    }

    int ans = 0;
    for (int i = 1; i <= n; i++) {
        if (!vis[i]) {
            if (bfs(i)) ans++;
        }
    }

    printf("%d\n", ans);
    return 0;
}

