#include <stdio.h>

int canMakeEqual(int n, int arr[]) {
    int xorsum = 0;
    for (int i = 0; i < n; i++) xorsum ^= arr[i];

    if (xorsum == 0) return 1;

    int prefix = 0, found = 0;
    for (int i = 0; i < n; i++) {
        prefix ^= arr[i];
        if (prefix == xorsum) {
            found++;
            prefix = 0;
        }
    }
    return (found >= 2);
}

int main() {
    int t;
    scanf("%d", &t);
    while (t--) {
        int n, a[2005];
        scanf("%d", &n);
        for (int i = 0; i < n; i++) scanf("%d", &a[i]);

        if (canMakeEqual(n, a)) printf("YES\n");
        else printf("NO\n");
    }
    return 0;
}

