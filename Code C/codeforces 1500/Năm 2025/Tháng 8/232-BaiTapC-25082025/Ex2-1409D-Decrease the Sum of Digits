#include <stdio.h>
#include <string.h>

int sumDigits(char s[]) {
    int sum = 0;
    for (int i = 0; s[i]; i++) sum += s[i] - '0';
    return sum;
}

void roundUp(char s[], int i) {
    while (i >= 0 && s[i] == '9') {
        s[i] = '0';
        i--;
    }
    if (i >= 0) s[i]++;
    else {
        int len = strlen(s);
        for (int j = len; j >= 0; j--) s[j+1] = s[j];
        s[0] = '1';
    }
}

int main() {
    int t;
    scanf("%d", &t);
    while (t--) {
        unsigned long long n; int s;
        scanf("%llu %d", &n, &s);

        char str[32];
        sprintf(str, "%llu", n);

        if (sumDigits(str) <= s) {
            printf("0\n");
            continue;
        }

        char original[32];
        strcpy(original, str);

        int len = strlen(str);
        for (int i = len - 1; i >= 0 && sumDigits(str) > s; i--) {
            if (str[i] != '0') roundUp(str, i);
        }

        unsigned long long newN = 0;
        for (int i = 0; str[i]; i++) newN = newN * 10 + (str[i] - '0');

        printf("%llu\n", newN - n);
    }
    return 0;
}

