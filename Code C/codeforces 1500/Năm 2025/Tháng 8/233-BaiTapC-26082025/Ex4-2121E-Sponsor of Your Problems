#include <stdio.h>

int min_f(int l, int r) {
    int digits[10];
    int temp_l = l, temp_r = r;
    int len = 0;

    while(temp_l > 0) {
        digits[len] = (temp_l % 10 == temp_r % 10) ? 1 : 0;
        temp_l /= 10;
        temp_r /= 10;
        len++;
    }

    int same = 0;
    for(int i = 0; i < len; i++) same += digits[i];

    return 2 * same;
}

int main() {
    int t;
    scanf("%d", &t);
    while(t--) {
        int l, r;
        scanf("%d %d", &l, &r);
        printf("%d\n", min_f(l, r));
    }
    return 0;
}

