#include <stdio.h>

#define MAX 200005

int garden[MAX];
int required[MAX];

int main() {
    int t;
    scanf("%d", &t);

    while(t--) {
        int n, m;
        scanf("%d %d", &n, &m);

        for(int i = 0; i < n; i++) scanf("%d", &garden[i]);
        for(int i = 0; i < m; i++) scanf("%d", &required[i]);

        int needed_k = 0;
        int pos = 0;

        for(int j = 0; j < m; j++) {
            int found = 0;
            while(pos < n) {
                if(garden[pos] >= required[j]) {
                    found = 1;
                    pos++;
                    break;
                }
                pos++;
            }
            if(!found) {
                needed_k = required[j];
                break;
            }
        }

        if(needed_k == 0) {
            printf("0\n");
        } else {
            printf("%d\n", needed_k);
        }
    }

    return 0;
}

