#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);

    while(t--) {
        int n;
        scanf("%d", &n);
        int a[n];
        for(int i = 0; i < n; i++) scanf("%d", &a[i]);

        int clones[n];
        int clones_count = 0;

        for(int i = 0; i < n; i++) {
            int placed = 0;
            for(int j = clones_count - 1; j >= 0; j--) {
                if(clones[j] >= a[i]) {
                    clones[j] = a[i];
                    placed = 1;
                    break;
                }
            }
            if(!placed) {
                clones[clones_count++] = a[i];
            }
        }

        printf("%d\n", clones_count);
    }

    return 0;
}

