#include <stdio.h>
#include <string.h>

#define MAXN 100005

char s[MAXN];
char w[MAXN];

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        scanf("%s", s);
        int n = strlen(s);
        int x;
        scanf("%d", &x);

        for (int i = 0; i < n; i++) w[i] = '0';
        w[n] = '\0';

        for (int i = 0; i < n; i++) {
            if (s[i] == '1') {
                if (i + x < n) w[i + x] = '1';
                else if (i - x >= 0) w[i - x] = '1';
            }
        }

        int valid = 1;
        for (int i = 0; i < n; i++) {
            int one = 0;
            if (i - x >= 0 && w[i - x] == '1') one = 1;
            if (i + x < n && w[i + x] == '1') one = 1;
            if (one != (s[i] - '0')) {
                valid = 0;
                break;
            }
        }

        if (valid) printf("%s\n", w);
        else printf("-1\n");
    }

    return 0;
}

