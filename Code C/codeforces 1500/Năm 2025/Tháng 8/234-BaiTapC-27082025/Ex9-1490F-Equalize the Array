#include <stdio.h>
#include <stdlib.h>

#define MAXN 200005

long long a[MAXN];
long long freq[MAXN];
int n;

int cmp_ll(const void *x, const void *y){
    long long a = *(long long*)x;
    long long b = *(long long*)y;
    return (a > b) - (a < b);
}

int main(){
    int t;
    scanf("%d",&t);
    while(t--){
        scanf("%d",&n);
        for(int i=0;i<n;i++) scanf("%lld",&a[i]);

        qsort(a,n,sizeof(long long),cmp_ll);

        int m=0;
        for(int i=0;i<n;){
            int j=i;
            while(j<n && a[j]==a[i]) j++;
            freq[m++] = j-i;
            i=j;
        }

        qsort(freq,m,sizeof(long long),cmp_ll);

        long long ans = n;
        for(int i=0;i<m;i++){
            long long C = freq[i];
            long long remove = 0;
            for(int j=0;j<m;j++){
                if(freq[j]<C) remove += freq[j];
                if(freq[j]>C) remove += freq[j]-C;
            }
            if(remove < ans) ans = remove;
        }
        printf("%lld\n",ans);
    }
    return 0;
}

