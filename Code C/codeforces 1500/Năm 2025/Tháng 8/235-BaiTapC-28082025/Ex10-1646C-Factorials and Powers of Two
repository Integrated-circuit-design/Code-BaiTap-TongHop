#include <stdio.h>

typedef long long ll;

ll facts[20];
int fcnt = 0;

void init_factorials() {
    facts[fcnt++] = 1;
    ll f = 1;
    for (int i = 2; ; i++) {
        f *= i;
        if (f > (ll)1e12) break;
        facts[fcnt++] = f;
    }
}

int popcount(ll x) {
    int cnt = 0;
    while (x) {
        cnt += x & 1;
        x >>= 1;
    }
    return cnt;
}

int min_powerful_sum(ll n) {
    int ans = 0;
    ll used[20] = {0};
    int used_count = 0;

    for (int i = fcnt - 1; i >= 0; i--) {
        if (facts[i] <= n) {
            n -= facts[i];
            ans++;
        }
    }

    if (n > 0) {
        ans += popcount(n);
    }

    return ans;
}

int main() {
    init_factorials();

    int t;
    scanf("%d", &t);
    while (t--) {
        ll n;
        scanf("%lld", &n);
        int res = min_powerful_sum(n);
        printf("%d\n", res);
    }
    return 0;
}

