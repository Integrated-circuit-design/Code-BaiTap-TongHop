#include <stdio.h>

int gcd(int a, int b) {
    return (b == 0) ? a : gcd(b, a % b);
}

int main() {
    int T; 
    scanf("%d", &T);
    while (T--) {
        int n, arr[5005];
        scanf("%d", &n);

        for (int i = 0; i < n; i++) {
            scanf("%d", &arr[i]);
        }

        int g = arr[0];
        for (int i = 1; i < n; i++) {
            g = gcd(g, arr[i]);
        }

        int equalG = 0;
        for (int i = 0; i < n; i++) {
            if (arr[i] == g) equalG++;
        }

        if (equalG > 0) {
            printf("%d\n", n - equalG);
        } else {
            printf("%d\n", n);
        }
    }
    return 0;
}

