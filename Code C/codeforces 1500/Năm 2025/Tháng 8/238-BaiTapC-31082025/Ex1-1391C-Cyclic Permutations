#include <stdio.h>

#define MOD 1000000007LL
typedef long long ll;

ll factorial(int n) {
    ll f = 1;
    for (int i = 1; i <= n; i++) {
        f = (f * i) % MOD;
    }
    return f;
}

ll power(ll a, int b) {
    ll result = 1;
    for (int i = 0; i < b; i++) {
        result = (result * a) % MOD;
    }
   

