#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int prime[1005] = {0};
    int ans[1005], cnt = 0;

    for (int i = 2; i <= n; i++) prime[i] = 1;
    for (int i = 2; i * i <= n; i++) {
        if (prime[i]) {
            for (int j = i * i; j <= n; j += i)
                prime[j] = 0;
        }
    }

    for (int p = 2; p <= n; p++) {
        if (prime[p]) {
            int x = p;
            while (x <= n) {
                ans[cnt++] = x;
                x *= p;
            }
        }
    }

    printf("%d\n", cnt);
    for (int i = 0; i < cnt; i++) {
        printf("%d ", ans[i]);
    }
    printf("\n");

    return 0;
}

