#include <stdio.h>

#define ll long long
#define MAXN 200005

ll a[MAXN], b[MAXN], ans[MAXN];

int main() {
    int t;
    scanf("%d", &t);

    while(t--) {
        int n;
        scanf("%d", &n);

        for(int i=1; i<=n; i++) scanf("%lld", &a[i]);
        for(int i=1; i<=n; i++) scanf("%lld", &b[i]);

        for(int i=1; i<=n; i++) ans[i] = 0;

        int j = 1;
        ll remain = 0;

        for(int i=1; i<=n; i++) {
            remain += a[i];

            while(j <= i && remain >= b[j]) {
                ans[j] += b[j];
                remain -= b[j];
                j++;
            }
            if(j <= i) {
                ans[j] += remain;
            }
        }

        for(int i=1; i<=n; i++) {
            printf("%lld%c", ans[i], " \n"[i==n]);
        }
    }
    return 0;
}

