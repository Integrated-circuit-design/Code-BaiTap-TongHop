#include <stdio.h>

int count_positive_subarrays(int a[], int n) {
    int cnt = 0;
    for (int l = 0; l < n; l++) {
        int sum = 0;
        for (int r = l; r < n; r++) {
            sum += a[r];
            if (sum > 0) cnt++;
        }
    }
    return cnt;
}

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        int n, k;
        scanf("%d %d", &n, &k);

        int a[35];

        for (int i = 0; i < n; i++) a[i] = -1;

        for (int i = 0; i < n; i++) {
            a[i] = 1000;
            int cnt = count_positive_subarrays(a, n);

            if (cnt > k) {
                a[i] = -1000;
            } else if (cnt == k) {
                for (int j = i + 1; j < n; j++) a[j] = -1000;
                break;
            }
        }

        for (int i = 0; i < n; i++) {
            printf("%d%c", a[i], (i == n - 1 ? '\n' : ' '));
        }
    }

    return 0;
}

