#include <stdio.h>

#define MAXN 200005

int a[MAXN];
int updates_left[MAXN];

int sum_digits(int x) {
    int s = 0;
    while (x > 0) {
        s += x % 10;
        x /= 10;
    }
    return s;
}

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        int n, q;
        scanf("%d %d", &n, &q);

        for (int i = 1; i <= n; i++) {
            scanf("%d", &a[i]);
            if (a[i] > 9) updates_left[i] = 3;
            else updates_left[i] = 0;
        }

        while (q--) {
            int type;
            scanf("%d", &type);

            if (type == 1) {
                int l, r;
                scanf("%d %d", &l, &r);
                for (int i = l; i <= r; i++) {
                    if (updates_left[i] > 0) {
                        a[i] = sum_digits(a[i]);
                        updates_left[i]--;
                        if (a[i] <= 9) updates_left[i] = 0;
                    }
                }
            } else {
                int x;
                scanf("%d", &x);
                printf("%d\n", a[x]);
            }
        }
    }

    return 0;
}

