#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);

    while(t--) {
        int n, m;
        scanf("%d %d", &n, &m);

        int a[30][30];
        for(int i = 0; i < n; i++)
            for(int j = 0; j < m; j++)
                scanf("%d", &a[i][j]);

        int result = 0;
        int max_dist = n + m - 2;

        for(int dist = 0; dist <= max_dist / 2; dist++) {
            int count0 = 0, count1 = 0;

            for(int i = 0; i < n; i++) {
                for(int j = 0; j < m; j++) {
                    int d = i + j;
                    if(d == dist || d == max_dist - dist) {
                        if(a[i][j] == 0) count0++;
                        else count1++;
                    }
                }
            }

            if(dist != max_dist - dist) {
                result += (count0 < count1 ? count0 : count1);
            }
        }

        printf("%d\n", result);
    }

    return 0;
}

