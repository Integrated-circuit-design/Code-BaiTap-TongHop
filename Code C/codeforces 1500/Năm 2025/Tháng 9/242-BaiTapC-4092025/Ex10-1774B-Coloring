#include <stdio.h>

const char* coTheTo(long long n, int m, long long k, long long a[]) {
    long long gioiHan = (n + k - 1) / k;

    for (int i = 0; i < m; i++) {
        if (a[i] > gioiHan) {
            return "NO";
        }
    }
    return "YES";
}

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        long long n, k;
        int m;
        scanf("%lld %d %lld", &n, &m, &k);

        long long a[m];
        for (int i = 0; i < m; i++) {
            scanf("%lld", &a[i]);
        }

        printf("%s\n", coTheTo(n, m, k, a));
    }

    return 0;
}

