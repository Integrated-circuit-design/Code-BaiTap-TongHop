#include <stdio.h>

#define MAXN 200005

int f[MAXN];
int used[MAXN];
int zero[MAXN], freev[MAXN];

int main() {
    int n;
    scanf("%d", &n);

    for (int i = 1; i <= n; i++) {
        scanf("%d", &f[i]);
        if (f[i] != 0) used[f[i]] = 1;
    }

    int zc = 0, fc = 0;
    for (int i = 1; i <= n; i++) {
        if (f[i] == 0) zero[zc++] = i;
        if (!used[i]) freev[fc++] = i;
    }

    for (int i = 0; i < zc; i++) {
        f[zero[i]] = freev[(i + 1) % fc];
    }

    for (int i = 0; i < zc; i++) {
        if (f[zero[i]] == zero[i]) {
            int j = (i + 1) % zc;
            int tmp = f[zero[i]];
            f[zero[i]] = f[zero[j]];
            f[zero[j]] = tmp;
        }
    }

    for (int i = 1; i <= n; i++) {
        printf("%d ", f[i]);
    }
    printf("\n");

    return 0;
}

