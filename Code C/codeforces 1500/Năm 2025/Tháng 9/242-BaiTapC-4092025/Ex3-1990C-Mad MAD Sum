#include <stdio.h>

#define MAXN 200005

int a[MAXN], b[MAXN], freq[MAXN];

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        int n;
        scanf("%d", &n);

        for (int i = 0; i < n; i++) {
            scanf("%d", &a[i]);
        }

        long long ans = 0;

        while (1) {
            long long total = 0;
            for (int i = 0; i < n; i++) total += a[i];
            ans += total;
            if (total == 0) break;

            for (int i = 0; i <= n; i++) freq[i] = 0;

            int mad = 0;
            for (int i = 0; i < n; i++) {
                int x = a[i];
                freq[x]++;
                if (freq[x] == 2 && x > mad) mad = x;
                else if (freq[x] > 2 && x > mad) mad = x;
                b[i] = mad;
            }

            for (int i = 0; i < n; i++) a[i] = b[i];
        }

        printf("%lld\n", ans);
    }
    return 0;
}

