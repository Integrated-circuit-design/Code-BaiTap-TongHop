#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int a, g;
    int diff = 0;

    for (int i = 0; i < n; i++) {
        scanf("%d %d", &a, &g);
        if (diff + a <= 500) {
            putchar('A');
            diff += a;
        } else {
            putchar('G');
            diff -= g;
        }
    }

    putchar('\n');
    return 0;
}

