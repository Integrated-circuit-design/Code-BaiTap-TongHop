#include <stdio.h>

#define MAXN 100005

int p[MAXN], pos[MAXN];

int main() {
    int t;
    scanf("%d", &t);
    while (t--) {
        int n;
        scanf("%d", &n);

        for (int i = 1; i <= n; i++) {
            scanf("%d", &p[i]);
            pos[p[i]] = i;
        }

        int ok = 1;
        int i = 1;
        while (i <= n) {
            int start = pos[i];
            int j = i;
            while (j < n && pos[j+1] == pos[j] + 1) {
                j++;
            }
            i = j + 1;
        }

        printf(ok ? "Yes\n" : "No\n");
    }
    return 0;
}

