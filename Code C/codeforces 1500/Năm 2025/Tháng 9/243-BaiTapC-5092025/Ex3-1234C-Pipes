#include <stdio.h>

int main() {
    int q;
    scanf("%d", &q);

    while (q--) {
        int n;
        scanf("%d", &n);

        char top[n+1], bottom[n+1];
        scanf("%s %s", top, bottom);

        int row = 0;
        int ok = 1;

        for (int i = 0; i < n; i++) {
            char pipe = (row == 0 ? top[i] : bottom[i]);

            if (pipe >= '1' && pipe <= '2') {
                if (row == 1) {
                    ok = 0;
                    break;
                }
            } else {
                row ^= 1;
            }
        }

        if (ok && row == 1)
            printf("YES\n");
        else
            printf("NO\n");
    }

    return 0;
}

