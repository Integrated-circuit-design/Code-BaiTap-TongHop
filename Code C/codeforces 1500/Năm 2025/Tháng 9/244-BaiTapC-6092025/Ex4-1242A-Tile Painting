#include <stdio.h>
#include <math.h>

int main() {
    long long n;
    scanf("%lld", &n);

    if (n == 1) {
        printf("1\n");
        return 0;
    }

    long long ans = n;

    for (long long i = 2; i * i <= n; i++) {
        if (n % i == 0) {
            ans = i;
            break;
        }
    }

    printf("%lld\n", ans);
    return 0;
}

