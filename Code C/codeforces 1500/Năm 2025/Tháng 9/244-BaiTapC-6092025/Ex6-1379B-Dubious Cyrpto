#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        long long l, r, m;
        scanf("%lld %lld %lld", &l, &r, &m);

        long long a, b, c;
        int found = 0;

        for (a = l; a <= r && !found; a++) {
            for (long long n = 1; n <= 2; n++) {
                long long temp = m - n * a;
                if (temp >= -(r - l) && temp <= r - l) {
                    if (temp >= 0) {
                        b = l + temp;
                        c = l;
                    } else {
                        b = l;
                        c = l - temp;
                    }
                    printf("%lld %lld %lld\n", a, b, c);
                    found = 1;
                    break;
                }
            }
        }
    }
    return 0;
}

