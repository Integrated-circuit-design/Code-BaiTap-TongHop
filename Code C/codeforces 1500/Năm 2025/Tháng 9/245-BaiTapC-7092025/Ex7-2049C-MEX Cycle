#include <stdio.h>

#define MAXN 200005

int a[MAXN];

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        int n, x, y;
        scanf("%d %d %d", &n, &x, &y);

        for (int i = 1; i <= n; i++) {
            a[i] = (i - 1) % 3;
        }

        for (int i = 1; i <= n; i++) {
            printf("%d%c", a[i], (i == n ? '\n' : ' '));
        }
    }
    return 0;
}

