#include <stdio.h>

#define MAXN 200005

int a[MAXN], used[MAXN], result[MAXN];

int main() {
    int t;
    scanf("%d", &t);
    while (t--) {
        int n;
        scanf("%d", &n);
        for (int i = 0; i < n; i++) {
            scanf("%d", &a[i]);
            used[i] = 0;
        }

        int cur_or = 0;
        int res_len = 0;

        for (int step = 0; step < n; step++) {
            int best_index = -1;
            int best_val = -1;
            for (int i = 0; i < n; i++) {
                if (!used[i]) {
                    int val = cur_or | a[i];
                    if (val > best_val) {
                        best_val = val;
                        best_index = i;
                    }
                }
            }
            result[res_len++] = a[best_index];
            used[best_index] = 1;
            cur_or |= a[best_index];
        }

        for (int i = 0; i < res_len; i++) {
            printf("%d ", result[i]);
        }
        printf("\n");
    }
    return 0;
}

