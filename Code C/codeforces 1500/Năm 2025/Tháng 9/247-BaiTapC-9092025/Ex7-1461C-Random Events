#include <stdio.h>

typedef struct {
    int r;
    double p;
} Experiment;

int main() {
    int t;
    scanf("%d", &t);

    for (int test = 0; test < t; test++) {
        int n, m;
        scanf("%d %d", &n, &m);

        int a[n];
        for (int i = 0; i < n; i++) {
            scanf("%d", &a[i]);
        }

        Experiment exp[m];
        for (int i = 0; i < m; i++) {
            scanf("%d %lf", &exp[i].r, &exp[i].p);
        }

        int last = n - 1;
        while (last >= 0 && a[last] == last + 1) last--;

        if (last == -1) {
            printf("1.000000\n");
            continue;
        }

        double prob_unsorted = 1.0;
        for (int i = 0; i < m; i++) {
            if (exp[i].r > last) {
                prob_unsorted *= (1.0 - exp[i].p);
            }
        }

        double prob_sorted = 1.0 - prob_unsorted;
        printf("%.6lf\n", prob_sorted);
    }

    return 0;
}

