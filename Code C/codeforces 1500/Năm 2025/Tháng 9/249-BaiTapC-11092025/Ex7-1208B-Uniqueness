#include <stdio.h>
#include <stdbool.h>

#define MAXN 2000

int a[MAXN];
bool used[MAXN * 2];

int main() {
    int n;
    scanf("%d", &n);
    for(int i = 0; i < n; i++) scanf("%d", &a[i]);

    int min_remove = n;

    for(int l = 0; l < n; l++) {
        for(int r = l; r < n; r++) {
            bool conflict = false;

            for(int i = 0; i < n; i++) used[i] = false;

            for(int i = 0; i < l; i++) {
                for(int j = 0; j < i; j++) {
                    if(a[i] == a[j]) { conflict = true; break; }
                }
                if(conflict) break;
            }
            if(conflict) continue;

            for(int i = r+1; i < n; i++) {
                for(int j = r+1; j < i; j++) {
                    if(a[i] == a[j]) { conflict = true; break; }
                }
                if(conflict) break;
            }
            if(conflict) continue;

            for(int i = 0; i < l && !conflict; i++)
                for(int j = r+1; j < n; j++)
                    if(a[i] == a[j]) conflict = true;

            if(!conflict) {
                int remove_len = r - l + 1;
                if(remove_len < min_remove) min_remove = remove_len;
            }
        }
    }

    printf("%d\n", min_remove);
    return 0;
}

