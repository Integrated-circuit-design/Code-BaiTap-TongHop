#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);
    while (t--) {
        int n;
        scanf("%d", &n);
        int p[400005];
        for (int i = 0; i < n; i++) scanf("%d", &p[i]);

        int half = n / 2;
        int g = 0, s = 0, b = 0;

        int i = 0;
        while (i < n && p[i] == p[0]) {
            g++; i++;
        }

        while (i < n && s <= g) {
            int val = p[i], cnt = 0;
            while (i < n && p[i] == val) { cnt++; i++; }
            s += cnt;
        }

        while (i < n && g + s + b + (p[i] == p[i]) <= half) {
            int val = p[i], cnt = 0;
            while (i < n && p[i] == val) { cnt++; i++; }
            if (g + s + b + cnt <= half) b += cnt;
            else break;
        }

        if (g > 0 && s > 0 && b > 0 && g < s && g < b) {
            printf("%d %d %d\n", g, s, b);
        } else {
            printf("0 0 0\n");
        }
    }
    return 0;
}

