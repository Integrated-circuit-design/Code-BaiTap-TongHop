#include <stdio.h>

int lowbit(int x) {
    return x & -x;
}

int main() {
    int sum, limit;
    scanf("%d %d", &sum, &limit);

    int S[100005];
    int n = 0;

    for (int i = 1; i <= limit; i++) {
        int lb = lowbit(i);
        if (sum >= lb) {
            S[n++] = i;
            sum -= lb;
        }
        if (sum == 0) break;
    }

    if (sum != 0) {
        printf("-1\n");
    } else {
        printf("%d\n", n);
        for (int i = 0; i < n; i++) {
            printf("%d ", S[i]);
        }
        printf("\n");
    }

    return 0;
}

