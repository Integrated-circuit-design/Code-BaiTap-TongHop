#include <stdio.h>
#include <stdlib.h>

#define MAX 105

typedef struct {
    int x, y, d;
} Node;

int n, m;
char grid[MAX][MAX];
int vis[MAX][MAX];
int dx[4] = {1, -1, 0, 0};
int dy[4] = {0, 0, 1, -1};

int bfs(int sx, int sy) {
    Node q[MAX*MAX];
    int front = 0, back = 0;
    int maxDist = 0;

    for(int i=0;i<n;i++)
        for(int j=0;j<m;j++)
            vis[i][j] = 0;

    vis[sx][sy] = 1;
    q[back++] = (Node){sx, sy, 0};

    while(front < back) {
        Node cur = q[front++];
        if(cur.d > maxDist) maxDist = cur.d;

        for(int k=0;k<4;k++) {
            int nx = cur.x + dx[k];
            int ny = cur.y + dy[k];
            if(nx>=0 && nx<n && ny>=0 && ny<m &&
               grid[nx][ny]=='.' && !vis[nx][ny]) {
                vis[nx][ny] = 1;
                q[back++] = (Node){nx, ny, cur.d+1};
            }
        }
    }
    return maxDist;
}

int main() {
    scanf("%d %d", &n, &m);
    for(int i=0;i<n;i++) scanf("%s", grid[i]);

    int ans = 0;
    for(int i=0;i<n;i++) {
        for(int j=0;j<m;j++) {
            if(grid[i][j] == '.') {
                int d = bfs(i,j);
                if(d > ans) ans = d;
            }
        }
    }
    printf("%d\n", ans);
    return 0;
}

