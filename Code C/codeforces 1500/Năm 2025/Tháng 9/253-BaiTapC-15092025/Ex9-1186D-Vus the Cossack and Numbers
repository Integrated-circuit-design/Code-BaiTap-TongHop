#include <stdio.h>
#include <math.h>

int main() {
    int n;
    scanf("%d", &n);

    double x;
    int sum = 0;
    int b[n];
    int round_up_count = 0;

    for (int i = 0; i < n; i++) {
        scanf("%lf", &x);
        int f = (int)floor(x);
        b[i] = f;
        sum += f;
        if (fabs(x - f) > 1e-9) round_up_count++;
    }

    int diff = -sum;
    int used = 0;

    fseek(stdin, 0, SEEK_SET);
    scanf("%d", &n);

    for (int i = 0; i < n; i++) {
        scanf("%lf", &x);
        int f = (int)floor(x);
        if (fabs(x - f) > 1e-9 && used < diff) {
            b[i] = f + 1;
            used++;
        }
    }

    for (int i = 0; i < n; i++) {
        printf("%d\n", b[i]);
    }

    return 0;
}

