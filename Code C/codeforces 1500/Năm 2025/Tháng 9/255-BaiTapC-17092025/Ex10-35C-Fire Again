#include <stdio.h>
#include <stdlib.h>

#define MAXK 15

int absVal(int x) { return x < 0 ? -x : x; }

int main() {
    int N, M, K;
    scanf("%d %d", &N, &M);
    scanf("%d", &K);

    int x0[MAXK], y0[MAXK];
    for (int i = 0; i < K; i++) {
        scanf("%d %d", &x0[i], &y0[i]);
    }

    int bestX = 1, bestY = 1;
    int maxDist = -1;

    for (int i = 1; i <= N; i++) {
        for (int j = 1; j <= M; j++) {
            int minDist = 1000000000;
            for (int k = 0; k < K; k++) {
                int d = absVal(i - x0[k]) + absVal(j - y0[k]);
                if (d < minDist) minDist = d;
            }
            if (minDist > maxDist) {
                maxDist = minDist;
                bestX = i;
                bestY = j;
            }
        }
    }

    printf("%d %d\n", bestX, bestY);
    return 0;
}

