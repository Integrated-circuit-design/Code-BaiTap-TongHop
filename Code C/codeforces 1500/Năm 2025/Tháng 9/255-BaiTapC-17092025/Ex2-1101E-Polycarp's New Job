#include <stdio.h>

long long max_small = 0;
long long max_big = 0;

void add_bill(long long a, long long b) {
    if (a > b) { long long t = a; a = b; b = t; }
    if (a > max_small) max_small = a;
    if (b > max_big)   max_big   = b;
}

void check_wallet(long long h, long long w) {
    if (h > w) { long long t = h; h = w; w = t; }
    if (h >= max_small && w >= max_big)
        printf("YES\n");
    else
        printf("NO\n");
}

int main() {
    int n;
    scanf("%d", &n);
    while (n--) {
        char query[2];
        long long x, y;
        scanf("%s %lld %lld", query, &x, &y);
        if (query[0] == '+')
            add_bill(x, y);
        else
            check_wallet(x, y);
    }
    return 0;
}

