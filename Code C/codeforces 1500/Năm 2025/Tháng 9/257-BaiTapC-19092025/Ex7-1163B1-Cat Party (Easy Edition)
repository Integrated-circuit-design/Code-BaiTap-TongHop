#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);
    int u[n];
    for (int i = 0; i < n; i++) scanf("%d", &u[i]);

    int count[11] = {0};
    int maxStreak = 0;

    for (int i = 0; i < n; i++) {
        count[u[i]]++;

        int freq[11] = {0};
        for (int c = 1; c <= 10; c++) {
            if (count[c] > 0) freq[count[c]]++;
        }

        int distinctFreq = 0;
        int f1 = 0, f2 = 0;
        for (int f = 1; f <= n; f++) {
            if (freq[f] > 0) {
                distinctFreq++;
                if (f1 == 0) f1 = f;
                else f2 = f;
            }
        }

        int valid = 0;
        if (distinctFreq == 1) valid = 1;
        else if (distinctFreq == 2) {
            if ((freq[f1] == 1 && f1 - f2 == 1) || 
                (freq[f2] == 1 && f2 - f1 == 1) || 
                (freq[f1] == 1 && f1 == 1) || 
                (freq[f2] == 1 && f2 == 1))
                valid = 1;
        }

        if (valid) maxStreak = i + 1;
    }

    printf("%d\n", maxStreak);
    return 0;
}

