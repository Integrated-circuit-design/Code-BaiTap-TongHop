#include <stdio.h>

int main() {
    int n, m;
    scanf("%d %d", &n, &m);

    int A[505][505], B[505][505];

    for (int i = 0; i < n; i++)
        for (int j = 0; j < m; j++)
            scanf("%d", &A[i][j]);

    for (int i = 0; i < n; i++)
        for (int j = 0; j < m; j++)
            scanf("%d", &B[i][j]);

    for (int i = 0; i < n; i++) {
        int count = 0;
        for (int j = 0; j < m; j++)
            if (A[i][j] != B[i][j])
                count++;
        if (count % 2 != 0) {
            printf("No\n");
            return 0;
        }
    }

    for (int j = 0; j < m; j++) {
        int count = 0;
        for (int i = 0; i < n; i++)
            if (A[i][j] != B[i][j])
                count++;
        if (count % 2 != 0) {
            printf("No\n");
            return 0;
        }
    }

    printf("Yes\n");
    return 0;
}

