#include <stdio.h>

#define MOD 1000000007
#define MAX_LEN 100005

int base64_val(char c) {
    if (c >= '0' && c <= '9') return c - '0';
    if (c >= 'A' && c <= 'Z') return c - 'A' + 10;
    if (c >= 'a' && c <= 'z') return c - 'a' + 36;
    return (c == '-') ? 62 : 63;
}

int pair_count(int x) {
    int cnt = 0;
    for (int a = 0; a < 64; a++) {
        for (int b = 0; b < 64; b++) {
            if ((a & b) == x) cnt++;
        }
    }
    return cnt;
}

int main() {
    char s[MAX_LEN];
    scanf("%s", s);

    int table[64];
    for (int i = 0; i < 64; i++) {
        table[i] = pair_count(i);
    }

    long long result = 1;
    for (int i = 0; s[i]; i++) {
        int val = base64_val(s[i]);
        result = (result * table[val]) % MOD;
    }

    printf("%lld\n", result);
    return 0;
}

