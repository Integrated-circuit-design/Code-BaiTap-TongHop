#include <stdio.h>

int main() {
    int n;
    long long a, b;
    scanf("%d %lld %lld", &n, &a, &b);

    long long x[100000];
    long long ans[100000];

    for (int i = 0; i < n; i++) {
        scanf("%lld", &x[i]);
    }

    for (int i = 0; i < n; i++) {
        long long value = a * x[i];
        long long remainder = value % b;
        ans[i] = remainder / a;
    }

    for (int i = 0; i < n; i++) {
        printf("%lld", ans[i]);
        if (i < n - 1) printf(" ");
    }
    printf("\n");

    return 0;
}

