#include <stdio.h>
#include <string.h>

#define MAXN 100005

char s[MAXN];
char sum[MAXN * 2];
char best[MAXN * 2];

int main() {
    int n;
    scanf("%d", &n);
    scanf("%s", s);

    int mid = n / 2;
    best[0] = '\0';

    for (int d = -1; d <= 1; d++) {
        int pos = mid + d;
        if (pos <= 0 || pos >= n) continue;
        if (s[pos] == '0') continue;

        int i = pos - 1, j = n - 1, k = 0, carry = 0;
        while (i >= 0 || j >= pos || carry) {
            int val = carry;
            if (i >= 0) val += s[i--] - '0';
            if (j >= pos) val += s[j--] - '0';
            sum[k++] = (val % 10) + '0';
            carry = val / 10;
        }
        for (int p = 0; p < k / 2; p++) {
            char tmp = sum[p];
            sum[p] = sum[k - 1 - p];
            sum[k - 1 - p] = tmp;
        }
        sum[k] = '\0';

        if (best[0] == '\0' || 
            (strlen(sum) < strlen(best)) || 
            (strlen(sum) == strlen(best) && strcmp(sum, best) < 0)) {
            strcpy(best, sum);
        }
    }

    printf("%s\n", best);
    return 0;
}

