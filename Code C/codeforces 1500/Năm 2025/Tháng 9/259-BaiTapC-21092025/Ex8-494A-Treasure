#include <stdio.h>
#include <string.h>

#define MAXN 100005

char s[MAXN];
int ans[MAXN];

int main() {
    scanf("%s", s);
    int n = strlen(s);

    int totalOpen = 0, totalClose = 0, totalHash = 0;
    for (int i = 0; i < n; i++) {
        if (s[i] == '(') totalOpen++;
        else if (s[i] == ')') totalClose++;
        else totalHash++;
    }

    if (totalHash == 0) {
        printf("-1\n");
        return 0;
    }

    int need = totalOpen - totalClose;
    if (need < totalHash) {
        printf("-1\n");
        return 0;
    }

    for (int i = 0; i < totalHash; i++) ans[i] = 1;
    ans[totalHash - 1] += (need - (totalHash - 1));

    int bal = 0, idx = 0;
    for (int i = 0; i < n; i++) {
        if (s[i] == '(') bal++;
        else if (s[i] == ')') bal--;
        else {
            bal -= ans[idx++];
        }
        if (bal < 0) {
            printf("-1\n");
            return 0;
        }
    }

    if (bal != 0) {
        printf("-1\n");
        return 0;
    }

    for (int i = 0; i < totalHash; i++) {
        printf("%d\n", ans[i]);
    }

    return 0;
}

