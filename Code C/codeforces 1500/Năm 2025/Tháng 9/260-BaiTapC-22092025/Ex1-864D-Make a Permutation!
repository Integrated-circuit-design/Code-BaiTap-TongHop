#include <stdio.h>
#include <stdlib.h>

#define MAXN 200005

int a[MAXN], freq[MAXN], missing[MAXN];
int n;

int cmp(const void *x, const void *y) {
    return *(int*)x - *(int*)y;
}

int main() {
    scanf("%d", &n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &a[i]);
        freq[a[i]]++;
    }

    int m = 0;
    for (int i = 1; i <= n; i++) {
        if (!freq[i]) missing[m++] = i;
    }

    qsort(missing, m, sizeof(int), cmp);

    int changes = 0, pos = 0;
    int keep[MAXN] = {0};

    for (int i = 0; i < n; i++) {
        if (freq[a[i]] > 1) {
            int need = missing[pos];
            if (a[i] > need || keep[a[i]]) {
                freq[a[i]]--;
                a[i] = need;
                pos++;
                changes++;
            } else {
                keep[a[i]] = 1;
            }
        }
    }

    printf("%d\n", changes);
    for (int i = 0; i < n; i++) {
        printf("%d%c", a[i], (i == n-1 ? '\n' : ' '));
    }
    return 0;
}

