#include <stdio.h>
#include <string.h>

#define MAXN 100005

long long countPairs(const char *s) {
    long long freq[256] = {0}, ans = 0;
    int i;
    for (i = 0; s[i]; i++) freq[(unsigned char)s[i]]++;
    for (i = 0; i < 256; i++) if (freq[i]) ans += freq[i] * freq[i];
    return ans;
}

int main() {
    char s[MAXN];
    scanf("%s", s);
    printf("%lld\n", countPairs(s));
    return 0;
}

