#include <stdio.h>

#define MAXN 200000

char s[MAXN+5];

int main() {
    int n;
    scanf("%d", &n);
    scanf("%s", s);

    int aliveD = 0, aliveR = 0;
    for (int i = 0; i < n; i++) {
        if (s[i] == 'D') aliveD++;
        else aliveR++;
    }

    int banD = 0, banR = 0;
    while (aliveD > 0 && aliveR > 0) {
        for (int i = 0; i < n; i++) {
            if (s[i] == 0) continue;

            if (s[i] == 'D') {
                if (banD > 0) {
                    banD--;
                    s[i] = 0;
                    aliveD--;
                } else {
                    banR++;
                }
            } else {
                if (banR > 0) {
                    banR--;
                    s[i] = 0;
                    aliveR--;
                } else {
                    banD++;
                }
            }
        }
    }

    if (aliveD > 0) printf("D\n");
    else printf("R\n");
    return 0;
}

