#include <stdio.h>

typedef long long ll;

ll C2(ll n) { return n * (n - 1) / 2; }
ll C3(ll n) { return n * (n - 1) * (n - 2) / 6; }

int main() {
    int n;
    scanf("%d", &n);

    ll a[n];
    for (int i = 0; i < n; i++) scanf("%lld", &a[i]);

    ll m1 = 1e18, m2 = 1e18, m3 = 1e18;
    for (int i = 0; i < n; i++) {
        if (a[i] < m1) {
            m3 = m2; m2 = m1; m1 = a[i];
        } else if (a[i] < m2) {
            m3 = m2; m2 = a[i];
        } else if (a[i] < m3) {
            m3 = a[i];
        }
    }

    ll c1 = 0, c2 = 0, c3 = 0;
    for (int i = 0; i < n; i++) {
        if (a[i] == m1) c1++;
        else if (a[i] == m2) c2++;
        else if (a[i] == m3) c3++;
    }

    ll ans = 0;

    if (m1 == m2 && m2 == m3) {
        ans = C3(c1);
    } else if (m1 == m2 && m2 != m3) {
        ans = C2(c1) * c3;
    } else if (m1 != m2 && m2 == m3) {
        ans = c1 * C2(c2);
    } else {
        ans = c1 * c2 * c3;
    }

    printf("%lld\n", ans);
    return 0;
}

