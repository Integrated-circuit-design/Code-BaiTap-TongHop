#include <stdio.h>
#include <string.h>

int main() {
    int n;
    char s[1005];
    char best[1005];

    scanf("%d", &n);
    scanf("%s", s);

    strcpy(best, s);

    for (int shift = 0; shift < n; shift++) {
        char temp[1005];
        for (int i = 0; i < n; i++) {
            int index = (i + shift) % n;
            temp[i] = s[index];
        }

        int add = (10 - (temp[0] - '0')) % 10;

        for (int i = 0; i < n; i++) {
            temp[i] = ((temp[i] - '0' + add) % 10) + '0';
        }
        temp[n] = '\0';

        int smaller = 0;
        for (int i = 0; i < n; i++) {
            if (temp[i] < best[i]) { smaller = 1; break; }
            if (temp[i] > best[i]) { break; }
        }

        if (smaller) {
            strcpy(best, temp);
        }
    }

    printf("%s\n", best);
    return 0;
}

