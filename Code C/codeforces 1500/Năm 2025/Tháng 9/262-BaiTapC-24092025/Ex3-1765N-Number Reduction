#include <stdio.h>
#include <string.h>

#define MAXN 500005

char s[MAXN];
char res[MAXN];

int main() {
    int t;
    scanf("%d", &t);
    while (t--) {
        scanf("%s", s);
        int k;
        scanf("%d", &k);

        int n = strlen(s);
        int len = 0;

        for (int i = 0; i < n; i++) {
            while (len > 0 && res[len - 1] > s[i] && k > 0) {
                len--;
                k--;
            }
            res[len++] = s[i];
        }

        if (k > 0) len -= k;
        if (len < 1) len = 1;

        int pos = 0;
        while (pos < len - 1 && res[pos] == '0') pos++;

        res[len] = '\0';

        printf("%s\n", res + pos);
    }
    return 0;
}

