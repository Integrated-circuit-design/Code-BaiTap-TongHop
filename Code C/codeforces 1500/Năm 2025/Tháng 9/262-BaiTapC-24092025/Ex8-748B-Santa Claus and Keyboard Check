#include <stdio.h>
#include <string.h>

#define MAX 1005

int addMapping(int a, int b, int map[26]) {
    if (map[a] == -1 && map[b] == -1) {
        map[a] = b;
        map[b] = a;
    } else {
        if ((map[a] != -1 && map[a] != b) || (map[b] != -1 && map[b] != a)) {
            return 0;
        }
        map[a] = b;
        map[b] = a;
    }
    return 1;
}

int main() {
    char s[MAX], t[MAX];
    int map[26], used[26] = {0};
    int pairs[26][2], k = 0;

    for (int i = 0; i < 26; i++) map[i] = -1;

    scanf("%s %s", s, t);
    int n = strlen(s);

    for (int i = 0; i < n; i++) {
        int a = s[i] - 'a';
        int b = t[i] - 'a';
        if (a == b) continue;
        if (!addMapping(a, b, map)) {
            printf("-1\n");
            return 0;
        }
    }

    for (int i = 0; i < 26; i++) {
        if (map[i] != -1 && !used[i]) {
            pairs[k][0] = i;
            pairs[k][1] = map[i];
            used[i] = used[map[i]] = 1;
            k++;
        }
    }

    printf("%d\n", k);
    for (int i = 0; i < k; i++) {
        printf("%c %c\n", pairs[i][0] + 'a', pairs[i][1] + 'a');
    }
    return 0;
}

