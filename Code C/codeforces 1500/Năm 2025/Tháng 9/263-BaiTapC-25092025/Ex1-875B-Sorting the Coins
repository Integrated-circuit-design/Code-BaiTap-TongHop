#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int p[n+1];
    for (int i = 1; i <= n; i++) {
        scanf("%d", &p[i]);
    }

    int used[n+1];
    for (int i = 1; i <= n; i++) used[i] = 0;

    printf("1");

    int rightmost = 0;

    for (int step = 1; step <= n; step++) {
        int pos = p[step];
        used[pos] = 1;

        if (pos > rightmost) {
            rightmost = pos;
        }

        int hardness = n - rightmost + 1;
        printf(" %d", hardness);
    }

    printf("\n");
    return 0;
}

