#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int s[100];
    for (int i = 0; i < n; i++) {
        scanf("%d", &s[i]);
    }

    int result = 0;
    int prev = 0;

    for (int i = 0; i < n; i++) {
        if (s[i] == 1) {
            result++;
            prev = 1;
        } else {
            if (prev == 0) {
                result++;
            }
        }
    }

    printf("%d\n", result);
    return 0;
}

