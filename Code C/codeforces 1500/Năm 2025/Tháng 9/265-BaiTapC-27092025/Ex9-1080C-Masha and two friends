#include <stdio.h>
typedef long long ll;

ll countWhite(ll x1, ll y1, ll x2, ll y2) {
    if (x1 > x2 || y1 > y2) return 0;
    ll width  = x2 - x1 + 1;
    ll height = y2 - y1 + 1;
    ll area   = width * height;
    if (area % 2 == 0) return area / 2;
    else {
        if ((x1 + y1) % 2 == 0) return area / 2 + 1;
        else return area / 2;
    }
}

ll countBlack(ll x1, ll y1, ll x2, ll y2) {
    if (x1 > x2 || y1 > y2) return 0;
    ll width  = x2 - x1 + 1;
    ll height = y2 - y1 + 1;
    ll area   = width * height;
    return area - countWhite(x1, y1, x2, y2);
}

int main() {
    int t;
    scanf("%d", &t);
    while (t--) {
        ll n, m;
        scanf("%lld %lld", &n, &m);

        ll x1, y1, x2, y2;
        scanf("%lld %lld %lld %lld", &x1, &y1, &x2, &y2);

        ll x3, y3, x4, y4;
        scanf("%lld %lld %lld %lld", &x3, &y3, &x4, &y4);

        ll total = n * m;
        ll W = countWhite(1, 1, m, n);
        ll B = total - W;

        ll blacksInR1 = countBlack(x1, y1, x2, y2);
        W += blacksInR1;
        B -= blacksInR1;

        ll whitesInR2 = countWhite(x3, y3, x4, y4);
        W -= whitesInR2;
        B += whitesInR2;

        printf("%lld %lld\n", W, B);
    }
    return 0;
}

