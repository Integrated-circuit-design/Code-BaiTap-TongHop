#include <stdio.h>
#include <string.h>

#define MAXN 100005
#define MAXLEN 100005

int main() {
    int n;
    scanf("%d", &n);

    char words[n][MAXLEN];
    for (int i = 0; i < n; i++) {
        scanf("%s", words[i]);
    }

    char message[MAXLEN];
    scanf("%s", message);

    int pos = 0;
    int valid = 1;

    for (int i = 0; i < n; i++) {
        int found = 0;
        while (message[pos] != '\0') {
            if (message[pos] == '<' && message[pos + 1] == '3') {
                pos += 2;
                found = 1;
                break;
            } else {
                pos++;
            }
        }
        if (!found) { valid = 0; break; }

        int wlen = strlen(words[i]);
        int j;
        for (j = 0; j < wlen && message[pos] != '\0'; j++, pos++) {
            if (message[pos] != words[i][j]) {
                valid = 0;
                break;
            }
        }
        if (j < wlen) { valid = 0; break; }
    }

    int found = 0;
    while (message[pos] != '\0') {
        if (message[pos] == '<' && message[pos + 1] == '3') {
            found = 1;
            break;
        }
        pos++;
    }
    if (!found) valid = 0;

    if (valid) printf("yes\n");
    else printf("no\n");

    return 0;
}

