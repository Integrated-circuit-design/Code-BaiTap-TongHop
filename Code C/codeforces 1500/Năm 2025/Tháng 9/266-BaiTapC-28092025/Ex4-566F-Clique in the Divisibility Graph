#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int a[n];
    for(int i = 0; i < n; i++) scanf("%d", &a[i]);

    int max_clique = 1;

    for(int i = 0; i < n; i++) {
        int count = 1;

        for(int j = i + 1; j < n; j++) {
            int divisible = 1;

            for(int k = i; k < j; k++) {
                if(a[j] % a[k] != 0 && a[k] % a[j] != 0) {
                    divisible = 0;
                    break;
                }
            }

            if(divisible) count++;
        }

        if(count > max_clique) max_clique = count;
    }

    printf("%d\n", max_clique);
    return 0;
}

