#include <stdio.h>

int main() {
    int n, d, l;
    scanf("%d %d %d", &n, &d, &l);

    int soCong = (n + 1) / 2;
    int soTru  = n / 2;

    int maxS = soCong * l - soTru * 1;
    int minS = soCong * 1 - soTru * l;

    if (d < minS || d > maxS) {
        puts("-1");
        return 0;
    }

    int a[105];
    for (int i = 0; i < n; i++) {
        if (i % 2 == 0) a[i] = 1;
        else a[i] = l;
    }

    int tong = minS;

    for (int i = 0; i < n && tong != d; i++) {
        if (i % 2 == 0) {
            int tangToiDa = l - a[i];
            int can = d - tong;
            int thucTe = (can < tangToiDa ? can : tangToiDa);
            a[i] += thucTe;
            tong += thucTe;
        } else {
            int giamToiDa = a[i] - 1;
            int can = d - tong;
            int thucTe = (can < giamToiDa ? can : giamToiDa);
            a[i] -= thucTe;
            tong += thucTe;
        }
    }

    for (int i = 0; i < n; i++) {
        if (i) printf(" ");
        printf("%d", a[i]);
    }
    printf("\n");
    return 0;
}

