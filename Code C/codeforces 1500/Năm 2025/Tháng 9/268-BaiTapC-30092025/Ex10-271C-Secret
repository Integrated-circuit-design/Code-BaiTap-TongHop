#include <stdio.h>
#include <stdlib.h>

int main() {
    long long n, k;
    scanf("%lld %lld", &n, &k);

    if (n < 3 * k) {
        printf("-1\n");
        return 0;
    }

    int *ans = (int*)malloc((n + 1) * sizeof(int));

    for (long long i = 1; i <= n; i++) {
        ans[i] = (int)((i - 1) % k + 1);
    }

    if (k > 1) {
        ans[k] = 1;
    }

    for (long long i = 1; i <= n; i++) {
        if (i > 1) printf(" ");
        printf("%d", ans[i]);
    }
    printf("\n");

    free(ans);
    return 0;
}

