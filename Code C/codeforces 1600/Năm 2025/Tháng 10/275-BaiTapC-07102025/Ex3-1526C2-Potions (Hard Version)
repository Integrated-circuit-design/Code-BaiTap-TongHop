#include <stdio.h>
#include <stdlib.h>

#define ll long long
#define MAXN 200005

ll a[MAXN], taken[MAXN];
int cnt = 0;

int main() {
    int n;
    scanf("%d", &n);

    for (int i = 0; i < n; i++)
        scanf("%lld", &a[i]);

    ll health = 0;
    for (int i = 0; i < n; i++) {
        health += a[i];
        taken[cnt++] = a[i];
        if (health < 0) {
            int worst = 0;
            for (int j = 1; j < cnt; j++)
                if (taken[j] < taken[worst])
                    worst = j;
            health -= taken[worst];
            taken[worst] = taken[--cnt];
        }
    }

    printf("%d\n", cnt);
    return 0;
}

