#include <stdio.h>
#include <string.h>

void toColumn(int num, char *res) {
    char tmp[20];
    int len = 0;
    while (num > 0) {
        num--;
        tmp[len++] = 'A' + (num % 26);
        num /= 26;
    }
    for (int i = 0; i < len; i++)
        res[i] = tmp[len - i - 1];
    res[len] = '\0';
}

int toNumber(const char *s) {
    int val = 0;
    for (int i = 0; s[i]; i++)
        val = val * 26 + (s[i] - 'A' + 1);
    return val;
}

int isRXCY(const char *s) {
    if (s[0] != 'R') return 0;
    int i = 1;
    if (s[i] < '0' || s[i] > '9') return 0;
    while (s[i] >= '0' && s[i] <= '9') i++;
    if (s[i] != 'C') return 0;
    return 1;
}

int main() {
    int t;
    scanf("%d", &t);
    while (t--) {
        char s[32];
        scanf("%s", s);
        if (isRXCY(s)) {
            int row = 0, col = 0;
            int i = 1;
            while (s[i] >= '0' && s[i] <= '9')
                row = row * 10 + (s[i++] - '0');
            i++;
            while (s[i] >= '0' && s[i] <= '9')
                col = col * 10 + (s[i++] - '0');
            char colStr[20];
            toColumn(col, colStr);
            printf("%s%d\n", colStr, row);
        } else {
            char colStr[20];
            int i = 0, j = 0;
            while (s[i] >= 'A' && s[i] <= 'Z')
                colStr[j++] = s[i++];
            colStr[j] = '\0';
            int row = 0;
            while (s[i] >= '0' && s[i] <= '9')
                row = row * 10 + (s[i++] - '0');
            int col = toNumber(colStr);
            printf("R%dC%d\n", row, col);
        }
    }
    return 0;
}

