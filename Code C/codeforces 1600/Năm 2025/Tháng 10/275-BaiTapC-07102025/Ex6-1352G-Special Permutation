#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);
    while (t--) {
        int n;
        scanf("%d", &n);

        if (n == 2 || n == 3) {
            printf("-1\n");
            continue;
        }

        int base[6] = {6, 2, 4, 1, 3, 5};
        int shift = 0;

        while (n - shift >= 6) {
            for (int i = 0; i < 6; i++)
                printf("%d ", base[i] + shift);
            shift += 6;
        }

        int rem = n - shift;

        if (rem == 5)
            printf("%d %d %d %d %d", shift+5, shift+3, shift+1, shift+4, shift+2);
        else if (rem == 4)
            printf("%d %d %d %d", shift+3, shift+1, shift+4, shift+2);
        else if (rem == 3)
            printf("%d %d %d", shift+2, shift, shift+1);
        else if (rem == 2)
            printf("%d %d", shift+1, shift);
        else if (rem == 1)
            printf("%d", shift+1);

        printf("\n");
    }
    return 0;
}

