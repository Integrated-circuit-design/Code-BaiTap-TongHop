#include <stdio.h>

typedef long long ll;

void swap(ll *x, ll *y) {
    ll tmp = *x;
    *x = *y;
    *y = tmp;
}

void sort(ll a[], int n) {
    for (int i = 0; i < n - 1; i++)
        for (int j = i + 1; j < n; j++)
            if (a[i] > a[j])
                swap(&a[i], &a[j]);
}

int main() {
    int n, m;
    ll a[200005];
    scanf("%d %d", &n, &m);

    for (int i = 0; i < n; i++)
        scanf("%lld", &a[i]);

    if (n > m) {
        printf("0\n");
        return 0;
    }

    sort(a, n);

    ll result = 1;

    for (int i = 0; i < n; i++) {
        for (int j = i + 1; j < n; j++) {
            ll diff = a[j] - a[i];
            if (diff < 0) diff = -diff;
            result = (result * (diff % m)) % m;
        }
    }

    printf("%lld\n", result);
    return 0;
}

