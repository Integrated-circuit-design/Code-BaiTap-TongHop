#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        int n;
        long long q;
        scanf("%d %lld", &n, &q);

        long long a[n];
        for (int i = 0; i < n; i++) scanf("%lld", &a[i]);

        char s[n + 1];
        s[n] = '\0';

        long long currentIQ = q;
        int flag = 0;

        for (int i = 0; i < n; i++) {
            if (flag) {
                s[i] = '0';
                continue;
            }

            if (a[i] <= currentIQ) {
                s[i] = '1';
            } else if (currentIQ > 0) {
                s[i] = '1';
                currentIQ--;
                if (currentIQ == 0)
                    flag = 1;
            } else {
                s[i] = '0';
            }
        }

        printf("%s\n", s);
    }

    return 0;
}

