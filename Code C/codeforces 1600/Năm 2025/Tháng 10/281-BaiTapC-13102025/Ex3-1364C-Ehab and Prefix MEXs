#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int a[100005], b[100005];
    int used[1000007] = {0};

    for (int i = 0; i < n; i++)
        scanf("%d", &a[i]);

    for (int i = 1; i < n; i++) {
        if (a[i] < a[i - 1]) {
            printf("-1\n");
            return 0;
        }
    }

    int mex_candidate = 0;
    b[0] = (a[0] == 0) ? 1 : 0;
    used[b[0]] = 1;

    for (int i = 1; i < n; i++) {
        if (a[i] > a[i - 1]) {
            b[i] = a[i - 1];
            used[b[i]] = 1;
        } else {
            while (used[mex_candidate] || mex_candidate == a[i])
                mex_candidate++;
            b[i] = mex_candidate;
            used[b[i]] = 1;
        }
    }

    for (int i = 0; i < n; i++) {
        printf("%d", b[i]);
        if (i < n - 1) printf(" ");
    }
    printf("\n");

    return 0;
}

