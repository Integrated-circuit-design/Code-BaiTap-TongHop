#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);
    while (t--) {
        int n;
        long long x;
        scanf("%d %lld", &n, &x);
        long long a[200005], prefix[200005];
        prefix[0] = 0;
        for (int i = 1; i <= n; i++) {
            scanf("%lld", &a[i]);
            prefix[i] = prefix[i - 1] + a[i];
        }
        long long ans = 0;
        int l = 1;
        for (int r = 1; r <= n; r++) {
            while (l <= r && prefix[r] - prefix[l - 1] > x) {
                l++;
            }
            ans += (r - l + 1);
        }
        printf("%lld\n", ans);
    }
    return 0;
}

