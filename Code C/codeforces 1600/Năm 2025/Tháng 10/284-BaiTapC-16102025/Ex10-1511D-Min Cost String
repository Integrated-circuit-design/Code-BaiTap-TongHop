#include <stdio.h>

int main() {
    int n, k;
    scanf("%d %d", &n, &k);

    char s[n + 1];

    if (k == 1) {
        for (int i = 0; i < n; i++) s[i] = 'a';
        s[n] = '\0';
        printf("%s\n", s);
        return 0;
    }

    int pos = 0;
    for (int i = 0; i < k && pos < n; i++) {
        for (int j = i; j < k && pos < n; j++) {
            s[pos++] = 'a' + i;
            if (pos < n) s[pos++] = 'a' + j;
        }
    }

    for (int i = 0; pos < n; i++) {
        s[pos++] = s[i];
    }

    s[n] = '\0';
    printf("%s\n", s);

    return 0;
}

