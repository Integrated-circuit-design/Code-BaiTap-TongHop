#include <stdio.h>
#include <stdlib.h>

#define MAXN 200005

typedef struct {
    long long val;
    int used;
} Prefix;

Prefix seen[MAXN];
int cnt;

void reset_seen() {
    for (int i = 0; i < cnt; i++)
        seen[i].used = 0;
    cnt = 0;
}

int has_seen(long long x) {
    for (int i = 0; i < cnt; i++)
        if (seen[i].used && seen[i].val == x)
            return 1;
    return 0;
}

void add_seen(long long x) {
    seen[cnt].val = x;
    seen[cnt].used = 1;
    cnt++;
}

int main() {
    int t;
    scanf("%d", &t);
    while (t--) {
        int n;
        scanf("%d", &n);
        long long a[MAXN];
        for (int i = 0; i < n; i++)
            scanf("%lld", &a[i]);
        long long sum = 0;
        int score = 0;
        cnt = 0;
        for (int i = 0; i < n; i++) {
            sum += a[i];
            if (sum == 0 || has_seen(sum)) {
                score++;
                sum = 0;
                reset_seen();
            } else {
                add_seen(sum);
            }
        }
        printf("%d\n", score);
    }
    return 0;
}

