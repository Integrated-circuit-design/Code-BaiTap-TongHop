#include <stdio.h>

int main() {
    int n, m;
    scanf("%d %d", &n, &m);
    int a[505][505];
    int choose[505];
    int i, j;
    for (i = 0; i < n; i++) {
        for (j = 0; j < m; j++) {
            scanf("%d", &a[i][j]);
        }
        choose[i] = 1;
    }
    int totalXor = 0;
    for (i = 0; i < n; i++)
        totalXor ^= a[i][0];
    if (totalXor > 0) {
        printf("TAK\n");
        for (i = 0; i < n; i++)
            printf("%d ", choose[i]);
        printf("\n");
        return 0;
    }
    int changeRow = -1, changeCol = -1;
    for (i = 0; i < n; i++) {
        int base = a[i][0];
        for (j = 1; j < m; j++) {
            if (a[i][j] != base) {
                changeRow = i;
                changeCol = j;
                break;
            }
        }
        if (changeRow != -1) break;
    }
    if (changeRow == -1) {
        printf("NIE\n");
        return 0;
    }
    choose[changeRow] = changeCol + 1;
    printf("TAK\n");
    for (i = 0; i < n; i++)
        printf("%d ", choose[i]);
    printf("\n");
    return 0;
}

