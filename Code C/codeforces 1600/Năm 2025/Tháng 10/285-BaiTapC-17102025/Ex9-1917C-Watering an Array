#include <stdio.h>
#include <string.h>

#define MAXN 2005
#define MAXK 100005

int t, n, k;
long long d;
int a[MAXN], v[MAXK];

int main() {
    scanf("%d", &t);
    while (t--) {
        scanf("%d %d %lld", &n, &k, &d);
        for (int i = 0; i < n; i++) scanf("%d", &a[i]);
        for (int i = 0; i < k; i++) scanf("%d", &v[i]);

        int curr[MAXN];
        memset(curr, 0, sizeof(curr));

        long long max_score = 0;
        long long day = 0;

        while (day < d) {
            int c = 0;
            for (int j = 0; j < n; j++) {
                if (curr[j] == j + 1) c++;
            }

            if (c > 0) {
                max_score += c;
                memset(curr, 0, sizeof(curr));
            }

            int bi = v[day % k];
            for (int j = 0; j < bi && j < n; j++) curr[j]++;

            day++;
        }

        printf("%lld\n", max_score);
    }
    return 0;
}

