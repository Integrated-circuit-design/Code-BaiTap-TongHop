#include <stdio.h>

#define MAXN 305

void swap(int *x, int *y) {
    int tmp = *x;
    *x = *y;
    *y = tmp;
}

void sort_array(int a[], int n) {
    for (int i = 0; i < n - 1; ++i)
        for (int j = 0; j < n - i - 1; ++j)
            if (a[j] > a[j + 1])
                swap(&a[j], &a[j + 1]);
}

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        int n;
        scanf("%d", &n);

        int matrix[MAXN][MAXN];
        int tail[MAXN];

        for (int i = 0; i < n; ++i)
            for (int j = 0; j < n; ++j)
                scanf("%d", &matrix[i][j]);

        for (int i = 0; i < n; ++i) {
            int count = 0;
            for (int j = n - 1; j >= 0; --j) {
                if (matrix[i][j] == 1) count++;
                else break;
            }
            tail[i] = count;
        }

        sort_array(tail, n);

        int mex = 0;
        for (int i = 0; i < n; ++i) {
            if (tail[i] >= mex) mex++;
        }

        printf("%d\n", mex);
    }

    return 0;
}

