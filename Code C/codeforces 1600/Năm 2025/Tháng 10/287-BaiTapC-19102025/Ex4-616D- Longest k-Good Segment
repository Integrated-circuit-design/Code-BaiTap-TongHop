#include <stdio.h>
#include <stdlib.h>

#define MAXN 500005
#define MAXVAL 1000005

int a[MAXN];
int count[MAXVAL];

int main() {
    int n, k;
    scanf("%d %d", &n, &k);

    for (int i = 0; i < n; i++) scanf("%d", &a[i]);

    int l = 0, r = 0;
    int distinct = 0;
    int best_l = 0, best_r = 0, best_len = 0;

    for (r = 0; r < n; r++) {
        count[a[r]]++;
        if (count[a[r]] == 1) distinct++;

        while (distinct > k) {
            count[a[l]]--;
            if (count[a[l]] == 0) distinct--;
            l++;
        }

        if (r - l + 1 > best_len) {
            best_len = r - l + 1;
            best_l = l;
            best_r = r;
        }
    }

    printf("%d %d\n", best_l + 1, best_r + 1);
    return 0;
}

