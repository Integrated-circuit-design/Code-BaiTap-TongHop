#include <stdio.h>
#include <string.h>

#define MAXN 200005
#define min(a,b) ((a)<(b)?(a):(b))
#define max(a,b) ((a)>(b)?(a):(b))

char s[MAXN];
int suf0[MAXN], pre1[MAXN];

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        scanf("%s", s);
        int n = strlen(s);

        pre1[0] = 0;
        for (int i = 1; i <= n; i++)
            pre1[i] = pre1[i - 1] + (s[i - 1] == '1');

        suf0[n] = 0;
        for (int i = n - 1; i >= 0; i--)
            suf0[i] = suf0[i + 1] + (s[i] == '0');

        int ans = n;

        for (int i = 0; i <= n; i++) {
            int ones_removed = pre1[i];
            int zeros_remaining = suf0[i];
            ans = min(ans, max(ones_removed, zeros_remaining));
        }

        printf("%d\n", ans);
    }
    return 0;
}

