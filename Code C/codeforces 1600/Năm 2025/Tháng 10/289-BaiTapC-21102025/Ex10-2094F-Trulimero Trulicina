#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);
    while(t--) {
        int n, m, k;
        scanf("%d %d %d", &n, &m, &k);

        for(int i = 0; i < n; i++) {
            for(int j = 0; j < m; j++) {
                int num = ((j * n + i) % k) + 1;
                printf("%d%c", num, j == m-1 ? '\n' : ' ');
            }
        }
    }
    return 0;
}

