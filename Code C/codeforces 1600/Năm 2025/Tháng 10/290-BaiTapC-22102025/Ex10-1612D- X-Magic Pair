#include <stdio.h>

typedef long long ll;

ll gcd_recursive(ll a, ll b) {
    if (b == 0) return a;
    return gcd_recursive(b, a % b);
}

int main() {
    int t;
    scanf("%d", &t);

    for (int i = 0; i < t; i++) {
        ll a, b, x;
        scanf("%lld %lld %lld", &a, &b, &x);

        ll max = (a > b) ? a : b;
        ll min = (a > b) ? b : a;

        ll g = gcd_recursive(max, min);

        if (x <= max && (max - x) % g == 0)
            printf("YES\n");
        else
            printf("NO\n");
    }

    return 0;
}

