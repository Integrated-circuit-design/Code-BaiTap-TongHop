#include <stdio.h>
#include <string.h>

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        long long n, m;
        char s[1000005];
        scanf("%lld %lld", &n, &m);
        scanf("%s", s);

        long long top = 0, bottom = 0;
        long long left = 0, right = 0;
        long long x = 0, y = 0;
        long long bestRow = 0, bestCol = 0;

        for (int i = 0; s[i]; i++) {
            char move = s[i];

            if (move == 'U') x--;
            else if (move == 'D') x++;
            else if (move == 'L') y--;
            else if (move == 'R') y++;

            if (x < -top) top = -x;
            if (x > bottom) bottom = x;
            if (y < -left) left = -y;
            if (y > right) right = y;

            if (top + bottom >= n || left + right >= m) break;

            bestRow = top;
            bestCol = left;
        }

        printf("%lld %lld\n", bestRow + 1, bestCol + 1);
    }

    return 0;
}

