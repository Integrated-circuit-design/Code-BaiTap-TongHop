#include <stdio.h>

int getXor(int n) {
    int res;
    if (n % 4 == 0) res = n;
    else if (n % 4 == 1) res = 1;
    else if (n % 4 == 2) res = n + 1;
    else res = 0;
    return res;
}

int rangeXor(int L, int R) {
    if (L == 0) return getXor(R);
    return getXor(R) ^ getXor(L - 1);
}

int main() {
    int t;
    scanf("%d", &t);

    for (int tc = 0; tc < t; tc++) {
        int l, r;
        scanf("%d %d", &l, &r);

        int len = r - l + 1;
        int xorArray = 0;

        for (int i = 0; i < len; i++) {
            int value;
            scanf("%d", &value);
            xorArray ^= value;
        }

        int xorOriginal = rangeXor(l, r);
        int x = xorArray ^ xorOriginal;

        printf("%d\n", x);
    }

    return 0;
}

