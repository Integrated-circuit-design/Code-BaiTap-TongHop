#include <stdio.h>
#include <string.h>

#define MAXN 400005

char s[MAXN + 5];
int pref[MAXN + 5];
int res[MAXN + 10];

int main() {
    int t;
    scanf("%d", &t);
    while (t--) {
        int n;
        scanf("%d %s", &n, s);

        int sum = 0;
        for (int i = n - 1; i >= 0; --i) {
            sum += s[i] - '0';
            pref[i] = sum;
        }

        int len = 0;
        int carry = 0;
        for (int i = n - 1; i >= 0; --i) {
            int val = pref[i] + carry;
            res[len++] = val % 10;
            carry = val / 10;
        }

        while (carry > 0) {
            res[len++] = carry % 10;
            carry /= 10;
        }

        for (int i = len - 1; i >= 0; --i)
            putchar('0' + res[i]);
        putchar('\n');
    }
    return 0;
}

