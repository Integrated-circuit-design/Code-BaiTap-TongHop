#include <stdio.h>

#define MAXN 200005

long long a[MAXN];

int check_possible(int n, long long k) {
    int has_small = 0;
    int possible = 0;

    for (int i = 0; i < n; i++) {
        if (a[i] <= k) has_small = 1;
    }

    if (!has_small) return 0;

    for (int i = 0; i < n - 1; i++) {
        if (a[i] <= k && a[i + 1] <= k) possible = 1;
    }
    for (int i = 0; i < n - 2; i++) {
        if (a[i] <= k && a[i + 2] <= k) possible = 1;
    }

    return possible;
}

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        int n;
        long long k;
        scanf("%d %lld", &n, &k);

        for (int i = 0; i < n; i++)
            scanf("%lld", &a[i]);

        if (check_possible(n, k))
            printf("YES\n");
        else
            printf("NO\n");
    }

    return 0;
}

