#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);
    long long a[105];
    long long max = 0, min = 1000000000;

    for (int i = 0; i < n; i++) {
        scanf("%lld", &a[i]);
        if (a[i] > max) max = a[i];
        if (a[i] < min) min = a[i];
    }

    long long total_possible = max - min + 1;
    long long missing = total_possible - n;

    if (missing % 2 == 1) {
        printf("Alice\n");
    } else {
        printf("Bob\n");
    }

    return 0;
}

