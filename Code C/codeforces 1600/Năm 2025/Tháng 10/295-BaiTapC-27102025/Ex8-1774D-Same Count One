#include <stdio.h>

int a[105][105];
int cnt[105];
int X[1000005], Y[1000005], Z[1000005];

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        int n, m;
        scanf("%d%d", &n, &m);

        int sum = 0;
        for (int i = 0; i < n; i++) {
            cnt[i] = 0;
            for (int j = 0; j < m; j++) {
                scanf("%d", &a[i][j]);
                cnt[i] += a[i][j];
            }
            sum += cnt[i];
        }

        if (sum % n != 0) {
            printf("-1\n");
            continue;
        }

        int target = sum / n;
        int k = 0;

        for (int col = 0; col < m; col++) {
            for (int i = 0; i < n; i++) {
                if (cnt[i] > target && a[i][col] == 1) {
                    for (int j = 0; j < n; j++) {
                        if (cnt[j] < target && a[j][col] == 0) {
                            a[i][col] = 0;
                            a[j][col] = 1;
                            cnt[i]--;
                            cnt[j]++;
                            X[k] = j + 1;
                            Y[k] = i + 1;
                            Z[k] = col + 1;
                            k++;
                            break;
                        }
                    }
                }
            }
        }

        printf("%d\n", k);
        for (int i = 0; i < k; i++)
            printf("%d %d %d\n", X[i], Y[i], Z[i]);
    }

    return 0;
}

