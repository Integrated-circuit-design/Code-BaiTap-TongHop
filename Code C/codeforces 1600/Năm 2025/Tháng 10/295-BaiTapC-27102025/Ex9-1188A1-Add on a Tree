#include <stdio.h>

#define MAXN 100005

int deg[MAXN];

void read_tree(int n) {
    for (int i = 0; i < n - 1; i++) {
        int u, v;
        scanf("%d %d", &u, &v);
        deg[u]++;
        deg[v]++;
    }
}

int has_degree_two(int n) {
    for (int i = 1; i <= n; i++) {
        if (deg[i] == 2)
            return 1;
    }
    return 0;
}

int main() {
    int n;
    scanf("%d", &n);

    read_tree(n);

    if (has_degree_two(n))
        printf("NO\n");
    else
        printf("YES\n");

    return 0;
}

