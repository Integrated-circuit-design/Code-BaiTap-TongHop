#include <stdio.h>

int main() {
    int T;
    scanf("%d", &T);
    while (T--) {
        int n;
        scanf("%d", &n);
        int A[1005], B[1005];
        for (int i = 0; i < n; ++i) scanf("%d", &A[i]);
        for (int i = 0; i < n; ++i) scanf("%d", &B[i]);

        int flag = 1;
        for (int i = 0; i < n; ++i)
            if (A[i] > B[i]) { flag = 0; break; }

        if (flag) printf("YES\n");
        else printf("NO\n");
    }
    return 0;
}

