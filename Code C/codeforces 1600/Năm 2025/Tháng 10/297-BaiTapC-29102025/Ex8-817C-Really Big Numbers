#include <stdio.h>

typedef long long ll;

ll digit_sum(ll num) {
    ll res = 0;
    for (; num > 0; num /= 10)
        res += num % 10;
    return res;
}

int is_really_big(ll x, ll s) {
    return (x - digit_sum(x) >= s);
}

int main() {
    ll n, s;
    scanf("%lld %lld", &n, &s);

    if (s > n) {
        printf("0\n");
        return 0;
    }

    ll lo = 1, hi = n, mid;
    ll first = n + 1;

    while (lo < hi) {
        mid = lo + (hi - lo) / 2;
        if (is_really_big(mid, s))
            hi = mid;
        else
            lo = mid + 1;
    }

    if (is_really_big(lo, s))
        first = lo;

    if (first > n)
        printf("0\n");
    else
        printf("%lld\n", n - first + 1);

    return 0;
}

