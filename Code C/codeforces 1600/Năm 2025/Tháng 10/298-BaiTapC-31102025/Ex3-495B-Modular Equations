#include <stdio.h>

long long countSolutions(long long a, long long b) {
    if (a == b) return -1;
    if (a < b) return 0;

    long long d = a - b;
    long long res = 0;
    long long i = 1;

    while (i * i <= d) {
        if (d % i == 0) {
            long long other = d / i;
            if (i > b) res++;
            if (other > b && other != i) res++;
        }
        i++;
    }
    return res;
}

int main() {
    long long a, b;
    scanf("%lld %lld", &a, &b);

    long long ans = countSolutions(a, b);

    if (ans == -1)
        printf("infinity\n");
    else
        printf("%lld\n", ans);

    return 0;
}

