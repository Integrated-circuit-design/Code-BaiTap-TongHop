#include <stdio.h>
#include <string.h>

int main() {
    char s[1005];
    scanf("%s", s);
    int n = strlen(s);

    char mirror[128] = {0};
    mirror['A'] = 'A'; mirror['H'] = 'H'; mirror['I'] = 'I';
    mirror['M'] = 'M'; mirror['O'] = 'O'; mirror['o'] = 'o';
    mirror['T'] = 'T'; mirror['U'] = 'U'; mirror['V'] = 'V';
    mirror['v'] = 'v'; mirror['W'] = 'W'; mirror['w'] = 'w';
    mirror['X'] = 'X'; mirror['x'] = 'x'; mirror['Y'] = 'Y';
    mirror['b'] = 'd'; mirror['d'] = 'b';
    mirror['p'] = 'q'; mirror['q'] = 'p';

    int i, ok = 1;
    for (i = 0; i < n; i++) {
        char left = s[i];
        char right = s[n - 1 - i];
        if (mirror[left] == 0 || mirror[left] != right) {
            ok = 0;
            break;
        }
    }

    printf(ok ? "TAK\n" : "NIE\n");
    return 0;
}

