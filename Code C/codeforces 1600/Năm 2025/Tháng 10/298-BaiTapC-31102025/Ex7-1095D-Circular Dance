#include <stdio.h>

#define MAXN 200005

int a[MAXN][2];
int next_kid[MAXN];
int order[MAXN];

int main() {
    int n;
    scanf("%d", &n);

    for (int i = 1; i <= n; ++i)
        scanf("%d %d", &a[i][0], &a[i][1]);

    int x = a[1][0], y = a[1][1];

    int start_next;
    if (a[x][0] == y || a[x][1] == y)
        start_next = x;
    else
        start_next = y;

    order[1] = 1;
    order[2] = start_next;

    for (int i = 3; i <= n; ++i) {
        int prev = order[i - 2];
        int cur = order[i - 1];
        int u = a[cur][0], v = a[cur][1];
        if (u != prev) next_kid[cur] = u;
        else next_kid[cur] = v;
        order[i] = next_kid[cur];
    }

    for (int i = 1; i <= n; ++i)
        printf("%d ", order[i]);
    return 0;
}

