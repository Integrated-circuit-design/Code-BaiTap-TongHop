#include <stdio.h>

int main() {
    int n, m, ta, tb, k;
    scanf("%d %d %d %d %d", &n, &m, &ta, &tb, &k);

    int a[n], b[m];
    for (int i = 0; i < n; i++) scanf("%d", &a[i]);
    for (int i = 0; i < m; i++) scanf("%d", &b[i]);

    if (k >= n || k >= m) {
        printf("-1\n");
        return 0;
    }

    int maxC = 0;
    int impossible = 0;

    for (int cancelA = 0; cancelA <= k; cancelA++) {
        int cancelB = k - cancelA;

        if (cancelA >= n) { impossible = 1; break; }

        int arriveB = a[cancelA] + ta;

        int flightB = -1;
        for (int i = m - 1; i >= 0; i--) {
            if (b[i] >= arriveB) flightB = i;
        }

        if (flightB == -1 || flightB - cancelB < 0) {
            impossible = 1;
            break;
        }

        int arriveC = b[flightB - cancelB] + tb;
        if (arriveC > maxC) maxC = arriveC;
    }

    if (impossible) printf("-1\n");
    else printf("%d\n", maxC);

    return 0;
}

