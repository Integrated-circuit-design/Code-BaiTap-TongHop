#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    long long V[n], T[n];
    for (int i = 0; i < n; i++) scanf("%lld", &V[i]);
    for (int i = 0; i < n; i++) scanf("%lld", &T[i]);

    long long remaining[n];
    for (int i = 0; i < n; i++) remaining[i] = 0;

    long long result[n];

    for (int day = 0; day < n; day++) {
        long long melted = V[day];

        for (int prev = 0; prev < day; prev++) {
            if (remaining[prev] > 0) {
                if (remaining[prev] <= T[day]) {
                    melted += remaining[prev];
                    remaining[prev] = 0;
                } else {
                    melted += T[day];
                    remaining[prev] -= T[day];
                }
            }
        }

        result[day] = melted;

        remaining[day] = V[day] > T[day] ? V[day] - T[day] : 0;
    }

    for (int i = 0; i < n; i++) {
        if (i > 0) printf(" ");
        printf("%lld", result[i]);
    }
    printf("\n");

    return 0;
}

