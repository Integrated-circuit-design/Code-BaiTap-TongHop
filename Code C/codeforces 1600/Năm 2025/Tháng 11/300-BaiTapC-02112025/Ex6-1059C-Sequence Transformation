#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int res[n];
    for (int i = 0; i < n - 1; i++) {
        res[i] = 1;
    }

    res[n - 1] = n;

    for (int i = 0; i < n; i++) {
        printf("%d", res[i]);
        if (i != n - 1) printf(" ");
    }
    printf("\n");

    return 0;
}

