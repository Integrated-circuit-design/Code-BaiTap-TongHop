#include <stdio.h>

#define MAXA 10000

int main() {
    int T;
    scanf("%d", &T);

    while (T--) {
        int n;
        scanf("%d", &n);

        int freq[MAXA + 1] = {0};
        for (int i = 0; i < n; i++) {
            int x;
            scanf("%d", &x);
            freq[x]++;
        }

        int last = 0;
        int best_x = 0, best_y = 0;
        double best_val = 1e20;

        for (int i = 1; i <= MAXA; i++) {
            if (freq[i] >= 4) {
                double val = 4.0;
                if (val < best_val) {
                    best_val = val;
                    best_x = i;
                    best_y = i;
                }
            }

            if (freq[i] >= 2) {
                if (last != 0) {
                    double val = (double)(last + i) * (last + i) / (last * i);
                    if (val < best_val) {
                        best_val = val;
                        best_x = last;
                        best_y = i;
                    }
                }
                last = i;
            }
        }

        printf("%d %d %d %d\n", best_x, best_x, best_y, best_y);
    }

    return 0;
}

