#include <stdio.h>
#include <stdlib.h>

int cmp_value_desc_index_asc(const void *a, const void *b, void *arr_ptr) {
    long long *arr = (long long *)arr_ptr;
    int i = *(int *)a, j = *(int *)b;
    if (arr[i] != arr[j]) return (arr[i] < arr[j]) ? 1 : -1;
    return i - j;
}

int cmp_index_asc(const void *a, const void *b) {
    return *(int *)a - *(int *)b;
}

int main() {
    int n;
    scanf("%d", &n);

    long long a[105];
    for (int i = 0; i < n; ++i) scanf("%lld", &a[i]);

    long long answers[105][105] = {0};

    for (int k = 1; k <= n; ++k) {
        int idx[105];
        for (int i = 0; i < n; ++i) idx[i] = i;
        qsort_r(idx, n, sizeof(int), cmp_value_desc_index_asc, a);
        qsort(idx, k, sizeof(int), cmp_index_asc);
        for (int i = 0; i < k; ++i)
            answers[k][i] = a[idx[i]];
    }

    int m;
    scanf("%d", &m);

    while (m--) {
        int k, pos;
        scanf("%d %d", &k, &pos);
        printf("%lld\n", answers[k][pos - 1]);
    }

    return 0;
}

