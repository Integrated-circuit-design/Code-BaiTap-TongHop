#include <stdio.h>
#include <math.h>

#define MAX 1000000

int prime[MAX + 1];
int pref[MAX + 1];

void sieve(int n) {
    for (int i = 0; i <= n; i++)
        prime[i] = 1;
    prime[0] = prime[1] = 0;
    for (int i = 2; i * i <= n; i++) {
        if (prime[i]) {
            for (int j = i * i; j <= n; j += i)
                prime[j] = 0;
        }
    }
}

void build_prefix(int n) {
    pref[0] = 0;
    for (int i = 1; i <= n; i++)
        pref[i] = pref[i - 1] + prime[i];
}

int valid_length(int a, int b, int k, int l) {
    for (int x = a; x + l - 1 <= b; x++) {
        int y = x + l - 1;
        int cnt = pref[y] - pref[x - 1];
        if (cnt < k)
            return 0;
    }
    return 1;
}

int main() {
    int a, b, k;
    scanf("%d %d %d", &a, &b, &k);
    sieve(b);
    build_prefix(b);
    int ans = -1;
    for (int l = 1; l <= b - a + 1; l++) {
        if (valid_length(a, b, k, l)) {
            ans = l;
            break;
        }
    }
    printf("%d\n", ans);
    return 0;
}

