#include <stdio.h>
#include <math.h>

int main() {
    int n;
    scanf("%d", &n);

    int k = (int)ceil(sqrt(n));
    int start = n;

    while (start > 0) {
        int left = start - k + 1;
        if (left < 1) left = 1;
        for (int i = left; i <= start; i++)
            printf("%d ", i);
        start = left - 1;
    }

    return 0;
}

