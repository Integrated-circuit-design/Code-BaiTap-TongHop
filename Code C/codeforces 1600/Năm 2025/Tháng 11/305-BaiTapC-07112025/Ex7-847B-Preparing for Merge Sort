#include <stdio.h>

#define MAXN 200005

int a[MAXN], idx[MAXN];
char used[MAXN];

int main() {
    int n;
    scanf("%d", &n);
    for (int i = 0; i < n; i++) scanf("%d", &a[i]);

    int remain = n;
    for (int i = 0; i < n; i++) idx[i] = i;

    while (remain > 0) {
        int last = -1, newRemain = 0;
        for (int i = 0; i < remain; i++) {
            int p = idx[i];
            if (!used[p] && a[p] > last) {
                printf("%d ", a[p]);
                used[p] = 1;
                last = a[p];
            } else {
                idx[newRemain++] = p;
            }
        }
        remain = newRemain;
        printf("\n");
    }
    return 0;
}

