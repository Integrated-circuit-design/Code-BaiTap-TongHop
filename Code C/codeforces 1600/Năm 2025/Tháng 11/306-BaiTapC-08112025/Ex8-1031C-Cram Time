#include <stdio.h>

int main() {
    long long a, b;
    scanf("%lld %lld", &a, &b);

    int day1[1000], day2[1000];
    int cnt1 = 0, cnt2 = 0;
    long long sum1 = 0, sum2 = 0;
    int note = 1;

    while (sum1 + note <= a) {
        sum1 += note;
        day1[cnt1++] = note;
        note++;
    }

    while (sum2 + note <= b) {
        sum2 += note;
        day2[cnt2++] = note;
        note++;
    }

    printf("%d\n", cnt1);
    for (int i = 0; i < cnt1; i++) {
        printf("%d%c", day1[i], i == cnt1 - 1 ? '\n' : ' ');
    }

    printf("%d\n", cnt2);
    for (int i = 0; i < cnt2; i++) {
        printf("%d%c", day2[i], i == cnt2 - 1 ? '\n' : ' ');
    }

    return 0;
}

