#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);
    int cows[n];
    for(int i = 0; i < n; i++) {
        scanf("%d", &cows[i]);
    }

    long long prefix[n+1];
    prefix[0] = 0;
    for(int i = 1; i <= n; i++) {
        prefix[i] = prefix[i-1] + (cows[i-1] == 0 ? 1 : 0);
    }

    long long milkLost = 0;

    for(int i = 0; i < n; i++) {
        if(cows[i] == 1) {
            milkLost += prefix[n] - prefix[i+1];
        }
    }

    printf("%lld\n", milkLost);
    return 0;
}

