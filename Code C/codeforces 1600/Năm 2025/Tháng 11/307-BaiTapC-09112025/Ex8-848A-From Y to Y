#include <stdio.h>

int main() {
    long long k;
    scanf("%lld", &k);

    char result[100005];
    int len = 0;

    long long currentCost;
    
    for (char c = 'a'; c <= 'z'; c++) {
        if (k == 0) break;

        int count;
        for (count = 1; ; count++) {
            currentCost = (long long)count * (count - 1) / 2;
            if (currentCost > k) break;
        }
        count--;

        k -= (long long)count * (count - 1) / 2;

        for (int i = 0; i < count; i++) {
            result[len++] = c;
        }
    }

    if (len == 0) {
        result[len++] = 'a';
    }

    result[len] = '\0';

    printf("%s\n", result);

    return 0;
}

