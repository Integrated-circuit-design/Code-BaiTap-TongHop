#include <stdio.h>

int main() {
    int a, b;
    scanf("%d %d", &a, &b);

    if (a == b) {
        printf("YES\n");
        printf("0 0\n");
        printf("%d %d\n", a, 1);
        printf("%d %d\n", -1, b);
    } else {
        int found = 0;
        for (int x = 1; x < a && !found; x++) {
            int y2 = a * a - x * x;
            int y = 1;
            while (y * y < y2) y++;
            if (y * y != y2) continue;

            if ((b * y) % a == 0 && (b * x) % a == 0) {
                int x2 = -(b * y) / a;
                int y2b = (b * x) / a;
                printf("YES\n");
                printf("0 0\n");
                printf("%d %d\n", x, y);
                printf("%d %d\n", x2, y2b);
                found = 1;
            }
        }
        if (!found) printf("NO\n");
    }

    return 0;
}

