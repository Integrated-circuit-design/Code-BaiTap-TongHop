#include <stdio.h>

typedef long long ll;

int main() {
    int n;
    scanf("%d", &n);

    ll x1[n], y1[n], x2[n], y2[n];
    for (int i = 0; i < n; i++)
        scanf("%lld %lld %lld %lld", &x1[i], &y1[i], &x2[i], &y2[i]);

    ll pre_max_x1[n], pre_max_y1[n], pre_min_x2[n], pre_min_y2[n];
    ll suf_max_x1[n], suf_max_y1[n], suf_min_x2[n], suf_min_y2[n];

    pre_max_x1[0] = x1[0];
    pre_max_y1[0] = y1[0];
    pre_min_x2[0] = x2[0];
    pre_min_y2[0] = y2[0];
    for (int i = 1; i < n; i++) {
        pre_max_x1[i] = (x1[i] > pre_max_x1[i-1]) ? x1[i] : pre_max_x1[i-1];
        pre_max_y1[i] = (y1[i] > pre_max_y1[i-1]) ? y1[i] : pre_max_y1[i-1];
        pre_min_x2[i] = (x2[i] < pre_min_x2[i-1]) ? x2[i] : pre_min_x2[i-1];
        pre_min_y2[i] = (y2[i] < pre_min_y2[i-1]) ? y2[i] : pre_min_y2[i-1];
    }

    suf_max_x1[n-1] = x1[n-1];
    suf_max_y1[n-1] = y1[n-1];
    suf_min_x2[n-1] = x2[n-1];
    suf_min_y2[n-1] = y2[n-1];
    for (int i = n-2; i >= 0; i--) {
        suf_max_x1[i] = (x1[i] > suf_max_x1[i+1]) ? x1[i] : suf_max_x1[i+1];
        suf_max_y1[i] = (y1[i] > suf_max_y1[i+1]) ? y1[i] : suf_max_y1[i+1];
        suf_min_x2[i] = (x2[i] < suf_min_x2[i+1]) ? x2[i] : suf_min_x2[i+1];
        suf_min_y2[i] = (y2[i] < suf_min_y2[i+1]) ? y2[i] : suf_min_y2[i+1];
    }

    for (int i = 0; i < n; i++) {
        ll Lx, Ly, Rx, Ry;
        if (i == 0) {
            Lx = suf_max_x1[1];
            Ly = suf_max_y1[1];
            Rx = suf_min_x2[1];
            Ry = suf_min_y2[1];
        } else if (i == n-1) {
            Lx = pre_max_x1[n-2];
            Ly = pre_max_y1[n-2];
            Rx = pre_min_x2[n-2];
            Ry = pre_min_y2[n-2];
        } else {
            Lx = (pre_max_x1[i-1] > suf_max_x1[i+1]) ? pre_max_x1[i-1] : suf_max_x1[i+1];
            Ly = (pre_max_y1[i-1] > suf_max_y1[i+1]) ? pre_max_y1[i-1] : suf_max_y1[i+1];
            Rx = (pre_min_x2[i-1] < suf_min_x2[i+1]) ? pre_min_x2[i-1] : suf_min_x2[i+1];
            Ry = (pre_min_y2[i-1] < suf_min_y2[i+1]) ? pre_min_y2[i-1] : suf_min_y2[i+1];
        }

        if (Lx <= Rx && Ly <= Ry) {
            printf("%lld %lld\n", Lx, Ly);
            return 0;
        }
    }

    return 0;
}

