#include <stdio.h>

#define MAXN 1000

int main() {
    int n;
    scanf("%d", &n);

    int A[MAXN][MAXN];

    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            scanf("%d", &A[i][j]);

    int q;
    scanf("%d", &q);

    char output[q + 1];
    int idx = 0;

    for (int t = 0; t < q; t++) {
        int type;
        scanf("%d", &type);

        if (type == 1) {
            int row;
            scanf("%d", &row);
            row--;
            for (int j = 0; j < n; j++) {
                A[row][j] = 1 - A[row][j];
            }
        } else if (type == 2) {
            int col;
            scanf("%d", &col);
            col--;
            for (int i = 0; i < n; i++) {
                A[i][col] = 1 - A[i][col];
            }
        } else {
            int res = 0;
            for (int i = 0; i < n; i++) {
                int sum = 0;
                for (int j = 0; j < n; j++) {
                    sum ^= (A[i][j] & A[j][i]);
                }
                res ^= sum;
            }
            output[idx++] = '0' + res;
        }
    }

    output[idx] = '\0';
    printf("%s\n", output);

    return 0;
}

