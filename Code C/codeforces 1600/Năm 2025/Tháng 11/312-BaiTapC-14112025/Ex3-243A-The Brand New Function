#include <stdio.h>
#include <stdlib.h>

#define MAXN 100000
#define MAXA 1000000

int a[MAXN];
char appeared[MAXA + 1];
int orStack[25];

int main() {
    int n;
    scanf("%d", &n);
    for(int i = 0; i < n; i++) scanf("%d", &a[i]);

    int sz = 0;
    int distinctCount = 0;

    for(int i = 0; i < n; i++){
        int newSz = 0;
        int val;

        orStack[newSz++] = a[i];

        for(int j = 0; j < sz; j++){
            val = orStack[j] | a[i];
            if(val != orStack[newSz-1]) orStack[newSz++] = val;
        }

        for(int j = 0; j < newSz; j++){
            if(orStack[j] <= MAXA && !appeared[orStack[j]]){
                appeared[orStack[j]] = 1;
                distinctCount++;
            }
        }

        sz = newSz;
        for(int j = 0; j < sz; j++) orStack[j] = orStack[j];
    }

    printf("%d\n", distinctCount);
    return 0;
}

