#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int times[n];
    for(int i = 0; i < n; i++) {
        scanf("%d", &times[i]);
    }

    long long total[n];
    total[0] = 20;
    printf("%lld\n", total[0]);

    int idx90 = 0, idx1440 = 0;

    for(int i = 1; i < n; i++) {
        long long cost1 = total[i-1] + 20;

        while(times[idx90] < times[i] - 89) idx90++;
        long long cost90 = (idx90 > 0 ? total[idx90-1] : 0) + 50;

        while(times[idx1440] < times[i] - 1439) idx1440++;
        long long cost1440 = (idx1440 > 0 ? total[idx1440-1] : 0) + 120;

        long long minCost = cost1;
        if(cost90 < minCost) minCost = cost90;
        if(cost1440 < minCost) minCost = cost1440;

        total[i] = minCost;

        printf("%lld\n", total[i] - total[i-1]);
    }

    return 0;
}

