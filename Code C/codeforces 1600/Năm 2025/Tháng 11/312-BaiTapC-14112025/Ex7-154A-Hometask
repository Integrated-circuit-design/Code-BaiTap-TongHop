#include <stdio.h>
#include <string.h>

int forbidden[26];

int main() {
    char s[100005];
    scanf("%s", s);
    int n = strlen(s);

    int k;
    scanf("%d", &k);
    for (int i = 0; i < 26; i++) forbidden[i] = -1;

    for (int i = 0; i < k; i++) {
        char a, b;
        scanf(" %c%c", &a, &b);
        forbidden[a - 'a'] = b - 'a';
        forbidden[b - 'a'] = a - 'a';
    }

    int removed = 0;
    char lastChar = 0;

    for (int i = 0; i < n; i++) {
        int cur = s[i] - 'a';
        if (lastChar != 0 && forbidden[lastChar] == cur) {
            removed++;
        } else {
            lastChar = cur;
        }
    }

    printf("%d\n", removed);
    return 0;
}

