#include <stdio.h>

int main() {
    int N, A, B;
    scanf("%d %d %d", &N, &A, &B);

    int found = 0;
    int countA = 0, countB = 0;

    for (int i = 0; i * A <= N; i++) {
        int rem = N - i * A;
        if (rem % B == 0) {
            countA = i;
            countB = rem / B;
            found = 1;
            break;
        }
    }

    if (!found) {
        printf("-1\n");
        return 0;
    }

    int cur = 1;

    for (int i = 0; i < countA; i++) {
        for (int j = 0; j < A; j++) {
            int val = cur + (j + 1) % A;
            printf("%d ", val);
        }
        cur += A;
    }

    for (int i = 0; i < countB; i++) {
        for (int j = 0; j < B; j++) {
            int val = cur + (j + 1) % B;
            printf("%d ", val);
        }
        cur += B;
    }

    printf("\n");
    return 0;
}

