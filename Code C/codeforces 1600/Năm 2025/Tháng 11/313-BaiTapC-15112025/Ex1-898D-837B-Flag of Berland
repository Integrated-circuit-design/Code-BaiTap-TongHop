#include <stdio.h>

char flag[105][105];

int main() {
    int n, m;
    scanf("%d %d", &n, &m);

    for (int i = 0; i < n; i++)
        scanf("%s", flag[i]);

    int valid = 0;

    if (n % 3 == 0) {
        int h = n / 3;
        char colors[3];
        colors[0] = flag[0][0];
        colors[1] = flag[h][0];
        colors[2] = flag[2*h][0];

        if (colors[0] != colors[1] && colors[0] != colors[2] && colors[1] != colors[2]) {
            int ok = 1;
            for (int i = 0; i < n && ok; i++) {
                char expected = colors[i / h];
                for (int j = 0; j < m; j++) {
                    if (flag[i][j] != expected) {
                        ok = 0;
                        break;
                    }
                }
            }
            if (ok) valid = 1;
        }
    }

    if (!valid && m % 3 == 0) {
        int w = m / 3;
        char colors[3];
        colors[0] = flag[0][0];
        colors[1] = flag[0][w];
        colors[2] = flag[0][2*w];

        if (colors[0] != colors[1] && colors[0] != colors[2] && colors[1] != colors[2]) {
            int ok = 1;
            for (int j = 0; j < m && ok; j++) {
                char expected = colors[j / w];
                for (int i = 0; i < n; i++) {
                    if (flag[i][j] != expected) {
                        ok = 0;
                        break;
                    }
                }
            }
            if (ok) valid = 1;
        }
    }

    if (valid)
        printf("YES\n");
    else
        printf("NO\n");

    return 0;
}

