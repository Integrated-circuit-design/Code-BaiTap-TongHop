#include <stdio.h>
#include <stdlib.h>

#define MAX 100005
#define ll long long

ll absll(ll x) { return x < 0 ? -x : x; }

int main() {
    ll n, m, v;
    int cl, ce;
    scanf("%lld %lld %d %d %lld", &n, &m, &cl, &ce, &v);

    ll stairs[MAX], elevators[MAX];

    for(int i = 0; i < cl; i++) scanf("%lld", &stairs[i]);
    for(int i = 0; i < ce; i++) scanf("%lld", &elevators[i]);

    int q;
    scanf("%d", &q);

    while(q--) {
        ll x1, y1, x2, y2;
        scanf("%lld %lld %lld %lld", &x1, &y1, &x2, &y2);

        if(x1 == x2) {
            printf("%lld\n", absll(y1 - y2));
            continue;
        }

        ll floorDiff = absll(x1 - x2);
        ll minTime = 1e18;

        for(int i = 0; i < cl; i++) {
            ll t = absll(y1 - stairs[i]) + floorDiff + absll(y2 - stairs[i]);
            if(t < minTime) minTime = t;
        }

        for(int i = 0; i < ce; i++) {
            ll elevTime = (floorDiff + v - 1) / v;
            ll t = absll(y1 - elevators[i]) + elevTime + absll(y2 - elevators[i]);
            if(t < minTime) minTime = t;
        }

        printf("%lld\n", minTime);
    }

    return 0;
}

