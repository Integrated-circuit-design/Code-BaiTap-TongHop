#include <stdio.h>

int main() {
    int N, Q;
    scanf("%d %d", &N, &Q);

    int R[N + 1], C[N + 1];

    for (int i = 1; i <= N; i++) scanf("%d", &R[i]);
    for (int i = 1; i <= N; i++) scanf("%d", &C[i]);

    while (Q--) {
        int ra, ca, rb, cb;
        scanf("%d %d %d %d", &ra, &ca, &rb, &cb);

        int startParity = (R[ra] + C[ca]) % 2;
        int endParity   = (R[rb] + C[cb]) % 2;

        if (startParity != endParity) {
            printf("NO\n");
            continue;
        }

        int rowReachable = ((R[ra] % 2) == (R[rb] % 2));
        int colReachable = ((C[ca] % 2) == (C[cb] % 2));

        if (rowReachable && colReachable)
            printf("YES\n");
        else
            printf("NO\n");
    }

    return 0;
}

