#include <stdio.h>

#define MAX 1005

int n;
long long y[MAX];

int try_two_lines(int a, int b) {
    long long dx1 = b - a;
    long long dy1 = y[b] - y[a];

    int group[MAX] = {0};
    group[a] = 1;
    group[b] = 1;

    for (int i = 1; i <= n; i++) {
        if (group[i]) continue;
        if ((i - a) * dy1 == (y[i] - y[a]) * dx1) {
            group[i] = 1;
        }
    }

    int first = -1, second = -1;
    for (int i = 1; i <= n; i++) {
        if (group[i] == 0) {
            if (first == -1) first = i;
            else if (second == -1) second = i;
        }
    }

    if (first == -1 || second == -1) return 1;

    long long dx2 = second - first;
    long long dy2 = y[second] - y[first];

    if (dy1 * dx2 != dy2 * dx1) return 0;

    for (int i = 1; i <= n; i++) {
        if (group[i] == 0) {
            if ((i - first) * dy2 != (y[i] - y[first]) * dx2)
                return 0;
        }
    }

    return 1;
}

int main() {
    scanf("%d", &n);
    for (int i = 1; i <= n; i++) scanf("%lld", &y[i]);

    int result = 0;

    if (try_two_lines(1, 2) || try_two_lines(1, 3) || try_two_lines(2, 3))
        result = 1;

    if (result) printf("Yes\n");
    else printf("No\n");

    return 0;
}

