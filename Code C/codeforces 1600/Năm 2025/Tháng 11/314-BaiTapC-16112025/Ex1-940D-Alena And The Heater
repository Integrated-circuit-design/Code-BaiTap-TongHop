#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    long long a[n];
    for (int i = 0; i < n; i++) scanf("%lld", &a[i]);

    char b[n + 1];
    scanf("%s", b);

    long long l = -1000000000LL - 10;
    long long r =  1000000000LL + 10;

    for (int i = 4; i < n; i++) {
        int count0 = 0, count1 = 0;
        for (int j = i - 4; j < i; j++) {
            if (b[j] == '0') count0++;
            if (b[j] == '1') count1++;
        }

        if (b[i] == '1' && count0 == 4) {
            long long mx = a[i];
            for (int j = i - 4; j <= i; j++)
                if (a[j] > mx) mx = a[j];
            if (mx + 1 > l) l = mx + 1;
        }

        if (b[i] == '0' && count1 == 4) {
            long long mn = a[i];
            for (int j = i - 4; j <= i; j++)
                if (a[j] < mn) mn = a[j];
            if (mn - 1 < r) r = mn - 1;
        }
    }

    printf("%lld %lld\n", l, r);

    return 0;
}

