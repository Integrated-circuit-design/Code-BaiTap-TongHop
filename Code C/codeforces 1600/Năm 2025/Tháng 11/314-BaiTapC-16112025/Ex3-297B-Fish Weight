#include <stdio.h>
#include <stdlib.h>

int main() {
    int n, m;
    long long k;
    scanf("%d %d %lld", &n, &m, &k);

    int *alice = (int*)malloc(n * sizeof(int));
    int *bob = (int*)malloc(m * sizeof(int));

    for (int i = 0; i < n; i++) scanf("%d", &alice[i]);
    for (int i = 0; i < m; i++) scanf("%d", &bob[i]);

    int maxAlice = alice[0];
    for (int i = 1; i < n; i++) {
        if (alice[i] > maxAlice) maxAlice = alice[i];
    }

    int found = 0;
    for (int i = 0; i < m; i++) {
        if (bob[i] == maxAlice) {
            found = 1;
            break;
        }
    }

    if (found) {
        printf("NO\n");
    } else {
        printf("YES\n");
    }

    free(alice);
    free(bob);
    return 0;
}

