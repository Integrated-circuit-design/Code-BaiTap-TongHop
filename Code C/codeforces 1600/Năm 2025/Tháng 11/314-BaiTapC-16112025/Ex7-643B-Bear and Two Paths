#include <stdio.h>

int main() {
    int n, k;
    scanf("%d %d", &n, &k);

    int a, b, c, d;
    scanf("%d %d %d %d", &a, &b, &c, &d);

    if (n <= 4 || k < n + 1) {
        printf("-1\n");
        return 0;
    }

    int rest[1000], m = 0;
    for (int i = 1; i <= n; i++) {
        if (i != a && i != b && i != c && i != d) {
            rest[m++] = i;
        }
    }

    printf("%d ", a);
    for (int i = 0; i < m/2; i++) printf("%d ", rest[i]);
    printf("%d ", c);
    for (int i = m/2; i < m; i++) printf("%d ", rest[i]);
    printf("%d %d\n", d, b);

    printf("%d ", c);
    for (int i = m-1; i >= m/2; i--) printf("%d ", rest[i]);
    printf("%d ", a);
    for (int i = m/2-1; i >= 0; i--) printf("%d ", rest[i]);
    printf("%d %d\n", b, d);

    return 0;
}

