#include <stdio.h>

long long countWays(int n, int k) {
    long long res = 1;
    for(int i = 1; i <= k; i++) {
        res = res * (n + i - 1) / i;
    }
    return res;
}

int main() {
    int n;
    scanf("%d", &n);

    long long waysA = countWays(n, 5);
    long long waysB = countWays(n, 3);

    long long totalWays = waysA * waysB;

    printf("%lld\n", totalWays);

    return 0;
}

