#include <stdio.h>
#include <limits.h>

#define MAXN 100000

int main() {
    int n;
    scanf("%d", &n);

    int a[MAXN];
    int taken[MAXN] = {0};
    for (int i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }

    int totalMoves = 0;
    int remaining = n;

    while (remaining > 0) {
        int minVal = INT_MAX;
        for (int i = 0; i < n; i++) {
            if (!taken[i] && a[i] < minVal) {
                minVal = a[i];
            }
        }

        for (int i = 0; i < n; i++) {
            if (!taken[i]) {
                totalMoves++;
                if (a[i] == minVal) {
                    taken[i] = 1;
                    remaining--;
                    break;
                }
            }
        }
    }

    printf("%d\n", totalMoves);
    return 0;
}

