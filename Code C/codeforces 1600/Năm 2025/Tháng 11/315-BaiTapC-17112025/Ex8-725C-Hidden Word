#include <stdio.h>
#include <string.h>

int main() {
    char s[28];
    scanf("%s", s);

    int pos[26][2];
    int used[26] = {0};
    for(int i = 0; i < 26; i++) pos[i][0] = pos[i][1] = -1;

    for(int i = 0; i < 27; i++){
        int c = s[i] - 'A';
        if(pos[c][0] == -1) pos[c][0] = i;
        else pos[c][1] = i;
    }

    int start = -1, end = -1;
    for(int i = 0; i < 26; i++){
        if(pos[i][1] != -1){
            start = pos[i][0];
            end = pos[i][1];
            break;
        }
    }

    if(end - start == 1){
        printf("Impossible\n");
        return 0;
    }

    char row1[14], row2[14];
    for(int i = 0; i < 13; i++) row1[i] = row2[i] = 0;

    int middle = (start + end)/2;
    int len = end - start - 1;

    row1[len/2] = s[start];
    row2[len - len/2] = s[end];

    int r1 = len/2 - 1, r2 = len - len/2 + 1;
    for(int i = start+1; i < end; i++){
        if(r1 >= 0) row1[r1--] = s[i];
        else row2[r2++] = s[i];
    }

    int idx1 = 0, idx2 = 12;
    for(int i = start-1; i >=0; i--){
        if(row1[idx1] == 0) row1[idx1++] = s[i];
        else row2[idx2--] = s[i];
    }

    for(int i = end+1; i < 27; i++){
        if(row1[idx1] == 0) row1[idx1++] = s[i];
        else row2[idx2--] = s[i];
    }

    for(int i = 0; i < 13; i++) printf("%c", row1[i]);
    printf("\n");
    for(int i = 0; i < 13; i++) printf("%c", row2[i]);
    printf("\n");

    return 0;
}

