#include <stdio.h>
#include <string.h>
#include <ctype.h>

typedef enum {ADJ, NOUN, VERB} WordType;
typedef enum {MASC, FEM} Gender;

int check_word(const char *w, WordType *t, Gender *g) {
    int len = strlen(w);
    if (len >= 4 && strcmp(w + len - 4, "lios") == 0) { *t=ADJ; *g=MASC; return 1; }
    if (len >= 5 && strcmp(w + len - 5, "liala") == 0) { *t=ADJ; *g=FEM; return 1; }
    if (len >= 3 && strcmp(w + len - 3, "etr") == 0) { *t=NOUN; *g=MASC; return 1; }
    if (len >= 4 && strcmp(w + len - 4, "etra") == 0) { *t=NOUN; *g=FEM; return 1; }
    if (len >= 6 && strcmp(w + len - 6, "initis") == 0) { *t=VERB; *g=MASC; return 1; }
    if (len >= 6 && strcmp(w + len - 6, "inites") == 0) { *t=VERB; *g=FEM; return 1; }
    return 0;
}

int main() {
    char line[200010];
    fgets(line, sizeof(line), stdin);

    int i=0, start=0, word_count=0;
    WordType types[100000];
    Gender genders[100000];

    while (1) {
        while (line[i] && line[i]!=' ' && line[i]!='\n') i++;
        if (i>start) {
            line[i] = 0;
            WordType t; Gender g;
            if (!check_word(line+start, &t, &g)) { printf("NO\n"); return 0; }
            types[word_count] = t;
            genders[word_count] = g;
            word_count++;
        }
        if (line[i]=='\0' || line[i]=='\n') break;
        i++; start = i;
    }

    if (word_count==1) { printf("YES\n"); return 0; }

    for (i=1; i<word_count; i++) if (genders[i]!=genders[0]) { printf("NO\n"); return 0; }

    int noun_pos=-1;
    for (i=0;i<word_count;i++) {
        if (types[i]==NOUN) {
            if (noun_pos!=-1) { printf("NO\n"); return 0; }
            noun_pos = i;
        }
    }
    if (noun_pos==-1) { printf("NO\n"); return 0; }

    for (i=0;i<noun_pos;i++) if (types[i]!=ADJ) { printf("NO\n"); return 0; }
    for (i=noun_pos+1;i<word_count;i++) if (types[i]!=VERB) { printf("NO\n"); return 0; }

    printf("YES\n");
    return 0;
}

