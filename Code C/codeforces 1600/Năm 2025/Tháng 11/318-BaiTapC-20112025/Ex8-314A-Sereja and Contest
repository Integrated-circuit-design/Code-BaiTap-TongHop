#include <stdio.h>

#define MAXN 100000

int main() {
    int n, k;
    scanf("%d %d", &n, &k);

    int a[MAXN];
    for (int i = 0; i < n; i++) scanf("%d", &a[i]);

    int segments[MAXN];
    int segLen = 0;

    segments[segLen++] = a[0];
    for (int i = 1; i < n; i++) {
        if (a[i] != a[i-1]) segments[segLen++] = a[i];
    }

    int count[105] = {0};

    for (int i = 0; i < segLen; i++) {
        int cur = segments[i];

        if (i > 0) {
            int prev = segments[i-1];
            if (prev != cur) count[cur]++;
        }
        if (i + 1 < segLen) {
            int next = segments[i+1];
            if (next != cur) count[cur]++;
        }
    }

    int bestGenre = 1;
    int bestStress = count[1];

    for (int g = 2; g <= k; g++) {
        if (count[g] < bestStress) {
            bestStress = count[g];
            bestGenre = g;
        }
    }

    printf("%d\n", bestGenre);

    return 0;
}

