#include <stdio.h>

int main() {
    int n, k;
    scanf("%d %d", &n, &k);
    int a[1000];
    for(int i=0;i<n;i++) scanf("%d",&a[i]);

    int start[1000] = {0};
    int finish[1000] = {0};
    int interesting[1000] = {0};

    int proc[100] = {0};
    int ptr = 0;

    for(int i=0;i<k && i<n;i++){
        start[i] = 0;
        finish[i] = a[i];
        proc[i] = finish[i];
        ptr++;
    }

    while(ptr < n){
        int minTime = proc[0], idx=0;
        for(int i=1;i<k;i++){
            if(proc[i]<minTime){ minTime=proc[i]; idx=i; }
        }
        start[ptr] = minTime;
        finish[ptr] = minTime + a[ptr];
        proc[idx] = finish[ptr];
        ptr++;
    }

    int done = 0;
    for(int t=1;t<=n*150;t++){
        int d=0;
        for(int i=0;i<n;i++) if(finish[i]<=t) d++;
        int caption = (100*d + n/2)/n;
        for(int i=0;i<n;i++){
            if(start[i]<t && t<=finish[i] && !interesting[i]){
                int testNum = t - start[i];
                if(testNum==caption) interesting[i]=1;
            }
        }
        done=d;
        if(done==n) break;
    }

    int ans=0;
    for(int i=0;i<n;i++) if(interesting[i]) ans++;
    printf("%d\n",ans);
    return 0;
}

