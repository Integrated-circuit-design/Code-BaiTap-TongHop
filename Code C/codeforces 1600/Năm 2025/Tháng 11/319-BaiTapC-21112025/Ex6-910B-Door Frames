#include <stdio.h>

int main() {
    int n, a, b;
    scanf("%d %d %d", &n, &a, &b);

    int segments[6] = {a, a, a, a, b, b};
    int bars_used = 0;
    int used[6] = {0};

    for (int i = 0; i < 6; i++) {
        if (used[i]) continue;

        int remaining = n;
        bars_used++;
        for (int j = i; j < 6; j++) {
            if (!used[j] && segments[j] <= remaining) {
                remaining -= segments[j];
                used[j] = 1;
            }
        }
    }

    printf("%d\n", bars_used);
    return 0;
}

