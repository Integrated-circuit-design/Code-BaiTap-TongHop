#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);

    while(t--) {
        int n;
        scanf("%d", &n);
        int A[105];

        for(int i = 0; i < n; i++) {
            scanf("%d", &A[i]);
        }

        int startB = 1;

        printf("YES\n");
        for(int i = 0; i < n; i++) {
            int bj = startB + i * n;
            printf("%d ", bj);
        }
        printf("\n");
    }

    return 0;
}

