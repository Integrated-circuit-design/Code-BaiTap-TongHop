#include <stdio.h>

int main() {
    int n, k;
    scanf("%d %d", &n, &k);

    for (int step = 0; step < k; step++) {
        int i = 1;
        while (i <= n) {
            int cell = ((i - 1 + step) % n) + 1;
            printf("%d", cell);

            if (i < n) {
                printf(" ");
            }
            i++;
        }
        printf("\n");
    }

    return 0;
}

