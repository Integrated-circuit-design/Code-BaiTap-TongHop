#include <stdio.h>
#include <string.h>

int la_nguyen_am(char c) {
    return c=='a' || c=='e' || c=='i' || c=='o' || c=='u' || c=='y';
}

int main() {
    int n;
    scanf("%d", &n);

    char s[n+1];
    scanf("%s", s);

    char ket_qua[n+1];
    int k = 0;

    for (int i = 0; i < n; i++) {
        char c = s[i];

        if (k == 0) {
            ket_qua[k++] = c;
            continue;
        }

        char cuoi = ket_qua[k-1];

        if (!la_nguyen_am(c) || c != cuoi) {
            ket_qua[k++] = c;
        } else {
            if ((c=='e' || c=='o') && i+1 < n && s[i+1] != c) {
                ket_qua[k++] = c;
            }
        }
    }

    ket_qua[k] = '\0';
    printf("%s\n", ket_qua);

    return 0;
}

