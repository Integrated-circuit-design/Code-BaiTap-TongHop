#include <stdio.h>

int Solve(int qs[], int n) {
    int i = 0;
    int sum = 0;

    while (i < n) {
        sum += qs[i];
        i++;
    }

    if (sum == 1) return 1;

    return 0;
}

int main() {
    int n;
    scanf("%d", &n);

    int qs[8];
    int idx = 0;

    while (idx < n) {
        scanf("%d", &qs[idx]);
        idx++;
    }

    int result = Solve(qs, n);
    printf("%d\n", result);

    return 0;
}

