#include <stdio.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d", &n);

    int *mem = (int*)malloc(n * sizeof(int));
    for (int i = 0; i < n; i++) scanf("%d", &mem[i]);

    long long moves = 0;
    int emptyCount = 0;

    for (int i = 0; i < n; i++) {
        if (mem[i] == 0) {
            emptyCount++;
        } else {
            moves += emptyCount;
        }
    }

    printf("%lld\n", moves);
    free(mem);
    return 0;
}

