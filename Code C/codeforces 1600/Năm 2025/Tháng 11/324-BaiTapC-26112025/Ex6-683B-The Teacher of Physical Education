#include <stdio.h>
#include <string.h>

typedef struct {
    char name[6];
    int height;
} Student;

int main() {
    int n;
    scanf("%d", &n);

    Student s[n];
    for (int i = 0; i < n; i++) {
        scanf("%s %d", s[i].name, &s[i].height);
    }

    for (int i = 0; i < n - 1; i++) {
        int min_idx = i;
        for (int j = i + 1; j < n; j++) {
            if (s[j].height < s[min_idx].height)
                min_idx = j;
        }
        if (min_idx != i) {
            Student temp = s[i];
            s[i] = s[min_idx];
            s[min_idx] = temp;
        }
    }

    for (int i = 0; i < n; i++)
        printf("%s\n", s[i].name);

    return 0;
}

