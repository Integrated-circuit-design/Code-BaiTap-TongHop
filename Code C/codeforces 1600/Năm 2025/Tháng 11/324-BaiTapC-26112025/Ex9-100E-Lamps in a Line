#include <stdio.h>
#include <stdbool.h>

int main() {
    int n;
    scanf("%d", &n);

    bool lamp[n + 1];
    char buf[4];

    for (int i = 1; i <= n; i++) {
        scanf("%s", buf);
        lamp[i] = (buf[0] == 'o' && buf[1] == 'n');
    }

    int k;
    scanf("%d", &k);

    bool pressed[n + 1];
    for (int i = 0; i <= n; i++) pressed[i] = false;

    for (int i = 0; i < k; i++) {
        int key;
        scanf("%d", &key);
        pressed[key] = !pressed[key];
    }

    for (int key = 1; key <= n; key++) {
        if (pressed[key]) {
            for (int pos = key; pos <= n; pos += key)
                lamp[pos] = !lamp[pos];
        }
    }

    for (int i = 1; i <= n; i++) {
        if (lamp[i])
            printf("on ");
        else
            printf("off ");
    }

    return 0;
}

