#include <stdio.h>
#include <stdlib.h>

#define MAXN 200005

typedef struct Node {
    int v;
    struct Node *next;
} Node;

Node* adj[MAXN];
int used[MAXN];
int a[MAXN];

void addEdge(int u, int v) {
    Node* node = (Node*)malloc(sizeof(Node));
    node->v = v;
    node->next = adj[u];
    adj[u] = node;
}

int main() {
    int n;
    scanf("%d", &n);

    for(int i = 1; i <= n; i++) adj[i] = NULL;

    for(int i = 0; i < n-1; i++) {
        int u, v;
        scanf("%d %d", &u, &v);
        addEdge(u, v);
        addEdge(v, u);
    }

    for(int i = 1; i <= n; i++) scanf("%d", &a[i]);

    if(a[1] != 1) {
        printf("No\n");
        return 0;
    }

    int q[MAXN], front = 0, back = 0;
    q[back++] = 1;
    used[1] = 1;
    int idx = 2;

    while(front < back) {
        int v = q[front++];

        int childCount = 0;
        Node* ptr = adj[v];
        int children[MAXN];
        while(ptr) {
            if(!used[ptr->v]) {
                children[childCount++] = ptr->v;
                used[ptr->v] = 1;
            }
            ptr = ptr->next;
        }

        for(int i = 0; i < childCount; i++) {
            if(idx > n || children[i] != a[idx]) {
                printf("No\n");
                return 0;
            }
            q[back++] = children[i];
            idx++;
        }
    }

    printf("Yes\n");
    return 0;
}
