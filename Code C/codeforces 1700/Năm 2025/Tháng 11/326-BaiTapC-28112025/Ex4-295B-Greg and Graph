#include <stdio.h>
#define MAXN 505
#define INF 1000000000000000LL

long long g[MAXN][MAXN];
long long res[MAXN];
int delOrder[MAXN];
int exist[MAXN];

int main() {
    int n;
    scanf("%d", &n);

    for (int i = 1; i <= n; i++)
        for (int j = 1; j <= n; j++)
            scanf("%lld", &g[i][j]);

    for (int i = 1; i <= n; i++)
        scanf("%d", &delOrder[i]);

    for (int i = 1; i <= n; i++)
        exist[i] = 0;

    for (int step = n; step >= 1; step--) {
        int k = delOrder[step];
        exist[k] = 1;

        for (int i = 1; i <= n; i++) {
            if (!exist[i]) continue;
            for (int j = 1; j <= n; j++) {
                if (!exist[j]) continue;
                if (g[i][j] > g[i][k] + g[k][j])
                    g[i][j] = g[i][k] + g[k][j];
            }
        }

        long long sum = 0;
        for (int i = 1; i <= n; i++)
            if (exist[i])
                for (int j = 1; j <= n; j++)
                    if (exist[j])
                        sum += g[i][j];

        res[step] = sum;
    }

    for (int i = 1; i <= n; i++)
        printf("%lld ", res[i]);

    return 0;
}

