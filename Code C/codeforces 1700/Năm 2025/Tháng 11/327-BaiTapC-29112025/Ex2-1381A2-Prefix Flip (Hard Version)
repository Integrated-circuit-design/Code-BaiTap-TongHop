#include <stdio.h>
#include <string.h>

#define MAXN 100000

char a[MAXN + 5], b[MAXN + 5];
int ops[2 * MAXN + 5];

void flip_prefix(char *s, int len) {
    for (int i = 0; i < len / 2; i++) {
        char tmp = s[i] ^ 1;
        s[i] = s[len - i - 1] ^ 1;
        s[len - i - 1] = tmp;
    }
    if (len % 2 == 1) {
        s[len / 2] ^= 1;
    }
}

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        int n;
        scanf("%d", &n);
        scanf("%s", a);
        scanf("%s", b);

        int op_count = 0;

        for (int i = n - 1; i >= 0; i--) {
            if (a[i] == b[i]) continue;

            if (a[0] != b[i]) {
                ops[op_count++] = 1;
                flip_prefix(a, 1);
            }

            ops[op_count++] = i + 1;
            flip_prefix(a, i + 1);
        }

        printf("%d ", op_count);
        for (int i = 0; i < op_count; i++)
            printf("%d ", ops[i]);
        printf("\n");
    }
    return 0;
}

