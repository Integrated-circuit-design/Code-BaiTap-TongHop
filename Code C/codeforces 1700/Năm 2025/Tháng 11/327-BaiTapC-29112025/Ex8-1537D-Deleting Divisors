#include <stdio.h>

int isOdd(long long n) {
    return n % 2 != 0;
}

int isPowerOfTwo(long long n) {
    if (n < 1) return 0;
    while (n > 1) {
        if (n % 2 != 0) return 0;
        n /= 2;
    }
    return 1;
}

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        long long n;
        scanf("%lld", &n);

        if (n == 1 || isOdd(n) || isPowerOfTwo(n)) {
            printf("Bob\n");
        } else {
            printf("Alice\n");
        }
    }

    return 0;
}

