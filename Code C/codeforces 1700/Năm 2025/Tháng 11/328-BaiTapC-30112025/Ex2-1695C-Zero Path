#include <stdio.h>

int grid[1000][1000];
int n, m;
int found;

void dfs(int i, int j, int sum) {
    if (found) return;

    sum += grid[i][j];

    if (i == n-1 && j == m-1) {
        if (sum == 0) found = 1;
        return;
    }

    int steps_left = (n - 1 - i) + (m - 1 - j);
    if ((sum + steps_left) < 0 || (sum - steps_left) > 0) return;

    if (i + 1 < n) dfs(i+1, j, sum);
    if (j + 1 < m) dfs(i, j+1, sum);
}

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        scanf("%d %d", &n, &m);
        for (int i = 0; i < n; i++)
            for (int j = 0; j < m; j++)
                scanf("%d", &grid[i][j]);

        found = 0;

        if ((n + m - 1) % 2 != 0) {
            printf("NO\n");
            continue;
        }

        dfs(0, 0, 0);

        if (found) printf("YES\n");
        else printf("NO\n");
    }

    return 0;
}

