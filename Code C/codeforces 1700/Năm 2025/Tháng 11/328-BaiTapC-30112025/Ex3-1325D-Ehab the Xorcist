#include <stdio.h>

int main() {
    long long u, v;
    scanf("%lld %lld", &u, &v);

    if (u > v || ((v - u) & 1)) {
        printf("-1\n");
        return 0;
    }

    if (u == 0 && v == 0) {
        puts("0");
        return 0;
    }

    if (u == v) {
        printf("1\n%lld\n", u);
        return 0;
    }

    long long diff = (v - u) / 2;

    if ((diff & u) == 0) {
        printf("2\n");
        printf("%lld %lld\n", diff, diff + u);
    } else {
        printf("3\n");
        printf("%lld %lld %lld\n", diff, diff, u);
    }

    return 0;
}

