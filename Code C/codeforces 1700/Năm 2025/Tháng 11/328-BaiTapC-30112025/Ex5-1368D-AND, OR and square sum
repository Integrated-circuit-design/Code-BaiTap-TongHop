#include <stdio.h>

#define MAXN 200000
#define MAXBITS 20

int main() {
    int n;
    scanf("%d", &n);

    int numbers[MAXN];
    int bit_counter[MAXBITS] = {0};

    for (int i = 0; i < n; i++) {
        scanf("%d", &numbers[i]);
        int value = numbers[i];
        for (int b = 0; b < MAXBITS; b++) {
            if (value & (1 << b)) bit_counter[b]++;
        }
    }

    long long new_numbers[MAXN] = {0};
    for (int i = 0; i < n; i++) {
        long long current = 0;
        for (int b = 0; b < MAXBITS; b++) {
            if (bit_counter[b] > 0) {
                current |= (1LL << b);
                bit_counter[b]--;
            }
        }
        new_numbers[i] = current;
    }

    long long sum_squares = 0;
    for (int i = 0; i < n; i++) {
        sum_squares += new_numbers[i] * new_numbers[i];
    }

    printf("%lld\n", sum_squares);
    return 0;
}

