#include <stdio.h>
#include <stdint.h>

long long maxBitsMSB(long long l, long long r) {
    long long result = 0;
    for (int i = 63; i >= 0; i--) {
        long long bit = 1LL << i;
        if ((result | bit) <= r) {
            result |= bit;
        }
    }
    if (result < l) result = l;
    return result;
}

int main() {
    int n;
    scanf("%d", &n);
    for (int i = 0; i < n; i++) {
        long long l, r;
        scanf("%lld %lld", &l, &r);
        printf("%lld\n", maxBitsMSB(l, r));
    }
    return 0;
}

