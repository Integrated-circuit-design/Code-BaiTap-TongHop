#include <stdio.h>
#include <string.h>

#define MAX 1000005

char s[MAX];
int pi[MAX];

int min(int a, int b) {
    return a < b ? a : b;
}

void buildPrefixArray(char *str, int n, int *arr) {
    arr[0] = 0;
    for (int i = 1; i < n; i++) {
        int j = arr[i - 1];
        while (j > 0 && str[i] != str[j])
            j = arr[j - 1];
        if (str[i] == str[j])
            j++;
        arr[i] = j;
    }
}

int main() {
    scanf("%s", s);
    int n = strlen(s);

    buildPrefixArray(s, n, pi);

    int len = pi[n - 1];
    int candidate[MAX], c = 0;

    int temp = len;
    while (temp > 0) {
        candidate[c++] = temp;
        temp = pi[temp - 1];
    }

    int found = 0;
    for (int i = 0; i < c && !found; i++) {
        int border = candidate[i];
        for (int j = 0; j < n - 1; j++) {
            if (pi[j] == border) {
                found = border;
                break;
            }
        }
    }

    if (!found)
        printf("Just a legend\n");
    else {
        for (int i = 0; i < found; i++)
            putchar(s[i]);
        putchar('\n');
    }

    return 0;
}

