#include <stdio.h>
#include <stdlib.h>

typedef long long ll;

int cmp(const void *a, const void *b) {
    ll x = *(ll*)a, y = *(ll*)b;
    if (x < y) return -1;
    if (x > y) return 1;
    return 0;
}

ll sq(ll x) { return x*x; }

ll val(ll x, ll y, ll z) {
    return sq(x-y)+sq(y-z)+sq(z-x);
}

ll nearest(ll *arr, int n, ll target) {
    int i = 0;
    while (i < n && arr[i] < target) i++;
    if (i == 0) return arr[0];
    if (i == n) return arr[n-1];
    if (target - arr[i-1] <= arr[i] - target) return arr[i-1];
    else return arr[i];
}

int main() {
    int t;
    scanf("%d",&t);
    while(t--) {
        int nr, ng, nb;
        scanf("%d %d %d",&nr,&ng,&nb);

        ll R[nr], G[ng], B[nb];
        for(int i=0;i<nr;i++) scanf("%lld",&R[i]);
        for(int i=0;i<ng;i++) scanf("%lld",&G[i]);
        for(int i=0;i<nb;i++) scanf("%lld",&B[i]);

        qsort(R,nr,sizeof(ll),cmp);
        qsort(G,ng,sizeof(ll),cmp);
        qsort(B,nb,sizeof(ll),cmp);

        ll ans = (ll)4e18;

        for(int i=0;i<nr;i++){
            ll r = R[i];
            ll g = nearest(G, ng, r);
            ll b = nearest(B, nb, (r+g)/2);

            ll tmp = val(r,g,b);
            if(tmp < ans) ans = tmp;
        }

        printf("%lld\n", ans);
    }
    return 0;
}

