#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);

    while(t--) {
        int n, m;
        scanf("%d %d", &n, &m);

        int a[100005];
        for(int i = 0; i < n; i++)
            scanf("%d", &a[i]);

        int topic_covered[100005] = {0};
        int min_smart = 100000, max_smart = 0;

        for(int i = 0; i < n; i++) {
            if(a[i] < min_smart) min_smart = a[i];
            if(a[i] > max_smart) max_smart = a[i];

            for(int tnum = 1; tnum <= m; tnum++) {
                if(a[i] % tnum == 0)
                    topic_covered[tnum] = 1;
            }
        }

        int all_topics = 1;
        for(int tnum = 1; tnum <= m; tnum++) {
            if(topic_covered[tnum] == 0) {
                all_topics = 0;
                break;
            }
        }

        if(all_topics)
            printf("%d\n", max_smart - min_smart);
        else
            printf("-1\n");
    }

    return 0;
}

