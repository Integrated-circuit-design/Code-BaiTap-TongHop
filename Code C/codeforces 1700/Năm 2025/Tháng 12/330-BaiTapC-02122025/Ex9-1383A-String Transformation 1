#include <stdio.h>
#include <stdlib.h>
#include <string.h>

#define MAXN 100005

char A[MAXN], B[MAXN];
int pos[20][MAXN];
int cnt[20];

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        int n;
        scanf("%d", &n);
        scanf("%s", A);
        scanf("%s", B);

        int impossible = 0;

        for (int i = 0; i < 20; i++) cnt[i] = 0;

        for (int i = 0; i < n; i++) {
            int a = A[i] - 'a';
            int b = B[i] - 'a';
            if (a > b) impossible = 1;
            if (a < b) pos[a][cnt[a]++] = i;
        }

        if (impossible) {
            printf("-1\n");
            continue;
        }

        int moves = 0;

        for (int c = 0; c < 20; c++) {
            if (cnt[c] == 0) continue;

            int target = 21;
            for (int i = 0; i < cnt[c]; i++) {
                int idx = pos[c][i];
                int b = B[idx] - 'a';
                if (b < target) target = b;
            }

            moves++;

            for (int i = 0; i < cnt[c]; i++) {
                int idx = pos[c][i];
                if (B[idx] > target) {
                    pos[target][cnt[target]++] = idx;
                }
                A[idx] = target + 'a';
            }

            cnt[c] = 0;
        }

        printf("%d\n", moves);
    }

    return 0;
}

