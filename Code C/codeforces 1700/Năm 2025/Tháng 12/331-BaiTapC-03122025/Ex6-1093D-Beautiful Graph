#include <stdio.h>

#define MOD 998244353
#define MAXN 300005
#define MAXM 600005

int head[MAXN], to[MAXM], nxt[MAXM], edge_cnt;
int color[MAXN];
int n, m;

long long modpow(long long a, long long b) {
    long long res = 1;
    while (b) {
        if (b & 1) res = res * a % MOD;
        a = a * a % MOD;
        b >>= 1;
    }
    return res;
}

int dfs(int u, int c, int *cnt0, int *cnt1) {
    color[u] = c;
    if (c == 0) (*cnt0)++;
    else (*cnt1)++;

    for (int e = head[u]; e != -1; e = nxt[e]) {
        int v = to[e];
        if (color[v] == -1) {
            if (!dfs(v, c ^ 1, cnt0, cnt1)) return 0;
        } else if (color[v] == c) {
            return 0;
        }
    }
    return 1;
}

void add_edge(int u, int v) {
    to[edge_cnt] = v;
    nxt[edge_cnt] = head[u];
    head[u] = edge_cnt++;
}

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        scanf("%d %d", &n, &m);

        for (int i = 1; i <= n; i++) {
            head[i] = -1;
            color[i] = -1;
        }
        edge_cnt = 0;

        for (int i = 0; i < m; i++) {
            int u, v;
            scanf("%d %d", &u, &v);
            add_edge(u, v);
            add_edge(v, u);
        }

        long long ans = 1;
        for (int i = 1; i <= n; i++) {
            if (color[i] == -1) {
                int cnt0 = 0, cnt1 = 0;
                if (!dfs(i, 0, &cnt0, &cnt1)) {
                    ans = 0;
                    break;
                }
                long long ways = (modpow(2, cnt0) + modpow(2, cnt1)) % MOD;
                ans = ans * ways % MOD;
            }
        }

        printf("%lld\n", ans);
    }

    return 0;
}

