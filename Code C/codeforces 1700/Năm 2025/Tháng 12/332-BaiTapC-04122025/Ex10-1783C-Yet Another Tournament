#include <bits/stdc++.h>
using namespace std;
using ll = long long;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--) {

        int n;
        ll m;
        cin >> n >> m;

        vector<ll> a(n);
        for (int i = 0; i < n; i++) cin >> a[i];

        vector<pair<ll,int>> arr;
        arr.reserve(n);
        for (int i = 0; i < n; i++) arr.push_back({a[i], i + 1});

        sort(arr.begin(), arr.end());

        vector<ll> pref(n + 1, 0);
        for (int i = 1; i <= n; i++)
            pref[i] = pref[i - 1] + arr[i - 1].first;

        int best = n + 1;

        for (int k = 0; k <= n; k++) {

            if (pref[k] > m) continue;

            int total = max(0, n - (k + 1));

            int removed = 0;
            for (int i = 0; i < k; i++) {
                if (arr[i].second > k + 1)
                    removed++;
            }

            int rank = 1 + (total - removed);
            if (rank < best) best = rank;
        }

        cout << best << "\n";
    }

    return 0;
}

