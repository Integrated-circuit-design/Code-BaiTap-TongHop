#include <stdio.h>
#include <stdlib.h>
#include <math.h>

#define MAXA 10000000

int spf[MAXA + 1];
int primes[MAXA / 10];
int primeCount = 0;

long long f[MAXA + 1];

void linearSieve() {
    for(int i = 2; i <= MAXA; i++) {
        if(spf[i] == 0) {
            spf[i] = i;
            primes[primeCount++] = i;
        }
        for(int j = 0; j < primeCount && primes[j] <= spf[i] && i * primes[j] <= MAXA; j++) {
            spf[i * primes[j]] = primes[j];
        }
    }
}

int main() {
    int n;
    scanf("%d", &n);
    int *a = (int *)malloc(n * sizeof(int));
    for(int i = 0; i < n; i++) scanf("%d", &a[i]);

    linearSieve();

    for(int i = 0; i < n; i++) {
        int x = a[i];
        while(x > 1) {
            int p = spf[x];
            f[p]++;
            while(x % p == 0) x /= p;
        }
    }

    int m;
    scanf("%d", &m);
    while(m--) {
        long long l, r;
        scanf("%lld %lld", &l, &r);

        long long ans = 0;

        for(int i = 0; i < primeCount && primes[i] <= r; i++) {
            if(primes[i] >= l) {
                ans += f[primes[i]];
            }
        }
        printf("%lld\n", ans);
    }

    free(a);
    return 0;
}

