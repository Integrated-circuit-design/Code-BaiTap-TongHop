#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        int n;
        scanf("%d", &n);

        int p[n], q[n];
        for (int i = 0; i < n; i++) scanf("%d", &p[i]);

        int half = n / 2;

        int large = half + 1;
        int small = 1;

        for (int i = 0; i < n; i += 2) {
            q[i] = large;
            large++;
        }

        for (int i = 1; i < n; i += 2) {
            q[i] = small;
            small++;
        }

        for (int i = 0; i < n; i++) {
            printf("%d ", q[i]);
        }
        printf("\n");
    }

    return 0;
}

