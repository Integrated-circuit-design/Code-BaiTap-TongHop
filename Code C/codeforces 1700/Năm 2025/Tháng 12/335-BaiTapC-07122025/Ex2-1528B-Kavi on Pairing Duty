#include <stdio.h>
#include <stdlib.h>

#define MOD 998244353

int main() {
    int n;
    scanf("%d", &n);

    int *divCount = (int*)malloc((n + 1) * sizeof(int));
    int *ways     = (int*)malloc((n + 1) * sizeof(int));

    for (int i = 0; i <= n; ++i) {
        divCount[i] = 0;
        ways[i] = 0;
    }

    for (int j = 2; j <= n; ++j) {
        int x = 1;
        while (x * x <= j) {
            if (j % x == 0) {
                int y = j / x;
                if (x < j) divCount[j]++;
                if (y < j && y != x) divCount[j]++;
            }
            x++;
        }
    }

    ways[0] = 1;

    int prefixSum = 1;

    for (int i = 1; i <= n; ++i) {
        int val = divCount[i];
        val += prefixSum;
        if (val >= MOD) val -= MOD;

        ways[i] = val;

        prefixSum += val;
        if (prefixSum >= MOD) prefixSum -= MOD;
    }

    printf("%d\n", ways[n] % MOD);

    free(divCount);
    free(ways);
    return 0;
}

