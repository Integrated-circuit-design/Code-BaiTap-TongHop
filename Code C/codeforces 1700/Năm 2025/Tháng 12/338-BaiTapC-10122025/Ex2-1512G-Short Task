#include <stdio.h>
#include <stdlib.h>

#define MAX_C 10000000

int main() {
    int t;
    scanf("%d", &t);

    int *answers = (int*)calloc(MAX_C + 1, sizeof(int));
    for (int i = 0; i <= MAX_C; i++) answers[i] = -1;

    for (int n = 1; n <= MAX_C; n++) {
        int sum = 0;
        for (int i = 1; i*i <= n; i++) {
            if (n % i == 0) {
                sum += i;
                if (i != n/i) sum += n/i;
            }
        }

        if (sum <= MAX_C && answers[sum] == -1) {
            answers[sum] = n;
        }
    }

    while (t--) {
        int c;
        scanf("%d", &c);
        printf("%d\n", answers[c]);
    }

    free(answers);
    return 0;
}

