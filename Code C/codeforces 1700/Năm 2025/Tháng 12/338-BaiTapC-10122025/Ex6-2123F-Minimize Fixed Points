#include <stdio.h>

int find_gcd_candidate(int i, int used[], int n) {
    for (int j = 2; j <= n; j++) {
        if (!used[j] && __gcd(i, j) > 1) {
            return j;
        }
    }
    return -1;
}

void solve(int n) {
    int p[n + 1];
    int used[n + 1];
    for (int i = 0; i <= n; i++) used[i] = 0;

    p[1] = 1;
    used[1] = 1;

    for (int i = 2; i <= n; i++) {
        int candidate = find_gcd_candidate(i, used, n);
        if (candidate == -1) {
            p[i] = i;
            used[i] = 1;
        } else {
            p[i] = candidate;
            used[candidate] = 1;
        }
    }

    for (int i = 1; i <= n; i++) {
        printf("%d ", p[i]);
    }
    printf("\n");
}

int main() {
    int t;
    scanf("%d", &t);
    while (t--) {
        int n;
        scanf("%d", &n);
        solve(n);
    }
    return 0;
}

