#include <stdio.h>

typedef long long ll;

static ll get_gcd(ll x, ll y) {
    while (x != 0) {
        ll r = y % x;
        y = x;
        x = r;
    }
    return y;
}

int judge(ll r, ll b, ll k) {
    ll g = get_gcd(r, b);
    r /= g;
    b /= g;
    ll small = (r < b ? r : b);
    ll large = (r < b ? b : r);
    if (large > (k - 1) * small + 1)
        return 1;
    return 0;
}

int main() {
    int T;
    scanf("%d", &T);
    while (T--) {
        ll r, b, k;
        scanf("%lld %lld %lld", &r, &b, &k);
        if (judge(r, b, k))
            puts("REBEL");
        else
            puts("OBEY");
    }
    return 0;
}

