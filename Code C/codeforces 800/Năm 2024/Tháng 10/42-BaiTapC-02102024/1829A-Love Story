#include <stdio.h>

int main() {
    int t;
    char s[11];
    
    scanf("%d", &t); 
    
    while (t--) {
        scanf("%s", s);
        
        int demKhac = 0;
        
        for (int i = 0; i < 10; i++) {
            if (s[i] != "codeforces"[i]) {
                demKhac++;
            }
        }
        
        printf("%d\n", demKhac);
    }
    
    return 0;
}

