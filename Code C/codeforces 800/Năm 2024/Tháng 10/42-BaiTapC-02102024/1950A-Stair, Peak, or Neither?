#include <stdio.h>

int main() {
    int testCases;
    printf("Nhập số lượng bộ kiểm tra: ");
    scanf("%d", &testCases);

    for (int i = 0; i < testCases; i++) {
        int x, y, z;
        printf("Nhập ba chữ số x, y, z: ");
        scanf("%d %d %d", &x, &y, &z);

        if (x < y && y < z) {
            printf("STAIR\n");
        } else if (x < y && y > z) {
            printf("PEAK\n");
        } else {
            printf("NONE\n");
        }
    }

    return 0;
}

