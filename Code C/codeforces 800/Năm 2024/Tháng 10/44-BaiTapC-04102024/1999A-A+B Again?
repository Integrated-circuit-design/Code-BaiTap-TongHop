#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);

    for (int i = 0; i < t; i++) {
        int n;
        scanf("%d", &n);

        int chu_so_hang_chuc = n / 10;
        int chu_so_hang_don_vi = n % 10;
        int tong_chu_so = chu_so_hang_chuc + chu_so_hang_don_vi;

        printf("%d\n", tong_chu_so);
    }

    return 0;
}

