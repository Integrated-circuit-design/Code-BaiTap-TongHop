#include <stdio.h>

void print_min_max(int a, int b) {
    if (a < b) {
        printf("%d %d\n", a, b);
    } else {
        printf("%d %d\n", b, a);
    }
}

int main() {
    int num_cases;
    scanf("%d", &num_cases);

    for (int i = 0; i < num_cases; i++) {
        int x, y;
        scanf("%d %d", &x, &y);
        print_min_max(x, y);
    }

    return 0;
}

