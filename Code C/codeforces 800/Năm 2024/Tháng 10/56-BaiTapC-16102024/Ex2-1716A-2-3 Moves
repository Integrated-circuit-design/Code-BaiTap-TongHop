#include <stdio.h>

int main() {
    int numTestCases;
    scanf("%d", &numTestCases);

    for (int i = 0; i < numTestCases; i++) {
        int target;
        scanf("%d", &target);

        int minutes;
        if (target % 3 == 0) {
            minutes = target / 3; 
        } else {
            minutes = target / 3 + 1; 
        }

        printf("%d\n", minutes);
    }

    return 0;
}

