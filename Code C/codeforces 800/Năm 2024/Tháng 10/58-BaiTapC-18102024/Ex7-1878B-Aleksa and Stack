#include <stdio.h>

void generateArray(int n) {
    int num = 1;
    
    for (int i = 0; i < n; i++) {
        printf("%d ", num * 2);
        num++;
    }
    printf("\n");
}

int main() {
    int test_cases;
    scanf("%d", &test_cases);

    while (test_cases--) {
        int size;
        scanf("%d", &size);
        generateArray(size);
    }
    
    return 0;
}

