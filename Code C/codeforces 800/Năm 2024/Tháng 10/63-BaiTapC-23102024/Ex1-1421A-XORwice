#include <stdio.h>

int main() {
    int num_cases;
    scanf("%d", &num_cases);

    for (int i = 0; i < num_cases; i++) {
        int a_value, b_value;
        scanf("%d %d", &a_value, &b_value);
        
        int min_value = a_value ^ b_value;

        printf("%d\n", min_value);
    }

    return 0;
}

