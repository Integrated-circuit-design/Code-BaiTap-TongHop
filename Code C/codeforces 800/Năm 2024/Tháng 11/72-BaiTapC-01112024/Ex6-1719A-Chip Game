#include <stdio.h>

int main() {
    int numberOfTests;
    scanf("%d", &numberOfTests);

    for (int i = 0; i < numberOfTests; i++) {
        long long rows, cols;
        scanf("%lld %lld", &rows, &cols);

        if ((rows % 2 != 0) && (cols % 2 != 0)) {
            printf("Tonya\n");
        } else {
            printf("Burenka\n");
        }
    }

    return 0;
}

