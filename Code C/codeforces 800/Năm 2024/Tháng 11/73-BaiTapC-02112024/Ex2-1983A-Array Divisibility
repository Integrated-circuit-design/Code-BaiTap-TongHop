#include <stdio.h>

void generate_beautiful_array(int size) {
    for (int idx = 1; idx <= size; idx++) {
        printf("%d ", idx * size);
    }
    printf("\n");
}

int main() {
    int test_cases;
    scanf("%d", &test_cases);
    
    while (test_cases--) {
        int n;
        scanf("%d", &n);
        generate_beautiful_array(n);
    }
    
    return 0;
}

