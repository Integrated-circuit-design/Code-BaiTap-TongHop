#include <stdio.h>

int main() {
    int numTests;
    scanf("%d", &numTests);

    for (int i = 0; i < numTests; i++) {
        int countOnes;
        scanf("%d", &countOnes);

        if (countOnes % 2 == 0) {
            printf("Alice\n");
        } else {
            printf("Bob\n");
        }
    }

    return 0;
}

