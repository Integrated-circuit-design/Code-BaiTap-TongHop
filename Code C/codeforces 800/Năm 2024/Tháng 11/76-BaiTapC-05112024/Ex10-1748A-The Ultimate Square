#include <stdio.h>
#include <math.h>

int main() {
    int testCases;
    scanf("%d", &testCases);

    for (int i = 0; i < testCases; i++) {
        long long numberOfBlocks;
        scanf("%lld", &numberOfBlocks);
        long long maxSquareSide = (long long)sqrt(numberOfBlocks);
        printf("%lld\n", maxSquareSide);
    }

    return 0;
}

