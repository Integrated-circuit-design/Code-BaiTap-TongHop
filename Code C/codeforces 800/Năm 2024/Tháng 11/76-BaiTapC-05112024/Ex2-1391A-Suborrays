#include <stdio.h>

void inHoanViTotKhac(int n) {
    for (int i = 1; i <= n; i++) {
        printf("%d ", i);
    }
    printf("\n");
}

int main() {
    int t;
    scanf("%d", &t);
    
    while (t--) {
        int n;
        scanf("%d", &n);
        inHoanViTotKhac(n);
    }
    
    return 0;
}

