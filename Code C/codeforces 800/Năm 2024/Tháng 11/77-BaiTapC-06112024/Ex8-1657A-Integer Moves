#include <stdio.h>
#include <math.h>

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        int x, y;
        scanf("%d %d", &x, &y);

        if (x == 0 && y == 0) {
            printf("0\n");
        }
        else if ((x * x + y * y) == (int) sqrt(x * x + y * y) * (int) sqrt(x * x + y * y)) {
            printf("1\n");
        }
        else {
            printf("2\n");
        }
    }

    return 0;
}

