#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        int a, b;
        scanf("%d %d", &a, &b);

        if ((a % 2 == 0 || b % 2 == 0) && a > 1 && b > 1) {
            printf("Yes\n");
        } else {
            printf("No\n");
        }
    }

    return 0;
}

