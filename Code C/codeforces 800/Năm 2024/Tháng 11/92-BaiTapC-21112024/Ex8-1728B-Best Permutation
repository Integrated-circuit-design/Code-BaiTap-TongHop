#include <stdio.h>

void printOptimalPermutation(int n) {
    for (int i = 1; i <= n - 3; i++) {
        printf("%d ", i);
    }
    printf("%d %d %d\n", n - 2, n - 1, n);
}

int main() {
    int t, n;
    scanf("%d", &t);

    while (t--) {
        scanf("%d", &n);
        printOptimalPermutation(n);
    }

    return 0;
}
