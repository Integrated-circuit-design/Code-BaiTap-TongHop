#include <stdio.h>

void solve() {
    int t;
    scanf("%d", &t);

    while (t--) {
        int n;
        scanf("%d", &n);
        
        char binary_string[n + 1];
        for (int i = 0; i < n; i++) {
            binary_string[i] = (i % 2 == 0) ? '0' : '1';
        }
        
        binary_string[n] = '\0';
        
        printf("%s\n", binary_string);
    }
}

int main() {
    solve();
    return 0;
}

