#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        int n, k;
        scanf("%d %d", &n, &k);

        int a[n];
        int all_zero = 1;

        for (int i = 0; i < n; i++) {
            scanf("%d", &a[i]);
            if (a[i] == 1) {
                all_zero = 0;
            }
        }

        if (all_zero) {
            printf("NO\n");
        } else {
            if (k > 1) {
                printf("YES\n");
            } else {
                printf("YES\n");
            }
        }
    }

    return 0;
}

