#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);
    
    while (t--) {
        int n;
        scanf("%d", &n);
        
        for (int i = 0; i < n; i++) {
            if (i == n - 1) {
                printf("3");
            } else {
                printf("3 ");
            }
        }
        printf("\n");
    }

    return 0;
}

