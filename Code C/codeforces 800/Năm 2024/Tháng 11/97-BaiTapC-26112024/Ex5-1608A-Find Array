#include <stdio.h>

void xuly() {
    int t;
    scanf("%d", &t);
    
    while (t--) {
        int n;
        scanf("%d", &n);
        
        for (int i = 1; i <= n; i++) {
            printf("%d ", 1000 * i + 1);
        }
        printf("\n");
    }
}

int main() {
    xuly();
    return 0;
}

