#include <stdio.h>

void solve() {
    int n;
    scanf("%d", &n);
    
    for (int i = n + 1; i <= 2 * n; i++) {
        printf("%d ", 2 * i);
    }
    
    printf("\n");
}

int main() {
    int t;
    scanf("%d", &t);
    
    for (int i = 0; i < t; i++) {
        solve();
    }
    
    return 0;
}

