#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    for (int i = 0; i < n; i++) {
        char name[11];
        int before, after;

        scanf("%s %d %d", name, &before, &after);

        if (before >= 2400 && after > before) {
            printf("YES\n");
            return 0;
        }
    }

    printf("NO\n");

    return 0;
}

