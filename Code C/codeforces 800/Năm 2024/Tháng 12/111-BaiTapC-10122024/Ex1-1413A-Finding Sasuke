#include <stdio.h>

int main() {
    int T;
    scanf("%d", &T);

    while (T--) {
        int n;
        scanf("%d", &n);

        int a[n];
        for (int i = 0; i < n; i++) {
            scanf("%d", &a[i]);
        }

        int b[n];
        for (int i = 0; i < n; i++) {
            if (i % 2 == 0) {
                b[i] = 1;
            } else {
                b[i] = -1;
            }
        }

        for (int i = 0; i < n; i++) {
            printf("%d ", b[i]);
        }
        printf("\n");
    }

    return 0;
}

