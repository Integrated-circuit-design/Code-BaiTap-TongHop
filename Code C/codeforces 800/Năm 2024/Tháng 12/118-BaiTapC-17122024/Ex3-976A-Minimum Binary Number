#include <stdio.h>

int main() {
    int n;
    char s[101];

    scanf("%d", &n);
    scanf("%s", s);
    
    int i = 0;
    while (s[i] != '\0') {
        if (s[i] == '1') {
            printf("1");
        }
        i++;
    }

    for (int j = 0; j < n; j++) {
        if (s[j] == '0') {
            printf("0");
        }
    }

    printf("\n");
    
    return 0;
}

