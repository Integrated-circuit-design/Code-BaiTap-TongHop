def check_password_complexity(password):
    if len(password) < 5:
        return "Too weak"

    has_upper = False
    has_lower = False
    has_digit = False

    for char in password:
        if char.isupper():
            has_upper = True
        elif char.islower():
            has_lower = True
        elif char.isdigit():
            has_digit = True

        if has_upper and has_lower and has_digit:
            return "Correct"

    return "Too weak"

password = input()

print(check_password_complexity(password))

