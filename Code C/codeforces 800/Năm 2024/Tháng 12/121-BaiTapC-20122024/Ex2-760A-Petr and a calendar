#include <stdio.h>

int main() {
    int m, d;
    scanf("%d %d", &m, &d);

    int days_in_month[] = {0, 31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31};

    int total_days = days_in_month[m];

    int offset = d - 1;
    int columns = (total_days + offset + 6) / 7;

    printf("%d\n", columns);

    return 0;
}
