#include <stdio.h>

int main() {
    int m, d;
    scanf("%d %d", &m, &d);

    // Days in each month for a non-leap year
    int days_in_month[] = {0, 31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31};

    // Total days for the given month
    int total_days = days_in_month[m];

    // Calculate the number of columns needed
    // Offset is days before the first column starts (d - 1)
    int offset = d - 1;
    int columns = (total_days + offset + 6) / 7; // (offset + total days) rounds up to weeks

    printf("%d\n", columns);

    return 0;
}

