#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int board[n][n];
    int rowSum[n], colSum[n];

    for (int i = 0; i < n; i++) {
        rowSum[i] = 0;
        colSum[i] = 0;
    }

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            scanf("%d", &board[i][j]);
            rowSum[i] += board[i][j];
            colSum[j] += board[i][j];
        }
    }

    int winningSquares = 0;

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            if (colSum[j] > rowSum[i]) {
                winningSquares++;
            }
        }
    }

    printf("%d\n", winningSquares);

    return 0;
}

