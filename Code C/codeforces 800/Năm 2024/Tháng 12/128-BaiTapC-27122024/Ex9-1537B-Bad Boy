#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);
    
    while (t--) {
        long long n, m, i, j;
        scanf("%lld %lld %lld %lld", &n, &m, &i, &j);
        
        long long x1 = 1, y1 = 1, x2 = n, y2 = m;
        
        printf("%lld %lld %lld %lld\n", x1, y1, x2, y2);
    }
    
    return 0;
}

