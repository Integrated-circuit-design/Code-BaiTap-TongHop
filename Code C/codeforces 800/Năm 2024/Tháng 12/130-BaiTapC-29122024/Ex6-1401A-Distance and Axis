#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);
    
    while (t--) {
        int n, k;
        scanf("%d %d", &n, &k);
        
        if (k <= n) {
            printf("0\n");
        } else {
            int steps_needed = k - n;
            printf("%d\n", steps_needed);
        }
    }

    return 0;
}
