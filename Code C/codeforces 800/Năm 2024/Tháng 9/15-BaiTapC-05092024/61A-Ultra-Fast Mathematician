#include <stdio.h>
#include <string.h>

int main() {
    char num1[101], num2[101];

    scanf("%s", num1);
    scanf("%s", num2);

    int length = strlen(num1);

    for (int i = 0; i < length; i++) {
        if (num1[i] != num2[i]) {
            printf("1");
        } else {
            printf("0");
        }
    }

    printf("\n");

    return 0;
}

