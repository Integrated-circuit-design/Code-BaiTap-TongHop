#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);

    for (int i = 0; i < t; i++) {
        long long a, b;
        scanf("%lld %lld", &a, &b);

        long long phan_du = a % b;

        if (phan_du == 0) {
            printf("0\n");
        } else {
            long long buoc_can_them = b - phan_du;
            printf("%lld\n", buoc_can_them);
        }
    }

    return 0;
}

