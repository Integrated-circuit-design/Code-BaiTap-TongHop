#include <stdio.h>
#include <ctype.h>
#include <string.h>

int main() {
    int t;
    char s[4];

    scanf("%d", &t);

    for (int i = 0; i < t; i++) {
        scanf("%s", s);

        s[0] = tolower(s[0]);
        s[1] = tolower(s[1]);
        s[2] = tolower(s[2]);

        if (strcmp(s, "yes") == 0) {
            printf("YES\n");
        } else {
            printf("NO\n");
        }
    }

    return 0;
}

