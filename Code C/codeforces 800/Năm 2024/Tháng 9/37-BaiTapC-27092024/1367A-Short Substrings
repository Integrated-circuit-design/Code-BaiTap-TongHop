#include <stdio.h>
#include <string.h>

int main() {
    int soTest;
    scanf("%d", &soTest);

    while (soTest--) {
        char b[105];
        scanf("%s", b);

        int doDai = strlen(b);
        printf("%c", b[0]);

        for (int i = 1; i < doDai; i += 2) {
            printf("%c", b[i]);
        }

        printf("\n");
    }

    return 0;
}

