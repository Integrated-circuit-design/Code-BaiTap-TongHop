#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        long long x1, p1, x2, p2;
        scanf("%lld %lld", &x1, &p1);
        scanf("%lld %lld", &x2, &p2);

        if (p1 > p2) {
            printf(">\n");
        } else if (p1 < p2) {
            printf("<\n");
        } else {
            if (x1 > x2) {
                printf(">\n");
            } else if (x1 < x2) {
                printf("<\n");
            } else {
                printf("=\n");
            }
        }
    }

    return 0;
}

