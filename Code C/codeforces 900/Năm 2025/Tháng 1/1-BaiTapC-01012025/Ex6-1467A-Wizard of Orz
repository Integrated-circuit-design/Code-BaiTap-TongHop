#include <stdio.h>

void solve() {
    int t;
    scanf("%d", &t);

    while (t--) {
        int n;
        scanf("%d", &n);

        int result[n];

        for (int i = 0; i < n; i++) {
            result[i] = (9 - i) % 10;
        }

        for (int i = 0; i < n; i++) {
            printf("%d", result[i]);
        }

        printf("\n");
    }
}

int main() {
    solve();
    return 0;
}

