#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        int a1, b1, a2, b2;
        scanf("%d %d", &a1, &b1);
        scanf("%d %d", &a2, &b2);

        if ((a1 == a2 && b1 + b2 == a1) || (b1 == b2 && a1 + a2 == b1)) {
            printf("YES\n");
        } else {
            printf("NO\n");
        }
    }

    return 0;
}

