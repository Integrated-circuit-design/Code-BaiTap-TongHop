#include <stdio.h>

int main() {
    int test_cases;
    scanf("%d", &test_cases);

    while (test_cases--) {
        int rows, cols;
        scanf("%d %d", &rows, &cols);

        if (rows == 1 || cols == 1) {
            printf("1\n");
        } else {
            printf("2\n");
        }
    }

    return 0;
}

