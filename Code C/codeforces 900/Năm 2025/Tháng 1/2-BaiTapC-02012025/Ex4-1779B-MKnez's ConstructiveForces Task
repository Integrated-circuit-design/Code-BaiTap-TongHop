#include <stdio.h>

void solve() {
    int t;
    scanf("%d", &t);

    while (t--) {
        int n;
        scanf("%d", &n);

        if (n % 2 == 0) {
            printf("YES\n");
            for (int i = 0; i < n; i++) {
                printf("1 ");
            }
            printf("\n");
        } else {
            printf("NO\n");
        }
    }
}

int main() {
    solve();
    return 0;
}

