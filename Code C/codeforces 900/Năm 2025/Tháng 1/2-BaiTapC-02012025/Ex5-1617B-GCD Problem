#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);
    
    while (t--) {
        long long n;
        scanf("%lld", &n);
        
        long long a = n - 4;
        long long b = 2;
        long long c = 1;
        
        printf("%lld %lld %lld\n", a, b, c);
    }
    
    return 0;
}

