#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);
    
    while (t--) {
        int n;
        scanf("%d", &n);
        
        if (n == 1) {
            printf("69\n");
        } else {
            for (int i = 0; i < n; i++) {
                printf("%d ", 2 * i + 3);
            }
            printf("\n");
        }
    }
    
    return 0;
}

